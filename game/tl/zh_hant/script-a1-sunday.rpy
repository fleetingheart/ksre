    # game/script-a1-sunday.rpy:5298
    old "{size=55}{b}OFF LIMITS{/b}{/size}"
    new "{size=55}{b}禁止進入{/b}{/size}"

    # game/script-a1-sunday.rpy:5327
    old "AHHHHHHHHHHHHH!"
    new "啊啊啊啊啊啊啊啊！"

