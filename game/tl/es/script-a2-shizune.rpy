# game/script-a2-shizune.rpy:2164
translate es a2_shizune_advanced_game_theory_3e112ef6:

    # mi "This is a fun game, but it took so long. Almost the whole period!"
    mi "Este es un juego divertido, pero tomó mucho tiempo. ¡Casi toda la hora!"

# game/script-a2-shizune.rpy:2166
translate es a2_shizune_advanced_game_theory_fb9d3d0c:

    # mi "Chess is too formulaic, especially at this level. How about some advanced rules~?"
    mi "El ajedrez es muy formulista, especialmente a este nivel. ¿Qué tal algunas reglas avanzadas~?"

# game/script-a2-shizune.rpy:2168
translate es a2_shizune_advanced_game_theory_81d99227:

    # hi "What?"
    hi "¿Qué?"

# game/script-a2-shizune.rpy:2173
translate es a2_shizune_advanced_game_theory_59fa7f98:

    # ha "Ad… vanced…?"
    ha "¿A… vanzadas…?"

# game/script-a2-shizune.rpy:2184
