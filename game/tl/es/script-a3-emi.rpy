# game/script-a3-emi.rpy:10
translate es a3_emi_eet_ees_scienca_76b2fe88:

    # nvl clear
    nvl clear

# game/script-a3-emi.rpy:16
translate es a3_emi_eet_ees_scienca_15a98fa6:

    # n "{vspace=90}My head's in a spin all through Mutou's class."
    n "{vspace=90}Mi cabeza no para de dar vueltas durante la clase de Mutou."

# game/script-a3-emi.rpy:18
translate es a3_emi_eet_ees_scienca_9475f130:

    # n "I'm going to have dinner."
    n "Voy a salir a comer."

# game/script-a3-emi.rpy:20
translate es a3_emi_eet_ees_scienca_28dc41be:

    # n "With Emi."
    n "Con Emi."

# game/script-a3-emi.rpy:22
translate es a3_emi_eet_ees_scienca_0c7ebe42:

    # n "Who wants to be my girlfriend, no less."
    n "Quien quiere ser mi novia, nada menos."

# game/script-a3-emi.rpy:24
translate es a3_emi_eet_ees_scienca_c31dcf22:

    # n "A date…"
    n "Una cita…"

# game/script-a3-emi.rpy:26
translate es a3_emi_eet_ees_scienca_3816071a:

    # n "And then she kissed me."
    n "Y luego me besó."

# game/script-a3-emi.rpy:28
translate es a3_emi_eet_ees_scienca_3d029ca4:

    # n "That kiss. I keep going back to it, playing it over in my mind again and again."
    n "Ese beso. Sigo regresando a él, repitiéndolo una y otra vez en mi cabeza."

# game/script-a3-emi.rpy:30
translate es a3_emi_eet_ees_scienca_cf3c0ca0:

    # n "Everything about that moment felt so right."
    n "Todo sobre ese momento se sintió tan bien."

# game/script-a3-emi.rpy:32
translate es a3_emi_eet_ees_scienca_ed009a62:

    # n "{vspace=30}My mind drifts off, lost in thoughts of Emi."
    n "{vspace=30}Mi mente divaga, distraída con pensamientos de Emi."

# game/script-a3-emi.rpy:36
translate es a3_emi_eet_ees_scienca_76b2fe88_1:

    # nvl clear
    nvl clear

# game/script-a3-emi.rpy:42
translate es a3_emi_eet_ees_scienca_b3b5c77f:

    # mu "Nakai? Hello?"
    mu "¿Nakai? ¿Hola?"

# game/script-a3-emi.rpy:44
translate es a3_emi_eet_ees_scienca_3a9fbd58:

    # "It seems like I've drifted a bit too far."
    "Al parecer me perdí en mis divagaciones."

# game/script-a3-emi.rpy:46
translate es a3_emi_eet_ees_scienca_867ff869:

    # hi "Huh? What?"
    hi "¿Eh? ¿Qué?"

# game/script-a3-emi.rpy:51
translate es a3_emi_eet_ees_scienca_0e1b3b49:

    # mu "Egad! You've contracted some kind of amnesia!"
    mu "¡Dioses! ¡Has contraído algún tipo de amnesia!"

# game/script-a3-emi.rpy:53
translate es a3_emi_eet_ees_scienca_71f0eb36:

    # mu "Someone get the nurse!"
    mu "¡Alguien llame al enfermero!"

# game/script-a3-emi.rpy:55
translate es a3_emi_eet_ees_scienca_0a49a0cf:

    # "The class chuckles at Mutou's antics."
    "La clase ríe con las payasadas de Mutou."

# game/script-a3-emi.rpy:57
translate es a3_emi_eet_ees_scienca_0bd24c48:

    # hi "Sorry, sir."
    hi "Perdón, señor."

# game/script-a3-emi.rpy:62
translate es a3_emi_eet_ees_scienca_81ae6327:

    # mu "Hmm, won't happen again and all that, right?"
    mu "Hmm, no pasará de nuevo y todo eso, ¿no?"

# game/script-a3-emi.rpy:64
translate es a3_emi_eet_ees_scienca_ee8aef65:

    # hi "Exactly."
    hi "Exacto."

# game/script-a3-emi.rpy:66
translate es a3_emi_eet_ees_scienca_07a490bb:

    # "Mutou brightens considerably."
    "Mutou se anima considerablemente."

# game/script-a3-emi.rpy:71
translate es a3_emi_eet_ees_scienca_f4f17cce:

    # mu "Well! Lovely to hear!"
    mu "¡Bien! ¡Me alegra escucharlo!"

# game/script-a3-emi.rpy:73
translate es a3_emi_eet_ees_scienca_a41b8868:

    # mu "I'd hate to have my star pupil slacking off, after all."
    mu "No me gustaría tener a mi alumno estrella holgazaneando, después de todo."

# game/script-a3-emi.rpy:78
translate es a3_emi_eet_ees_scienca_97b1f6d3:

    # "I've been doing well, but I hardly qualify as a star pupil, I think."
    "Lo he estado haciendo bien, pero creo que difícilmente califico como alumno estrella."

# game/script-a3-emi.rpy:80
translate es a3_emi_eet_ees_scienca_af1cb1e4:

    # "I'm fairly certain that this class is the sort that everyone does well in. It's just memorizing formulas."
    "Estoy seguro de que este es el tipo de clase en la que todos lo hacen bien. Solo debes memorizar fórmulas."

# game/script-a3-emi.rpy:82
translate es a3_emi_eet_ees_scienca_8de748f9:

    # "True to my word, I manage to pay attention for the rest of the class."
    "Fiel a mi palabra, me las arreglo para prestar atención durante el resto de la clase."

# game/script-a3-emi.rpy:91
translate es a3_emi_eet_ees_scienca_f6185feb:

    # mu "Nakai, may I have a word with you?"
    mu "Nakai, ¿puedo hablar contigo?"

# game/script-a3-emi.rpy:93
translate es a3_emi_eet_ees_scienca_a8aefd35:

    # "I wonder if I'm in trouble for earlier."
    "Me pregunto si estoy en problemas por lo de antes."

# game/script-a3-emi.rpy:95
translate es a3_emi_eet_ees_scienca_5a036de1:

    # hi "Uh, sure."
    hi "Eh, claro."

# game/script-a3-emi.rpy:97
translate es a3_emi_eet_ees_scienca_4a1c05ff:

    # hi "Am I in trouble?"
    hi "¿Estoy en problemas?"

# game/script-a3-emi.rpy:102
translate es a3_emi_eet_ees_scienca_1d50d316:

    # "Mutou looks genuinely confused for a moment."
    "Mutou se ve genuinamente confundido por un instante."

# game/script-a3-emi.rpy:104
translate es a3_emi_eet_ees_scienca_628e116e:

    # mu "Beg your pardon?"
    mu "¿Disculpa?"

# game/script-a3-emi.rpy:106
translate es a3_emi_eet_ees_scienca_60653050:

    # "He tilts his head to one side and thinks for a moment."
    "Él inclina la cabeza hacia un lado y piensa por un momento."

# game/script-a3-emi.rpy:111
translate es a3_emi_eet_ees_scienca_3985dd59:

    # mu "Oh, that! No, no, you're not in any sort of trouble."
    mu "¡Oh, eso! No, no, no estás en problemas."

# game/script-a3-emi.rpy:113
translate es a3_emi_eet_ees_scienca_fe46af35:

    # mu "There's just a question I want to ask you."
    mu "Solo me gustaría preguntarte algo."

# game/script-a3-emi.rpy:115
translate es a3_emi_eet_ees_scienca_111a58e1:

    # hi "What's that?"
    hi "¿Qué cosa?"

# game/script-a3-emi.rpy:120
translate es a3_emi_eet_ees_scienca_4ac04583:

    # mu "Nothing terrible, I was just wondering what your plans for after graduation are."
    mu "Nada terrible, solo me preguntaba cuáles son tus planes para después de la graduación."

# game/script-a3-emi.rpy:124
translate es a3_emi_eet_ees_scienca_f5d0d72f:

    # mu "Are you going to university?"
    mu "¿Irás a la universidad?"

# game/script-a3-emi.rpy:126
translate es a3_emi_eet_ees_scienca_1312097b:

    # hi "Yeah, I guess. I can't really see a reason not to go."
    hi "Sí, supongo. En realidad no veo una razón para no ir."

# game/script-a3-emi.rpy:128
translate es a3_emi_eet_ees_scienca_2d060ffe:

    # mu "Given any thought to what you'll study?"
    mu "¿Has pensado en qué te gustaría estudiar?"

# game/script-a3-emi.rpy:130
translate es a3_emi_eet_ees_scienca_87b8c977:

    # hi "Not really, no. I figure I'll come up with something when I get there."
    hi "No, no realmente. Pensaba que ya se me ocurriría algo una vez llegara allá."

# game/script-a3-emi.rpy:135
translate es a3_emi_eet_ees_scienca_ec1cca9e:

    # "Mutou laughs."
    "Mutou se ríe."

# game/script-a3-emi.rpy:137
translate es a3_emi_eet_ees_scienca_b268058b:

    # mu "Taking things as they come, eh?"
    mu "Tomando las cosas como vengan, ¿eh?"

# game/script-a3-emi.rpy:139
translate es a3_emi_eet_ees_scienca_a2ec98ef:

    # mu "I'd argue against it, but that's how I did things when I went to university."
    mu "Estaría en contra de eso, pero así lo hice yo cuando fui a la universidad."

# game/script-a3-emi.rpy:141
translate es a3_emi_eet_ees_scienca_4f667d90:

    # mu "Well, not really."
    mu "Bueno, no realmente."

# game/script-a3-emi.rpy:143
translate es a3_emi_eet_ees_scienca_eff4d5fa:

    # mu "I knew I'd go into a science, I just wasn't sure which one."
    mu "Sabía que iría por alguna ciencia, solo que no estaba seguro de cuál."

# game/script-a3-emi.rpy:145
translate es a3_emi_eet_ees_scienca_09553ba6:

    # mu "Ended up with physics, but could just as well have gone for astronomy or what have you."
    mu "Terminé en física, pero igual podría haber terminado en astronomía o lo que fuese."

# game/script-a3-emi.rpy:150
translate es a3_emi_eet_ees_scienca_63e79513:

    # mu "Actually I did go for chemistry first, but there were all sorts of things…"
    mu "La verdad es que primero fui por química, pero había toda clase de cosas…"

# game/script-a3-emi.rpy:152
translate es a3_emi_eet_ees_scienca_ae1d1679:

    # "Mutou trails off, and frowns slightly."
    "Mutou se calla, y frunce el ceño levemente."

# game/script-a3-emi.rpy:154
translate es a3_emi_eet_ees_scienca_6bd23bfc:

    # "It takes a minute for him to recover his train of thought, and I wait patiently for him to continue."
    "Le toma un minuto recuperar su línea de pensamiento, y espero pacientemente a que continúe."

# game/script-a3-emi.rpy:159
translate es a3_emi_eet_ees_scienca_87482d27:

    # mu "So anyway, I did a lot of physics as well, because I had an interest in that, but I wasn't sure if it was for me."
    mu "En fin, también avancé mucho en física, porque me interesaba, pero no estaba seguro de si era lo mío."

# game/script-a3-emi.rpy:164
translate es a3_emi_eet_ees_scienca_8444312d:

    # mu "So I went back to chemistry, and here we are. Yes?"
    mu "Así que regresé a química, y aquí estamos. ¿Sí?"

# game/script-a3-emi.rpy:169
translate es a3_emi_eet_ees_scienca_d314a1b8:

    # "He smiles at me enthusiastically, as if waiting for me to confirm that yes, here is were we are."
    "Él me sonríe con entusiasmo, como si esperara que yo confirme ese sí, que aquí es donde estamos."

# game/script-a3-emi.rpy:171
translate es a3_emi_eet_ees_scienca_1c1105c6:

    # "Somehow I get the feeling that Mutou had a plan for this conversation, but I'll be damned if I can figure it out."
    "Por algún motivo tengo la sensación de que Mutou tenía un plan para esta conversación, pero mentiría si digo que lo puedo adivinar."

# game/script-a3-emi.rpy:173
translate es a3_emi_eet_ees_scienca_fe89e9d3:

    # hi "I'm sorry, I'm not following you."
    hi "Perdón, pero no te sigo."

# game/script-a3-emi.rpy:175
translate es a3_emi_eet_ees_scienca_2f95be7a:

    # "Mutou frowns and rubs his chin a bit, looking perplexed. He then snaps his fingers as if he's remembered what the point of all this was."
    "Mutou frunce el ceño y se rasca la barbilla por un momento, viéndose perplejo. Luego chasquea sus dedos como si hubiera recordado cuál era el punto de todo esto."

# game/script-a3-emi.rpy:177
translate es a3_emi_eet_ees_scienca_b216e9b6:

    # mu "Right! Yes! You!"
    mu "¡Cierto! ¡Sí! ¡Tú!"

# game/script-a3-emi.rpy:179
translate es a3_emi_eet_ees_scienca_f10eafa0:

    # hi "Me?"
    hi "¿Yo?"

# game/script-a3-emi.rpy:181
translate es a3_emi_eet_ees_scienca_ae3967dd:

    # mu "Yes! You should look into studying one of the sciences!"
    mu "¡Sí! ¡Deberías considerar estudiar una de las ciencias!"

# game/script-a3-emi.rpy:183
translate es a3_emi_eet_ees_scienca_bf24af5d:

    # mu "You're fantastic at it."
    mu "Eres fantástico con ellas."

# game/script-a3-emi.rpy:185
translate es a3_emi_eet_ees_scienca_32e22e1e:

    # mu "Unless you'd rather just go into math."
    mu "A menos de que quieras irte por las matemáticas."

# game/script-a3-emi.rpy:190
translate es a3_emi_eet_ees_scienca_d936ca48:

    # "Mutou makes a sour face."
    "Mutou pone cara de amargura."

# game/script-a3-emi.rpy:192
translate es a3_emi_eet_ees_scienca_5ea95289:

    # mu "Not a big fan of straight math. I always liked the experiments more than the proofs."
    mu "No soy un gran fanático de las matemáticas puras. Siempre me gustaron más los experimentos que las pruebas."

# game/script-a3-emi.rpy:194
translate es a3_emi_eet_ees_scienca_c57cad1d:

    # hi "You're saying I should study science at university?"
    hi "¿Estás diciendo que debería de estudiar ciencias en la universidad?"

# game/script-a3-emi.rpy:196
translate es a3_emi_eet_ees_scienca_06805c1b:

    # "Mutou seems thrown off balance by my question."
    "Mutou parece desbalancearse por mi pregunta."

# game/script-a3-emi.rpy:201
translate es a3_emi_eet_ees_scienca_2d3e99ea:

    # mu "Well, sort of."
    mu "Bueno, algo así."

# game/script-a3-emi.rpy:206
translate es a3_emi_eet_ees_scienca_4b406844:

    # mu "You could also join the science club!"
    mu "¡También podrías unirte al club de ciencia!"

# game/script-a3-emi.rpy:208
translate es a3_emi_eet_ees_scienca_3957abb0:

    # mu "Trouble is, there's not actually a science club."
    mu "El problema es, en realidad no hay un club de ciencia."

# game/script-a3-emi.rpy:210
translate es a3_emi_eet_ees_scienca_73ff24fb:

    # mu "But there could be!"
    mu "¡Pero podría haber uno!"

# game/script-a3-emi.rpy:212
translate es a3_emi_eet_ees_scienca_327fb976:

    # mu "You could even be a charter member!"
    mu "¡Incluso podrías ser uno de los primeros miembros!"

# game/script-a3-emi.rpy:214
translate es a3_emi_eet_ees_scienca_bd9457aa:

    # mu "A founding father!"
    mu "¡Un padre fundador!"

# game/script-a3-emi.rpy:216
translate es a3_emi_eet_ees_scienca_8eff14d6:

    # mu "Of course, you'd need to find other members."
    mu "Por supuesto, necesitarías encontrar otros miembros."

# game/script-a3-emi.rpy:221
translate es a3_emi_eet_ees_scienca_397c6726:

    # mu "Well, only if you wanted to."
    mu "Bueno, solo si querías."

# game/script-a3-emi.rpy:223
translate es a3_emi_eet_ees_scienca_1f341fff:

    # mu "We could just start it up with the two of us."
    mu "Podríamos empezarlo solo nosotros dos."

# game/script-a3-emi.rpy:225
translate es a3_emi_eet_ees_scienca_0815dcf2:

    # mu "And um."
    mu "Y um."

# game/script-a3-emi.rpy:230
translate es a3_emi_eet_ees_scienca_35d9dc44:

    # mu "I could give you things to read, and we could talk about them."
    mu "Podría darte cosas para leer, y podríamos hablar de ellas."

# game/script-a3-emi.rpy:232
translate es a3_emi_eet_ees_scienca_0ae31207:

    # mu "Er, and I could help you get ready for university and such as well."
    mu "Eh, y también podría ayudarte con la preparación para la universidad y eso."

# game/script-a3-emi.rpy:237
translate es a3_emi_eet_ees_scienca_2d4fc22a:

    # mu "Wait!"
    mu "¡Espera!"

# game/script-a3-emi.rpy:239
translate es a3_emi_eet_ees_scienca_43e0d8d6:

    # "Mutou rummages around in his briefcase and tosses me a book."
    "Mutou hurga en su maletín y me lanza un libro."

# game/script-a3-emi.rpy:244
translate es a3_emi_eet_ees_scienca_93e6a32a:

    # mu "Read that."
    mu "Lee eso."

# game/script-a3-emi.rpy:246
translate es a3_emi_eet_ees_scienca_e13c6d18:

    # mu "If it's interesting, then we can talk about it."
    mu "Si es interesante, entonces podríamos hablarlo."

# game/script-a3-emi.rpy:248
translate es a3_emi_eet_ees_scienca_2ffb81b6:

    # "'A Brief History of Time?'"
    "¿“Breve Historia del Tiempo”?"

# game/script-a3-emi.rpy:250
translate es a3_emi_eet_ees_scienca_acd6bd8c:

    # "I don't know if I actually want to read this, but Mutou seems pretty excited about it."
    "No sé si realmente quiero leerlo, pero Mutou parece muy emocionado por esto."

# game/script-a3-emi.rpy:252
translate es a3_emi_eet_ees_scienca_bd77b775:

    # hi "What's it about?"
    hi "¿De qué se trata?"

# game/script-a3-emi.rpy:257
translate es a3_emi_eet_ees_scienca_d32f4c8f:

    # mu "Time. Space. Space-time. Black holes and such."
    mu "Tiempo. Espacio. Espacio-tiempo. Agujeros negros y demás."

# game/script-a3-emi.rpy:259
translate es a3_emi_eet_ees_scienca_e367fed6:

    # mu "And it's not that dense. Just to see if that sort of thing's interesting for you, you understand."
    mu "Y no es tan denso. Es solo para ver si ese tipo de cosas te son interesantes, tú entiendes."

# game/script-a3-emi.rpy:261
translate es a3_emi_eet_ees_scienca_ba528a5b:

    # mu "Hang around after class, and we can either discuss it, or I can show you how to make explosives in the lab."
    mu "Pásate luego de clases, y podemos discutirlo, o puedo enseñarte a hacer explosivos en el laboratorio."

# game/script-a3-emi.rpy:266
translate es a3_emi_eet_ees_scienca_b3b8effd:

    # "He waves a hand at my quizzical expression."
    "Él sacude una mano ante mi expresión de curiosidad."

# game/script-a3-emi.rpy:268
translate es a3_emi_eet_ees_scienca_ddc7b293:

    # mu "Joking, sorry."
    mu "Bromeaba, lo siento."

# game/script-a3-emi.rpy:270
translate es a3_emi_eet_ees_scienca_3d64d466:

    # mu "Still, I've kept you here long enough for now."
    mu "Aun así, ya te he mantenido aquí por un buen rato."

# game/script-a3-emi.rpy:272
translate es a3_emi_eet_ees_scienca_14119bd6:

    # mu "Think about science as a career path, Nakai. I think you'd enjoy it."
    mu "Piensa en la ciencia como una trayectoria profesional, Nakai. Creo que lo disfrutarías."

# game/script-a3-emi.rpy:274
translate es a3_emi_eet_ees_scienca_69cef357:

    # hi "Uh, okay. I will. Thank you for the book."
    hi "Eh, de acuerdo. Lo haré. Gracias por el libro."

# game/script-a3-emi.rpy:281
translate es a3_emi_eet_ees_scienca_a72fedac:

    # "I leave the classroom and look up at the clock; quite a chunk of time to kill until Emi's out of practice."
    "Salgo del aula y miro hacia el reloj; una considerable porción de tiempo por matar hasta que Emi salga de la práctica."

# game/script-a3-emi.rpy:283
translate es a3_emi_eet_ees_scienca_41a18c00:

    # "Guess I'll give this book a look; I should probably shower as well."
    "Supongo que ojearé este libro; probablemente también debería de bañarme."

# game/script-a3-emi.rpy:285
translate es a3_emi_eet_ees_scienca_0f93e60e:

    # "Showering before a date's only proper, right?"
    "Bañarse antes de una cita es lo más adecuado, ¿verdad?"

# game/script-a3-emi.rpy:287
translate es a3_emi_eet_ees_scienca_bc26944c:

    # "I head back to the dorms."
    "Me dirijo de vuelta a los dormitorios."

# game/script-a3-emi.rpy:292
translate es a3_emi_eet_ees_scienca_0b897439:

    # "I wonder where I'm supposed to meet Emi, anyway."
    "De todas maneras, me pregunto en dónde se supone que me encuentre con Emi."

# game/script-a3-emi.rpy:294
translate es a3_emi_eet_ees_scienca_5b699534:

    # "She said 'after practice,' but she didn't say where I should find her."
    "Ella dijo “luego de la práctica”, pero no dijo dónde debía encontrarme con ella."

# game/script-a3-emi.rpy:296
translate es a3_emi_eet_ees_scienca_034f77f1:

    # "I guess I can just swing by the track; that's probably best, anyway."
    "Supongo que puedo pasar por la pista; probablemente sea lo mejor, de todos modos."

# game/script-a3-emi.rpy:298
translate es a3_emi_eet_ees_scienca_5d80b459:

    # "If she needs a shower, I can just wait for her in her room or something."
    "Si ella necesita ducharse, podría esperarla en su habitación o algo."

# game/script-a3-emi.rpy:300
translate es a3_emi_eet_ees_scienca_727cb58d:

    # "Or in the hallway, I guess; that would be better as well."
    "O en el corredor, supongo; eso también sería lo mejor."

# game/script-a3-emi.rpy:302
translate es a3_emi_eet_ees_scienca_b0473297:

    # "I take a quick shower, remembering to take my medication once I hop out."
    "Tomo una ducha rápida, recordando tomar mi medicina una vez salgo."

# game/script-a3-emi.rpy:304
translate es a3_emi_eet_ees_scienca_dffd4234:

    # "Now, for a look at this book."
    "Ahora, a echarle un vistazo a este libro."

# game/script-a3-emi.rpy:322
translate es a3_emi_definitions_4e17e492:

    # "I wake with a start."
    "Me despierto con un sobresalto."

# game/script-a3-emi.rpy:324
translate es a3_emi_definitions_ae963591:

    # "Shit! What time is it?"
    "¡Mierda! ¿Qué hora es?"

# game/script-a3-emi.rpy:326
translate es a3_emi_definitions_7acfdd73:

    # "A glance at the clock reveals that I've been asleep for nearly an hour."
    "Un vistazo al reloj revela que he estado dormido por casi una hora."

# game/script-a3-emi.rpy:328
translate es a3_emi_definitions_e0485a22:

    # hi "Thank goodness."
    hi "Gracias al cielo."

# game/script-a3-emi.rpy:330
translate es a3_emi_definitions_b4bf8be6:

    # "Emi's practice should be finishing up soon."
    "La práctica de Emi debería de estar por terminar."

# game/script-a3-emi.rpy:332
translate es a3_emi_definitions_d00a63a9:

    # "I throw on some casual clothes and head for the track."
    "Me pongo ropa casual y me dirijo a la pista."

# game/script-a3-emi.rpy:337
translate es a3_emi_definitions_53d50a9c:

    # "Somehow I get the feeling we won't be doing anything fancy for dinner."
    "Por algún motivo tengo el presentimiento de que no vamos a hacer nada estrafalario para la cena."

# game/script-a3-emi.rpy:339
translate es a3_emi_definitions_c4a17058:

    # "Emi doesn't strike me as a very fancy sort of person."
    "Emi no me da la impresión de ser una persona muy extravagante."

# game/script-a3-emi.rpy:341
translate es a3_emi_definitions_54682d0e:

    # "Still, I suppose there's a lot I have yet to know about Emi."
    "Aun así, supongo que todavía tengo muchas cosas que aprender sobre Emi."

# game/script-a3-emi.rpy:343
translate es a3_emi_definitions_d64ea69a:

    # "Despite our newfound closeness, I still feel like I don't know her as well as I should."
    "A pesar de nuestra recién descubierta cercanía, todavía siento que no la conozco tan bien como debería."

# game/script-a3-emi.rpy:345
translate es a3_emi_definitions_147987d9:

    # "Ah well, I have lots of time to fix that."
    "Oh bueno, tengo mucho tiempo para arreglar eso."

# game/script-a3-emi.rpy:347
translate es a3_emi_definitions_7f8abb80:

    # "To be honest, I'm looking forward to getting to know her more."
    "Para ser honesto, espero con ansias conocerla más."

# game/script-a3-emi.rpy:349
translate es a3_emi_definitions_ad17c48d:

    # "I'm so caught up in my own thoughts that I hardly register that I'm already at the track."
    "Estoy tan atrapado por mis propios pensamientos que apenas registro que ya estoy en la pista."

# game/script-a3-emi.rpy:351
translate es a3_emi_definitions_982a423b:

    # "Emi is nowhere to be found."
    "No veo a Emi por ningún lado."

# game/script-a3-emi.rpy:353
translate es a3_emi_definitions_62893847:

    # "I don't even see any signs of the track team."
    "Ni siquiera veo señales del equipo de atletismo."

# game/script-a3-emi.rpy:355
translate es a3_emi_definitions_0d2ea8a4:

    # "This could be embarrassing."
    "Esto podría ser vergonzoso."

# game/script-a3-emi.rpy:357
translate es a3_emi_definitions_1501abf3:

    # "I turn to head toward the girls' dormitory when I hear a shout."
    "Me doy vuelta para encaminarme hacia el dormitorio de las chicas cuando escucho a alguien."

# game/script-a3-emi.rpy:359
translate es a3_emi_definitions_41268bd2:

    # emi "Hey, Hisao!"
    emi "¡Oye, Hisao!"

# game/script-a3-emi.rpy:366
translate es a3_emi_definitions_0abe1912:

    # "I turn around to see Emi making a beeline for me with a gym bag slung over her shoulder."
    "Me doy vuelta para ver a Emi venir derechito hacia mí con una bolsa de gimnasia colgando del hombro."

# game/script-a3-emi.rpy:368
translate es a3_emi_definitions_4cddf139:

    # "She's changed into some decidedly more casual clothing; a pair of shorts and an olive green top."
    "Ella se puso ropa definitivamente más casual; unos pantalones cortos y una blusa verde oliva."

# game/script-a3-emi.rpy:370
translate es a3_emi_definitions_98275c52:

    # "Her running blades have been replaced by more realistic-looking legs that probably wouldn't fool anyone."
    "Sus prótesis para correr han sido reemplazadas por unas piernas de apariencia más realista que probablemente no engañan a nadie."

# game/script-a3-emi.rpy:372
translate es a3_emi_definitions_750be4f9:

    # "Emi doesn't seem to care about that, a fact which makes me smile."
    "A Emi no parece importarle eso, algo que me hace sonreír."

# game/script-a3-emi.rpy:377
translate es a3_emi_definitions_26ec0a3e:

    # emi "Hey, you came!"
    emi "¡Oye, viniste!"

# game/script-a3-emi.rpy:382
translate es a3_emi_definitions_207ff876:

    # emi "I mean I figured you would, but still…"
    emi "Digo, pensé que lo harías, pero aun así…"

# game/script-a3-emi.rpy:387
translate es a3_emi_definitions_9c0aecca:

    # "I suddenly find myself wrapped in a rather affectionate hug, and it proves to be impossible for me to keep what must be the world's largest grin off my face."
    "De repente me encuentro atrapado en un abrazo bastante afectuoso, y parece ser imposible para mí mantener lo que podría ser la mayor sonrisa del mundo fuera de mi rostro."

# game/script-a3-emi.rpy:389
translate es a3_emi_definitions_5f0487f3:

    # hi "Well, of course I came!"
    hi "Bueno, ¡claro que vine!"

# game/script-a3-emi.rpy:391
translate es a3_emi_definitions_24e52595:

    # hi "I'd be crazy not to, right?"
    hi "Estaría loco de no ser así, ¿cierto?"

# game/script-a3-emi.rpy:393
translate es a3_emi_definitions_9102d456:

    # "Emi ponders for a moment."
    "Emi reflexiona por un momento."

# game/script-a3-emi.rpy:398
translate es a3_emi_definitions_25ceccab:

    # emi "You know, that's true."
    emi "Sabes, es cierto."

# game/script-a3-emi.rpy:403
translate es a3_emi_definitions_c159b678:

    # emi "I mean I'm pretty amazing, after all."
    emi "Quiero decir, soy bastante asombrosa, después de todo."

# game/script-a3-emi.rpy:405
translate es a3_emi_definitions_a8f2ec77:

    # "I shrug in response."
    "Me encojo de hombros en respuesta."

# game/script-a3-emi.rpy:407
translate es a3_emi_definitions_2d969fbf:

    # hi "I certainly think so."
    hi "Ya lo creo."

# game/script-a3-emi.rpy:412
translate es a3_emi_definitions_4683b48d:

    # "It's an offhand remark, which is why I'm surprised to see that it seems to have caught Emi by surprise."
    "Es un comentario desinteresado, por eso me sorprendo al ver que parece haber tomado a Emi por sorpresa."

# game/script-a3-emi.rpy:417
translate es a3_emi_definitions_19867c98:

    # "She blushes and smiles warmly at me before planting a kiss on my lips."
    "Ella se sonroja y me sonríe cálidamente antes de plantar un beso en mis labios."

# game/script-a3-emi.rpy:419
translate es a3_emi_definitions_1f941396:

    # "Now it's my turn to be surprised."
    "Ahora es mi turno de estar sorprendido."

# game/script-a3-emi.rpy:424
translate es a3_emi_definitions_9e0d98a0:

    # "Emi steps back, resting her weight on her back heel, looking pleased with herself."
    "Emi da un paso atrás, descansando su peso sobre sus talones, viéndose complacida consigo misma."

# game/script-a3-emi.rpy:426
translate es a3_emi_definitions_7bd086b3:

    # "My brain fumbles for an appropriate response."
    "Mi cerebro busca a tientas una respuesta adecuada."

# game/script-a3-emi.rpy:428
translate es a3_emi_definitions_35c4f685:

    # hi "…"
    hi "…"

# game/script-a3-emi.rpy:430
translate es a3_emi_definitions_5813ea8a:

    # hi "I should compliment you more often."
    hi "Debería de halagarte más a menudo."

# game/script-a3-emi.rpy:435
translate es a3_emi_definitions_3feff31d:

    # "Emi laughs and gives me a playful shove."
    "Emi se ríe y me da un empujoncito juguetón."

# game/script-a3-emi.rpy:440
translate es a3_emi_definitions_16762d09:

    # emi "Jerk."
    emi "Idiota."

# game/script-a3-emi.rpy:445
translate es a3_emi_definitions_17633a63:

    # "I throw an arm around Emi's shoulders and am pleased when she immediately leans into me as if it were the most natural thing in the world."
    "Coloco un brazo alrededor de los hombros de Emi y me siento complacido cuando ella inmediatamente se inclina sobre mí como si fuera lo más natural del mundo."

# game/script-a3-emi.rpy:447
translate es a3_emi_definitions_16ab0094:

    # hi "So, where to?"
    hi "Entonces, ¿a dónde?"

# game/script-a3-emi.rpy:452
translate es a3_emi_definitions_b2652e9a:

    # emi "I'm not actually sure."
    emi "La verdad no estoy segura."

# game/script-a3-emi.rpy:457
translate es a3_emi_definitions_69449bd2:

    # emi "Where do people go on dates around here, anyway?"
    emi "Más bien, ¿a dónde va la gente a citas por estos lugares?"

# game/script-a3-emi.rpy:459
translate es a3_emi_definitions_2f0b2a3e:

    # "That's a damned good question."
    "Esa es una muy buena pregunta."

# game/script-a3-emi.rpy:461
translate es a3_emi_definitions_bc03e87b:

    # hi "I've got no idea."
    hi "No tengo ni idea."

# game/script-a3-emi.rpy:463
translate es a3_emi_definitions_aeaf8e56:

    # hi "Why don't we just head down to the Aura-Mart and grab something portable?"
    hi "¿Por qué no bajamos al Aura-Mart y buscamos algo para llevar?"

# game/script-a3-emi.rpy:465
translate es a3_emi_definitions_99b9c980:

    # "Emi's face brightens at this idea."
    "El rostro de Emi se ilumina con esta idea."

# game/script-a3-emi.rpy:470
translate es a3_emi_definitions_b0f9bfdf:

    # emi "A picnic!"
    emi "¡Un picnic!"

# game/script-a3-emi.rpy:475
translate es a3_emi_definitions_01fb0b5f:

    # emi "I think you're on to something, Hisao."
    emi "Creo que tienes razón, Hisao."

# game/script-a3-emi.rpy:480
translate es a3_emi_definitions_45095e39:

    # "Emi snakes her arm around my waist, and we begin to head for the front gate."
    "Emi acomoda su brazo alrededor de mi cintura, y nos dirigimos hacia el portón de la entrada."

# game/script-a3-emi.rpy:482
translate es a3_emi_definitions_31c35444:

    # "I'm entirely unsure of what I'm meant to do in this situation, but at least Emi seems to be equally clueless."
    "No tengo ni la más mínima idea de qué debería hacer en esta situación, pero al menos Emi se ve igual de perdida."

# game/script-a3-emi.rpy:487
translate es a3_emi_definitions_c02b5568:

    # "Despite the relaxing feeling of being with Emi, I still can't help feeling a little tense."
    "A pesar de la relajante sensación de estar con Emi, no puedo evitar sentirme un poco tenso."

# game/script-a3-emi.rpy:489
translate es a3_emi_definitions_19085674:

    # "What if I do something wrong?"
    "¿Qué tal si hago algo mal?"

# game/script-a3-emi.rpy:491
translate es a3_emi_definitions_8e94deef:

    # "I'd hate to make an ass out of myself."
    "No me gustaría quedar como un completo imbécil."

# game/script-a3-emi.rpy:496
translate es a3_emi_definitions_e9ed1ae9:

    # "The trip to the Aura-Mart is accompanied by Emi's chatter about how practice went."
    "El viaje al Aura-Mart es acompañado por la plática de Emi sobre la práctica."

# game/script-a3-emi.rpy:498
translate es a3_emi_definitions_b94a04f7:

    # "I keep quiet for the most part, merely enjoying the warmth of being around Emi."
    "Permanezco en silencio por la mayor parte, simplemente disfrutando de la calidez de estar cerca de Emi."

# game/script-a3-emi.rpy:500
translate es a3_emi_definitions_2975f30b:

    # "We get a few odd looks from passersby, but I don't mind."
    "Recibimos un par de miradas extrañas de los transeúntes, pero no me importa."

# game/script-a3-emi.rpy:502
translate es a3_emi_definitions_931be06b:

    # "We wind up buying some bread and instant noodles, realizing too late that we cannot actually cook the latter in the park."
    "Terminamos comprando algo de pan y fideos instantáneos, dándonos cuenta demasiado tarde de que en realidad no podemos cocinar estos últimos en el parque."

# game/script-a3-emi.rpy:507
translate es a3_emi_definitions_c0b1087b:

    # emi "Oh well. I'll make it for lunch or something."
    emi "Oh, bueno. Los haré para el almuerzo o algo así."

# game/script-a3-emi.rpy:509
translate es a3_emi_definitions_d858491e:

    # hi "That'll work."
    hi "Eso servirá."

# game/script-a3-emi.rpy:518
translate es a3_emi_definitions_3428ffca:

    # "The park is located after a brief loss of direction that I blame entirely on Emi."
    "Localizamos el parque luego de perdernos brevemente, algo por lo que culpo a Emi por completo."

# game/script-a3-emi.rpy:520
translate es a3_emi_definitions_4a3d4fc2:

    # "She, of course, blames me."
    "Ella, por supuesto, me culpa a mí."

# game/script-a3-emi.rpy:531
translate es a3_emi_definitions_1fc48247:

    # "We find a spot beneath a tree and sit down. I lean back against the trunk, Emi sits across from me."
    "Encontramos un lugar bajo un árbol y nos sentamos. Me recuesto contra el tronco, Emi se sienta frente a mí."

# game/script-a3-emi.rpy:535
translate es a3_emi_definitions_34ab8448:

    # hi "I guess we should have brought a blanket or something to sit on, huh?"
    hi "Supongo que deberíamos de haber traído una sábana o algo para sentarnos, ¿eh?"

# game/script-a3-emi.rpy:543
translate es a3_emi_definitions_18357699:

    # "Emi shrugs."
    "Emi se encoge de hombros."

# game/script-a3-emi.rpy:548
translate es a3_emi_definitions_5d96d514:

    # emi "I don't mind."
    emi "No me importa."

# game/script-a3-emi.rpy:550
translate es a3_emi_definitions_6e7ddf6e:

    # hi "Neither do I."
    hi "A mí tampoco."

# game/script-a3-emi.rpy:555
translate es a3_emi_definitions_d856fafa:

    # "Emi tosses me a package of bread and we dig in."
    "Emi me lanza un paquete de pan y empezamos a comer."

# game/script-a3-emi.rpy:557
translate es a3_emi_definitions_9c52b46c:

    # "Curry bread. Interesting."
    "Pan de curry. Interesante."

# game/script-a3-emi.rpy:559
translate es a3_emi_definitions_e0d3cfe1:

    # "I guess I wasn't really paying attention to what I grabbed in the store."
    "Supongo que no estaba poniendo mucha atención a lo que agarré en la tienda."

# game/script-a3-emi.rpy:564
translate es a3_emi_definitions_86ccae53:

    # emi "Hey, Hisao. You look like your bread's a little spicy."
    emi "Oye, Hisao. Parece que tu pan está algo picante."

# game/script-a3-emi.rpy:566
translate es a3_emi_definitions_50b71ca0:

    # "I shake my head, trying in vain to keep an image of manliness."
    "Sacudo la cabeza, intentando en vano mantener una imagen de masculinidad."

# game/script-a3-emi.rpy:568
translate es a3_emi_definitions_56db316a:

    # hi "Nah, it's hardly spicy at all."
    hi "Nah, apenas si pica."

# game/script-a3-emi.rpy:573
translate es a3_emi_definitions_98ecd4c9:

    # emi "I see, I see. That must be why your face has gotten so red."
    emi "Ya veo, ya veo. Debe de ser por eso que tu cara se ha puesto tan roja."

# game/script-a3-emi.rpy:575
translate es a3_emi_definitions_1611df17:

    # hi "Yes, exactly. The lack of spice has uh… gotten my blood up."
    hi "Sí, exacto. La falta de picante ha hecho que… se me suba la sangre a la cabeza."

# game/script-a3-emi.rpy:577
translate es a3_emi_definitions_340c3bb1:

    # hi "Because of the disappointment."
    hi "A causa de la decepción."

# game/script-a3-emi.rpy:582
translate es a3_emi_definitions_6440aad7:

    # "Emi laughs and swallows the last of her bread."
    "Emi ríe y se traga el resto de su pan."

# game/script-a3-emi.rpy:587
translate es a3_emi_definitions_70f1afd2:

    # emi "Well, if you can't handle it, I'll be glad to take it off of your hands."
    emi "Bueno, si no puedes con él, estaré encantada de quitártelo de las manos."

# game/script-a3-emi.rpy:589
translate es a3_emi_definitions_e4fd05f9:

    # hi "Hey, just because you wolfed down yours so quickly doesn't mean I'm just going to give you mine."
    hi "Oye, solo porque devoraste el tuyo tan rápido no significa que te voy a dar el mío."

# game/script-a3-emi.rpy:594
translate es a3_emi_definitions_283f3afa:

    # "Emi mock-pouts, causing me to nearly choke on my bread with laughter."
    "Emi bromea haciendo muecas, provocando que casi me atragante con mi pan por la risa."

# game/script-a3-emi.rpy:596
translate es a3_emi_definitions_674ffc1d:

    # emi "Aw, come on Hisao!"
    emi "Oh, ¡vamos, Hisao!"

# game/script-a3-emi.rpy:601
translate es a3_emi_definitions_0fe62206:

    # emi "Aren't you supposed to be concerned with making sure I've got enough to eat now?"
    emi "¿No se supone que ahora debes asegurarte de que yo tenga suficiente comida?"

# game/script-a3-emi.rpy:603
translate es a3_emi_definitions_e77ec99f:

    # emi "We're dating, you know!"
    emi "¡Estamos saliendo, sabes!"

# game/script-a3-emi.rpy:608
translate es a3_emi_definitions_7843de3e:

    # emi "Though…"
    emi "Aunque…"

# game/script-a3-emi.rpy:610
translate es a3_emi_definitions_8dbd99ba:

    # "Emi looks troubled all of a sudden."
    "De repente Emi se ve inquieta."

# game/script-a3-emi.rpy:615
translate es a3_emi_definitions_359d541d:

    # emi "I can't say I feel any different."
    emi "No puedo decir que me sienta diferente."

# game/script-a3-emi.rpy:617
translate es a3_emi_definitions_0ed9b29d:

    # hi "Hmm? What do you mean by that?"
    hi "¿Hmm? ¿A qué te refieres con eso?"

# game/script-a3-emi.rpy:622
translate es a3_emi_definitions_185bd514:

    # emi "What makes this a date?"
    emi "¿Qué hace de esto una cita?"

# game/script-a3-emi.rpy:624
translate es a3_emi_definitions_25ddb460:

    # emi "It's just what we would have done anyway, really."
    emi "Es simplemente lo que habríamos hecho de todos modos, la verdad."

# game/script-a3-emi.rpy:626
translate es a3_emi_definitions_e732113a:

    # emi "But this should feel different because before when we had lunch we were friends, and now we're a level above friends."
    emi "Pero esto debería sentirse distinto porque antes cuando almorzábamos eramos amigos, y ahora estamos un nivel por encima de amigos."

# game/script-a3-emi.rpy:628
translate es a3_emi_definitions_a4f727d4:

    # hi "You sound like Rin."
    hi "Suenas como Rin."

# game/script-a3-emi.rpy:633
translate es a3_emi_definitions_9bf45d72:

    # "Laughter escapes, and Emi grins."
    "La risa escapa, y Emi sonríe."

# game/script-a3-emi.rpy:638
translate es a3_emi_definitions_ab5e369e:

    # emi "Well, she might've put the thought into my mind."
    emi "Bueno, puede que ella haya puesto el pensamiento en mi mente."

# game/script-a3-emi.rpy:643
translate es a3_emi_definitions_cfa3e65c:

    # emi "We've talked about that sort of thing before."
    emi "Hemos hablado de estas cosas antes."

# game/script-a3-emi.rpy:645
translate es a3_emi_definitions_b5b7ab38:

    # hi "Really? About me?"
    hi "¿En serio? ¿De mí?"

# game/script-a3-emi.rpy:650
translate es a3_emi_definitions_80c5290c:

    # emi "Not really. Just… stuff, really."
    emi "No realmente. Solo… cosas, en realidad."

# game/script-a3-emi.rpy:655
translate es a3_emi_definitions_3d57e5f4:

    # emi "Rin thinks that the change of a label from 'friend' to 'girlfriend' seems arbitrary most of the time."
    emi "Rin cree que el cambio de término de “amiga” a “novia” parece arbitrario la mayor parte del tiempo."

# game/script-a3-emi.rpy:657
translate es a3_emi_definitions_8607efdf:

    # emi "Like there's no difference between the two."
    emi "Como si no hubiera diferencia entre los dos."

# game/script-a3-emi.rpy:659
translate es a3_emi_definitions_4365fd69:

    # hi "I can think of at least one, you know."
    hi "Puedo pensar en al menos una, sabes."

# game/script-a3-emi.rpy:661
translate es a3_emi_definitions_a47326ea:

    # hi "You don't tend to kiss your friends quite as much."
    hi "No sueles besar a tus amigos con tanta frecuencia."

# game/script-a3-emi.rpy:666
translate es a3_emi_definitions_ab0f69d4:

    # "For the second time today, Emi blushes slightly and giggles."
    "Por segunda vez en este día, Emi se sonroja ligeramente y suelta una risilla."

# game/script-a3-emi.rpy:671
translate es a3_emi_definitions_189bb8ec:

    # emi "I suppose you're right."
    emi "Creo que tienes razón."

# game/script-a3-emi.rpy:673
translate es a3_emi_definitions_74a346d6:

    # hi "Exactly. I'm always right about things like this."
    hi "Exacto. Siempre tengo razón en cosas como esta."

# game/script-a3-emi.rpy:678
translate es a3_emi_definitions_7053469f:

    # "Emi rolls her eyes and chuckles."
    "Emi pone los ojos en blanco y se ríe."

# game/script-a3-emi.rpy:680
translate es a3_emi_definitions_dba05d1d:

    # emi "Guess you're pretty smart, huh?"
    emi "Supongo que eres bastante listo, ¿eh?"

# game/script-a3-emi.rpy:682
translate es a3_emi_definitions_0066f219:

    # "I nod in agreement."
    "Asiento en acuerdo."

# game/script-a3-emi.rpy:684
translate es a3_emi_definitions_89a9aa28:

    # hi "Yep."
    hi "Síp."

# game/script-a3-emi.rpy:686
translate es a3_emi_definitions_187c0350:

    # hi "Even Mutou thinks so. He thinks I should go into some scientific study after graduation."
    hi "Hasta Mutou piensa eso. Él cree que yo debería estudiar alguna ciencia luego de la graduación."

# game/script-a3-emi.rpy:691
translate es a3_emi_definitions_3de31db1:

    # "Emi raises an eyebrow."
    "Emi levanta una ceja."

# game/script-a3-emi.rpy:693
translate es a3_emi_definitions_778a2d5a:

    # emi "Oh really?"
    emi "Oh, ¿en serio?"

# game/script-a3-emi.rpy:695
translate es a3_emi_definitions_f247f138:

    # hi "Yeah, I'm thinking I actually might do just that."
    hi "Sí, pienso que podría hacer exactamente eso."

# game/script-a3-emi.rpy:697
translate es a3_emi_definitions_ee390c3b:

    # "Really, the more I consider the idea, the more it appeals to me."
    "En realidad, entre más considero la idea, más me llama la atención."

# game/script-a3-emi.rpy:699
translate es a3_emi_definitions_1a5f1045:

    # "I make a mental note to look into it a little more closely."
    "Hago una nota mental para pensar más detenidamente en ello."

# game/script-a3-emi.rpy:701
translate es a3_emi_definitions_1ed9dcba:

    # hi "So what are you thinking of doing after graduation?"
    hi "Entonces, ¿qué piensas hacer luego de la graduación?"

# game/script-a3-emi.rpy:703
translate es a3_emi_definitions_66b207d9:

    # hi "Still planning on running?"
    hi "¿Aún piensas correr?"

# game/script-a3-emi.rpy:708
translate es a3_emi_definitions_7a2119e0:

    # "Emi shrugs, seeming almost a bit hesitant."
    "Emi se encoge de hombros, casi viéndose un poco insegura."

# game/script-a3-emi.rpy:713
translate es a3_emi_definitions_ee17fe9f:

    # emi "I dunno. If I'm good enough and I can find a team, I guess?"
    emi "No lo sé. ¿Si soy lo suficientemente buena y encuentro un equipo, supongo?"

# game/script-a3-emi.rpy:715
translate es a3_emi_definitions_56c5e17e:

    # hi "You mean you aren't sure?"
    hi "¿Quieres decir que no estás segura?"

# game/script-a3-emi.rpy:720
translate es a3_emi_definitions_78e9f252:

    # emi "I haven't… really thought about it, to be honest."
    emi "No he… pensado realmente en eso, para ser honesta."

# game/script-a3-emi.rpy:722
translate es a3_emi_definitions_ff728ce7:

    # hi "Really?"
    hi "¿En serio?"

# game/script-a3-emi.rpy:724
translate es a3_emi_definitions_79aab26d:

    # hi "You probably should, you know. Graduation isn't that far off."
    hi "Probablemente deberías, sabes. La graduación no está muy lejos."

# game/script-a3-emi.rpy:729
translate es a3_emi_definitions_4ac557af:

    # "Emi fidgets a little nervously."
    "Emi se retuerce un poco, nerviosa."

# game/script-a3-emi.rpy:731
translate es a3_emi_definitions_adf1802f:

    # emi "Yeah, well… it's far enough, right?"
    emi "Sí, bueno… está lo suficientemente lejos, ¿cierto?"

# game/script-a3-emi.rpy:736
translate es a3_emi_definitions_abc055a6:

    # emi "Besides, I've got other things to think about."
    emi "Además, tengo otras cosas en las que pensar."

# game/script-a3-emi.rpy:741
translate es a3_emi_definitions_534a36fa:

    # "There's a mischievous flash behind Emi's eyes, and I suddenly find myself gloriously pinned against the tree."
    "Hay un travieso resplandor tras los ojos de Emi, y de repente me encuentro gloriosamente atrapado contra el árbol."

# game/script-a3-emi.rpy:746
translate es a3_emi_definitions_757b745b:

    # emi "Like making sure this is a real date, right?"
    emi "Como asegurarme de que esta es una verdadera cita, ¿cierto?"

# game/script-a3-emi.rpy:751
translate es a3_emi_definitions_34b65fd9:

    # emi "I mean if we don't kiss then it's not a date at all, right?"
    emi "Quiero decir, si no nos besamos entonces no es para nada una cita, ¿cierto?"

# game/script-a3-emi.rpy:753
translate es a3_emi_definitions_fb9580dd:

    # hi "I suppose s— mmmph." with vpunch
    hi "Supongo qu— mmmff." with vpunch

# game/script-a3-emi.rpy:755
translate es a3_emi_definitions_2eea2b94:

    # "Strawberries and curry. Not the best combination, but I don't think I mind."
    "Fresas y curry. No es la mejor combinación, pero no creo que importe."

# game/script-a3-emi.rpy:760
translate es a3_emi_definitions_6eb7b73a:

    # "Emi sits back on my legs and grins again."
    "Emi se sienta en mis piernas y sonríe de nuevo."

# game/script-a3-emi.rpy:762
translate es a3_emi_definitions_9cd04634:

    # emi "There. Science would approve, right?"
    emi "Ahí. La ciencia lo aprobaría, ¿cierto?"

# game/script-a3-emi.rpy:764
translate es a3_emi_definitions_5f131064:

    # "I have the oddest mental image of Mutou nodding seriously and making a mark on some checklist."
    "Tengo la extrañísima imagen mental de Mutou asintiendo seriamente y haciendo una marca en alguna lista de verificación."

# game/script-a3-emi.rpy:766
translate es a3_emi_definitions_8dcd2f99:

    # "I can't help laughing at the idea."
    "No puedo evitar reírme de la idea."

# game/script-a3-emi.rpy:771
translate es a3_emi_definitions_a334c6e2:

    # emi "Well I'll admit, this is the first time I've ever witnessed a kiss being met with laughter."
    emi "Bueno lo admito, esta es la primera vez que veo un beso terminar en una carcajada."

# game/script-a3-emi.rpy:773
translate es a3_emi_definitions_9ad75397:

    # emi "Should I feel offended?"
    emi "¿Debería de sentirme ofendida?"

# game/script-a3-emi.rpy:775
translate es a3_emi_definitions_78097132:

    # hi "Heh, no, no."
    hi "Jeje, no, no."

# game/script-a3-emi.rpy:777
translate es a3_emi_definitions_e75a4180:

    # hi "I'm sure science approves."
    hi "Estoy seguro de que la ciencia lo aprueba."

# game/script-a3-emi.rpy:782
translate es a3_emi_definitions_082fc6bd:

    # "Emi beams at me, and I find it increasingly difficult to keep my brain functioning properly."
    "Emi me sonríe radiante, y cada vez me parece más difícil mantener a mi cerebro funcionando adecuadamente."

# game/script-a3-emi.rpy:784
translate es a3_emi_definitions_b1c20091:

    # emi "Oh good!"
    emi "¡Oh, bien!"

# game/script-a3-emi.rpy:786
translate es a3_emi_definitions_6f8f5ce2:

    # "It is at this point I notice that Emi has stolen the remainder of my curry bread while I was otherwise occupied with images of teachers wielding clipboards."
    "Es en este momento cuando noto que Emi ha robado el resto de mi pan de curry mientras yo estaba ocupado por otro lado con imágenes de maestros sosteniendo portapapeles."

# game/script-a3-emi.rpy:788
translate es a3_emi_definitions_d5322af5:

    # hi "Hey!"
    hi "¡Oye!"

# game/script-a3-emi.rpy:793
translate es a3_emi_definitions_16bdb200:

    # "Emi tries to look innocent, but considering she's just crammed the last bits of my bread into her mouth it does not appear to be working."
    "Emi intenta verse inocente, pero, considerando que acaba de atiborrarse los últimos pedazos de mi pan en la boca, no parece estar funcionando."

# game/script-a3-emi.rpy:795
translate es a3_emi_definitions_5124487e:

    # emi "Mmph? F'orry, couln't refisft."
    emi "¿Mmff? Ferdón, no fude refisftir."

# game/script-a3-emi.rpy:797
translate es a3_emi_definitions_60aeb693:

    # hi "Thief!"
    hi "¡Ladrona!"

# game/script-a3-emi.rpy:802
translate es a3_emi_definitions_c1609e14:

    # "A shrug from my companion is all I get in response."
    "Un encogimiento de hombros de mi acompañante es todo lo que recibo como respuesta."

# game/script-a3-emi.rpy:804
translate es a3_emi_definitions_a583e1e9:

    # hi "You used your feminine wiles on me!"
    hi "¡Usaste tu encanto femenino en mí!"

# game/script-a3-emi.rpy:806
translate es a3_emi_definitions_9ceb18fd:

    # "I wasn't that hungry anyway, but I still feel that the point needs to be made."
    "Igual no tenía tanta hambre, pero aun así siento que debo hacer el comentario."

# game/script-a3-emi.rpy:811
translate es a3_emi_definitions_3ad5a485:

    # "Emi seems confused by the phrase 'feminine wiles,' but the understanding dawns on her features after a moment's thought."
    "Emi parece confundida por la frase “encanto femenino”, pero la comprensión ilumina sus rasgos luego de unos momentos de reflexión."

# game/script-a3-emi.rpy:816
translate es a3_emi_definitions_c32ac0f7:

    # emi "Wasn't anything of the sort!"
    emi "¡No tuvo nada que ver con eso!"

# game/script-a3-emi.rpy:821
translate es a3_emi_definitions_5126aeb3:

    # emi "You were laughing! Feminine wiles don't involve laughing!"
    emi "¡Te estabas riendo! ¡Si usara mi encanto femenino no te reirías!"

# game/script-a3-emi.rpy:823
translate es a3_emi_definitions_469cb850:

    # "I guess I can't argue with this."
    "Supongo que no puedo rebatir eso."

# game/script-a3-emi.rpy:825
translate es a3_emi_definitions_2838d1b0:

    # hi "That doesn't change your thievery."
    hi "Eso no cambia tu robo."

# game/script-a3-emi.rpy:830
translate es a3_emi_definitions_830b4959:

    # "Emi laughs at my injured tone and gives me a playful shove."
    "Emi se ríe de mi tono afligido y me da un golpecito juguetón."

# game/script-a3-emi.rpy:835
translate es a3_emi_definitions_5006dc3c:

    # emi "Fine, you can have the instant noodles."
    emi "Bien, puedes quedarte con los fideos instantáneos."

# game/script-a3-emi.rpy:837
translate es a3_emi_definitions_2cf5043f:

    # hi "Are you kidding? That stuff's terrible!"
    hi "¿Estás bromeando? ¡Esas cosas son horribles!"

# game/script-a3-emi.rpy:839
translate es a3_emi_definitions_6a9d4567:

    # hi "If anything, you should definitely eat it as punishment!"
    hi "¡En todo caso, deberías comértelos como castigo!"

# game/script-a3-emi.rpy:844
translate es a3_emi_definitions_1fb59a7d:

    # "Another laugh from the girl sitting on my legs."
    "Otra carcajada de la chica sentada en mis piernas."

# game/script-a3-emi.rpy:846
translate es a3_emi_definitions_c470ba8c:

    # "…Both of which have fallen asleep by now."
    "… Las cuales ya se han dormido."

# game/script-a3-emi.rpy:863
translate es a3_emi_definitions_2ec1ccdd:

    # "I twitch one leg to try waking it up, which has the unintended effect of unbalancing Emi, who falls to the side with a startled 'Eep!'"
    "Retuerzo una pierna intentando despertarla, lo que tiene el indeseado efecto de desbalancear a Emi, quien cae de lado con un asustado ¡“Ayy”!"

# game/script-a3-emi.rpy:865
translate es a3_emi_definitions_291ee975:

    # hi "Whoops! Sorry about that."
    hi "¡Oops! Disculpa."

# game/script-a3-emi.rpy:867
translate es a3_emi_definitions_273ebd02:

    # hi "Legs fell asleep on me."
    hi "Se me durmieron las piernas."

# game/script-a3-emi.rpy:869
translate es a3_emi_definitions_3f396131:

    # "Emi remains on the ground, giggling."
    "Emi se queda en el suelo, riéndose infantilmente."

# game/script-a3-emi.rpy:871
translate es a3_emi_definitions_5f8dd429:

    # "I stand up a little shakily, feeling the nerves in my legs return to normal."
    "Me levanto algo tembloroso, sintiendo los nervios de mis piernas regresar a la normalidad."

# game/script-a3-emi.rpy:873
translate es a3_emi_definitions_0952903a:

    # "My eyes wander over the scenery before fixing on the figure of Emi, who has yet to get up."
    "Mis ojos vagan por el escenario hasta fijarse en la figura de Emi, que aún no se ha levantado."

# game/script-a3-emi.rpy:880
translate es a3_emi_definitions_4b15e252:

    # "Her hair is splayed out around her head, her arms are spread, and laughter is bubbling up through her mouth."
    "Su cabello está extendido alrededor de su cabeza, sus brazos abiertos, y la risa borbotea en su boca."

# game/script-a3-emi.rpy:882
translate es a3_emi_definitions_a5f2b003:

    # "Everything about Emi seems condensed into this one image."
    "Todo sobre Emi parece estar condensado en esta imagen."

# game/script-a3-emi.rpy:884
translate es a3_emi_definitions_ed9b9d89:

    # "Her energy, her spirit, her childish giggling."
    "Su energía, su espíritu, sus risas infantiles."

# game/script-a3-emi.rpy:886
translate es a3_emi_definitions_b1033e1c:

    # "The urge to lay down on the grass with her rises swiftly from the back of my mind to the forefront of my thoughts, and indeed I am convinced that I would love nothing more than to do just that."
    "La urgencia de acostarme en el césped con ella acelera rápidamente desde el fondo de mi mente hasta el primer plano de mis pensamientos, y realmente estoy convencido de que nada me gustaría más que hacer eso."

# game/script-a3-emi.rpy:888
translate es a3_emi_definitions_0ddc4540:

    # "Unfortunately the sun has set, and it is probably time for us to get back to the dormitories."
    "Desafortunadamente el sol se ha ocultado, y probablemente sea tiempo de que regresemos a los dormitorios."

# game/script-a3-emi.rpy:890
translate es a3_emi_definitions_aa57e11c:

    # "While Emi may be happy to stay out here all night, I don't think I have that ability."
    "Mientras Emi podría ser feliz quedándose aquí afuera toda la noche, no creo que yo tenga esa capacidad."

# game/script-a3-emi.rpy:892
translate es a3_emi_definitions_7c9fa530:

    # "Besides, homework soon beckons."
    "Además, la tarea me espera."

# game/script-a3-emi.rpy:894
translate es a3_emi_definitions_aed72eb6:

    # "It wouldn't make sense to start thinking about things like university and then slack off, would it?"
    "No tendría sentido empezar a considerar cosas como la universidad y luego aflojar el ritmo, ¿verdad?"

# game/script-a3-emi.rpy:896
translate es a3_emi_definitions_32f3fc84:

    # "I extend a hand to Emi to help her up."
    "Extiendo una mano para ayudar a Emi a levantarse."

# game/script-a3-emi.rpy:898
translate es a3_emi_definitions_51333a40:

    # hi "We should probably get going."
    hi "Probablemente deberíamos irnos."

# game/script-a3-emi.rpy:903
translate es a3_emi_definitions_4286a386:

    # "Emi makes a sour face."
    "Emi pone una mueca de irritación."

# game/script-a3-emi.rpy:905
translate es a3_emi_definitions_460a04ca:

    # emi "You're right."
    emi "Tienes razón."

# game/script-a3-emi.rpy:918
translate es a3_emi_definitions_08f83901:

    # "She grabs my proffered hand, and I pull her to her feet and into a hug."
    "Ella toma mi mano extendida, y la traigo sobre sus pies y hacia un abrazo."

# game/script-a3-emi.rpy:920
translate es a3_emi_definitions_b8bd7b98:

    # "This time I'm the one who kisses her, unable to resist having Emi against me."
    "Esta vez soy yo el que la beso, incapaz de resistir el tener a Emi contra mí."

# game/script-a3-emi.rpy:922
translate es a3_emi_definitions_dc71697a:

    # hi "Seems a shame to leave, you know."
    hi "Sería una lástima irse, sabes."

# game/script-a3-emi.rpy:927
translate es a3_emi_definitions_7cbaab54:

    # emi "Yeah, it does."
    emi "Sí, lo sería."

# game/script-a3-emi.rpy:932
translate es a3_emi_definitions_6d58aebc:

    # emi "But if we don't get back to the school soon, we'll probably get into trouble."
    emi "Pero si no volvemos pronto a la escuela, probablemente nos metamos en problemas."

# game/script-a3-emi.rpy:934
translate es a3_emi_definitions_4790dc4f:

    # "Emi pokes me in the ribs playfully."
    "Emi me da un golpecito travieso en las costillas."

# game/script-a3-emi.rpy:939
translate es a3_emi_definitions_6b65809e:

    # emi "And you need to do your homework, I'm sure."
    emi "Y tú necesitas hacer la tarea, estoy segura."

# game/script-a3-emi.rpy:941
translate es a3_emi_definitions_37495b88:

    # hi "Sadly, you're absolutely right."
    hi "Desgraciadamente, tienes toda la razón."

# game/script-a3-emi.rpy:946
translate es a3_emi_definitions_7613d788:

    # "I throw my arm around her shoulders, and we make the trek back to the school, accompanied by occasional bouts of laughter as our conversation jumps from subject to subject."
    "Pongo mi brazo sobre sus hombros, y caminamos el trayecto de vuelta a la escuela, acompañados por ocasionales episodios de risa mientras nuestra conversación salta de un tema a otro."

# game/script-a3-emi.rpy:948
translate es a3_emi_definitions_a0b31930:

    # "Everything from running, to school, to the peculiar way that one of the cafeteria workers smells."
    "Todo desde correr, hasta la escuela, hasta la peculiar manera en la que huele uno de los empleados de la cafetería."

# game/script-a3-emi.rpy:955
translate es a3_emi_definitions_7197d70c:

    # "All too soon we find ourselves outside of the girls' dormitory building."
    "Y así, muy repentinamente nos encontramos fuera del edificio del dormitorio de las chicas."

# game/script-a3-emi.rpy:960
translate es a3_emi_definitions_1d97fae6:

    # emi "Well, I guess I'll be going, then."
    emi "Bueno, supongo que me voy, entonces."

# game/script-a3-emi.rpy:962
translate es a3_emi_definitions_29155028:

    # hi "I guess so, huh?"
    hi "Supongo, ¿eh?"

# game/script-a3-emi.rpy:967
translate es a3_emi_definitions_2f477bb4:

    # "Emi grins at me again with that mischievous look."
    "Emi me sonríe de nuevo con esa mirada traviesa."

# game/script-a3-emi.rpy:969
translate es a3_emi_definitions_ae031d32:

    # emi "Are you going to be able to survive without me?"
    emi "¿Serás capaz de sobrevivir sin mí?"

# game/script-a3-emi.rpy:971
translate es a3_emi_definitions_e33f02ef:

    # "I laugh."
    "Me río."

# game/script-a3-emi.rpy:973
translate es a3_emi_definitions_f524916a:

    # hi "I'm sure I'll manage."
    hi "Estoy seguro de que me las arreglaré."

# game/script-a3-emi.rpy:978
translate es a3_emi_definitions_61a5fa32:

    # emi "How terrible! Aren't you supposed to say something like 'I'll be counting the seconds you are away?'"
    emi "¡Qué horrible! ¿No se supone que digas algo como “estaré contando los segundos que estés lejos”?"

# game/script-a3-emi.rpy:980
translate es a3_emi_definitions_c25de446:

    # hi "Nah, I don't think so."
    hi "Nah, no lo creo."

# game/script-a3-emi.rpy:988
translate es a3_emi_definitions_81b3f5c2:

    # "Emi pulls me down into a quick goodbye kiss and steps back, looking unexpectedly shy."
    "Emi me jala hacia un rápido beso de despedida y da un paso atrás, viéndose inesperadamente tímida."

# game/script-a3-emi.rpy:990
translate es a3_emi_definitions_476cb873:

    # emi "Thanks for dinner."
    emi "Gracias por la cena."

# game/script-a3-emi.rpy:992
translate es a3_emi_definitions_d0f45900:

    # emi "I really had fun."
    emi "Realmente me divertí."

# game/script-a3-emi.rpy:997
translate es a3_emi_definitions_30b916fe:

    # emi "Honestly, I did."
    emi "En serio, lo hice."

# game/script-a3-emi.rpy:999
translate es a3_emi_definitions_659fb68f:

    # hi "So did I."
    hi "Igual yo."

# game/script-a3-emi.rpy:1001
translate es a3_emi_definitions_d0f01ea9:

    # hi "I think we shall have to do it again, sometime."
    hi "Creo que tendremos que hacerlo de nuevo, en algún momento."

# game/script-a3-emi.rpy:1006
translate es a3_emi_definitions_f3d180ed:

    # "Emi laughs at my deadpan delivery and nods."
    "Emi se ríe de mi inexpresivo comentario y asiente."

# game/script-a3-emi.rpy:1008
translate es a3_emi_definitions_0ec3426f:

    # emi "See you bright and early tomorrow morning, right?"
    emi "Te veo temprano mañana por la mañana, ¿verdad?"

# game/script-a3-emi.rpy:1013
translate es a3_emi_definitions_793f8f55:

    # emi "You've gotta run off that bread, after all."
    emi "Tienes que quemar ese pan, después de todo."

# game/script-a3-emi.rpy:1015
translate es a3_emi_definitions_ffe65bfb:

    # hi "Of course. Despite the fact that you ate most of it."
    hi "Claro. A pesar de que tú te lo comiste casi todo."

# game/script-a3-emi.rpy:1020
translate es a3_emi_definitions_df12de13:

    # emi "Yes, despite that."
    emi "Sí, a pesar de eso."

# game/script-a3-emi.rpy:1025
translate es a3_emi_definitions_5bf52d09:

    # emi "See you later, Hisao!"
    emi "¡Te veo luego, Hisao!"

# game/script-a3-emi.rpy:1036
translate es a3_emi_definitions_eeeab59e:

    # "As Emi turns to head inside, I notice something weird."
    "Cuando Emi se da la vuelta para entrar, noto algo extraño."

# game/script-a3-emi.rpy:1038
translate es a3_emi_definitions_ec05250b:

    # "Something so weird that I'm surprised I didn't notice it earlier."
    "Algo tan extraño que estoy sorprendido de no haberlo notado antes."

# game/script-a3-emi.rpy:1040
translate es a3_emi_definitions_61603032:

    # "She's limping slightly, favoring the left leg."
    "Ella cojea levemente, favoreciendo la pierna izquierda."

# game/script-a3-emi.rpy:1044
translate es a3_emi_definitions_8659879e:

    # hi "Hey, Emi!"
    hi "¡Oye, Emi!"

# game/script-a3-emi.rpy:1055
translate es a3_emi_definitions_19972527:

    # emi "Hmm?"
    emi "¿Hmm?"

# game/script-a3-emi.rpy:1057
translate es a3_emi_definitions_79b3d0dc:

    # hi "Is your leg okay?"
    hi "¿Está bien tu pierna?"

# game/script-a3-emi.rpy:1062
translate es a3_emi_definitions_120a2642:

    # "Emi looks confused, or at least fakes confusion."
    "Emi se ve confundida, o al menos finge confusión."

# game/script-a3-emi.rpy:1067
translate es a3_emi_definitions_a0a354f2:

    # emi "What are you talking about?"
    emi "¿De qué estás hablando?"

# game/script-a3-emi.rpy:1069
translate es a3_emi_definitions_21b9321f:

    # hi "Your right leg. You're limping."
    hi "Tu pierna derecha. Estás cojeando."

# game/script-a3-emi.rpy:1077
translate es a3_emi_definitions_afd871b2:

    # "There's the briefest flash of concern on Emi's face."
    "Hay un brevísimo destello de preocupación en el rostro de Emi."

# game/script-a3-emi.rpy:1079
translate es a3_emi_definitions_9e79cfcb:

    # "Either she didn't want me to know, or she didn't think I'd notice - or, I prefer to think, she just didn't realize it."
    "O ella no quería que yo supiera, o creyó que no me daría cuenta… o, preferiría creer, simplemente no se había dado cuenta."

# game/script-a3-emi.rpy:1084
translate es a3_emi_definitions_508517ae:

    # emi "Oh, that."
    emi "Oh, eso."

# game/script-a3-emi.rpy:1086
translate es a3_emi_definitions_88ed0cab:

    # "She shrugs casually."
    "Ella se encoge casualmente de hombros."

# game/script-a3-emi.rpy:1091
translate es a3_emi_definitions_3661c41b:

    # emi "Must've gotten knocked a little out of alignment during the picnic."
    emi "Debe de haberse desacomodado un poco durante el picnic."

# game/script-a3-emi.rpy:1096
translate es a3_emi_definitions_b7440726:

    # emi "No idea what would have caused that, of course."
    emi "Ni idea de qué podría haberlo ocasionado, por supuesto."

# game/script-a3-emi.rpy:1098
translate es a3_emi_definitions_4a31df9c:

    # "I think back to being pinned under the tree."
    "Retrocedo mis pensamientos hasta estar atrapado bajo el árbol."

# game/script-a3-emi.rpy:1100
translate es a3_emi_definitions_9a287fc4:

    # hi "Ah."
    hi "Oh."

# game/script-a3-emi.rpy:1102
translate es a3_emi_definitions_8f9dff5f:

    # hi "You should have told me! We could have stopped and fixed it, you know."
    hi "¡Deberías habérmelo dicho! Podríamos habernos detenido y arreglarlo, sabes."

# game/script-a3-emi.rpy:1104
translate es a3_emi_definitions_0b8e06ce:

    # "Emi waves a hand airily."
    "Emi sacude una mano despreocupadamente."

# game/script-a3-emi.rpy:1109
translate es a3_emi_definitions_ec47ea25:

    # emi "Nah, it's not that big of a deal."
    emi "Nah, no es para tanto."

# game/script-a3-emi.rpy:1114
translate es a3_emi_definitions_8977c72c:

    # emi "Don't worry about it, okay Hisao?"
    emi "No te preocupes por eso, ¿de acuerdo, Hisao?"

# game/script-a3-emi.rpy:1119
translate es a3_emi_definitions_907d83cd:

    # emi "It's fine."
    emi "Está bien."

# game/script-a3-emi.rpy:1137
translate es a3_emi_definitions_4cdf677a:

    # emi "Now really, I need to get going."
    emi "Ahora en serio, necesito irme."

# game/script-a3-emi.rpy:1142
translate es a3_emi_definitions_6f625974:

    # emi "Your attempts to keep me around are doomed to fail!"
    emi "¡Tus intentos de mantenerme aquí están condenados al fracaso!"

# game/script-a3-emi.rpy:1144
translate es a3_emi_definitions_28f6dfcb:

    # hi "Heh, of course."
    hi "Jeje, por supuesto."

# game/script-a3-emi.rpy:1146
translate es a3_emi_definitions_8b0690d1:

    # hi "Just prolonging the goodbye, I suppose."
    hi "Solo prolongaba la despedida, supongo."

# game/script-a3-emi.rpy:1148
translate es a3_emi_definitions_d6c7a476:

    # "Another grin lights up Emi's face."
    "Otra sonrisa ilumina el rostro de Emi."

# game/script-a3-emi.rpy:1153
translate es a3_emi_definitions_b7ee3ae8:

    # emi "Goodnight, Hisao."
    emi "Buenas noches, Hisao."

# game/script-a3-emi.rpy:1155
translate es a3_emi_definitions_fbcbd67e:

    # hi "Goodnight."
    hi "Buenas noches."

# game/script-a3-emi.rpy:1162
translate es a3_emi_definitions_11ad5102:

    # "As she limps inside, I find myself hoping she's okay despite her assurances that she's fine."
    "Mientras ella entra cojeando, espero que realmente esté bien, a pesar de sus intentos de convencerme de que lo está."

# game/script-a3-emi.rpy:1164
translate es a3_emi_definitions_c3e802b2:

    # "I think I can call this a successful first date."
    "Creo que puedo llamar a esta una primera cita exitosa."

# game/script-a3-emi.rpy:1166
translate es a3_emi_definitions_76862fb7:

    # "Hell, any day that ends with Emi pinning me under a tree to kiss me can't be bad, can it?"
    "Diablos, cualquier día que termine con Emi arrinconándome bajo un árbol para besarme no puede ser malo, ¿verdad?"

# game/script-a3-emi.rpy:1168
translate es a3_emi_definitions_1abe9e96:

    # "I head back to my room, mentally thank the gods that Kenji doesn't ambush me in the hallway, and get started on my homework."
    "Me dirijo de vuelta a mi habitación, agradezco mentalmente a los dioses que Kenji no me emboscó en el pasillo, y empiezo con mi tarea."

# game/script-a3-emi.rpy:1184
translate es a3_emi_invisible_rock_685a5748:

    # "The morning is far too early for my taste."
    "La mañana llega demasiado temprano para mi gusto."

# game/script-a3-emi.rpy:1186
translate es a3_emi_invisible_rock_ee0f0fa3:

    # "It doesn't help that I had trouble sleeping last night."
    "Y no ayuda haber tenido problemas para dormir anoche."

# game/script-a3-emi.rpy:1188
translate es a3_emi_invisible_rock_92af9957:

    # "There were simply too many things to think about. My mind refused to slow down."
    "Simplemente había demasiadas cosas en las que pensar. Mi cerebro no quería descansar."

# game/script-a3-emi.rpy:1190
translate es a3_emi_invisible_rock_42266a4c:

    # "Instead I replayed the rooftop, the park, and everything else over and over in my mind."
    "En vez de eso, repetí lo de la azotea, el parque, y todo lo demás una y otra vez en mi mente."

# game/script-a3-emi.rpy:1192
translate es a3_emi_invisible_rock_a104ba75:

    # "There's a small part of my mind that is still paranoid that this has all been some kind of joke."
    "Hay una pequeña parte de mi mente que aún está paranoica con que todo esto ha sido alguna especie de broma."

# game/script-a3-emi.rpy:1194
translate es a3_emi_invisible_rock_5a108556:

    # "That I'll meet up with Emi at the track, and she'll act like nothing happened yesterday."
    "Que me encontraré con Emi en la pista, y ella actuará como si no hubiera pasado nada ayer."

# game/script-a3-emi.rpy:1196
translate es a3_emi_invisible_rock_89a9127c:

    # "Pushing these thoughts to the back of my mind, I throw on my running clothes and open the door."
    "Empujando estos pensamientos hasta el fondo de mi mente, me pongo la ropa para correr y abro la puerta."

# game/script-a3-emi.rpy:1202
translate es a3_emi_invisible_rock_2b82c8ff:

    # "Emi's waiting for me with her usual smile."
    "Emi está esperándome con su sonrisa habitual."

# game/script-a3-emi.rpy:1207
translate es a3_emi_invisible_rock_d198e095:

    # emi "You're late!"
    emi "¡Llegas tarde!"

# game/script-a3-emi.rpy:1212
translate es a3_emi_invisible_rock_f0c39461:

    # emi "Or at least, you're not early today."
    emi "O al menos no llegaste temprano hoy."

# game/script-a3-emi.rpy:1217
translate es a3_emi_invisible_rock_2ecf04ef:

    # emi "Are you tired or something?"
    emi "¿Estás cansado o algo?"

# game/script-a3-emi.rpy:1219
translate es a3_emi_invisible_rock_d039dee4:

    # "I find myself ruefully rubbing the back of my head."
    "Me froto tristemente la parte trasera de la cabeza."

# game/script-a3-emi.rpy:1221
translate es a3_emi_invisible_rock_e004f71c:

    # hi "Something like that, yeah."
    hi "Sí, algo así."

# game/script-a3-emi.rpy:1223
translate es a3_emi_invisible_rock_50d3d545:

    # hi "Lots to think about and all that."
    hi "Mucho en lo que pensar y todo eso."

# game/script-a3-emi.rpy:1228
translate es a3_emi_invisible_rock_d5c6d9dd:

    # "Emi giggles at my mild understatement."
    "Emi se ríe con mi leve indirecta."

# game/script-a3-emi.rpy:1233
translate es a3_emi_invisible_rock_e5033404:

    # emi "Yeah, I didn't sleep that well either."
    emi "Sí, yo tampoco dormí muy bien."

# game/script-a3-emi.rpy:1238
translate es a3_emi_invisible_rock_c3609a0e:

    # emi "I was actually glad you weren't early, 'cause I wasn't early either."
    emi "La verdad me alegra que no hayas llegado temprano, porque yo tampoco lo hice."

# game/script-a3-emi.rpy:1240
translate es a3_emi_invisible_rock_7ce3aab6:

    # "I wonder if the same thing kept us awake."
    "Me pregunto si lo mismo nos mantuvo despiertos."

# game/script-a3-emi.rpy:1242
translate es a3_emi_invisible_rock_a6a67833:

    # "The image of her weeping face passes through my mind."
    "La imagen de su rostro lleno de lágrimas pasa por mi mente."

# game/script-a3-emi.rpy:1244
translate es a3_emi_invisible_rock_fdcc44c9:

    # hi "What kept you up?"
    hi "¿Qué te mantuvo despierta?"

# game/script-a3-emi.rpy:1249
translate es a3_emi_invisible_rock_8b6da760:

    # "Emi's expression falters, but she quickly notices my curiosity and forces a smile."
    "La expresión de Emi se tambalea, pero nota mi curiosidad con rapidez y fuerza una sonrisa."

# game/script-a3-emi.rpy:1254
translate es a3_emi_invisible_rock_659d84e0:

    # emi "Nothing important."
    emi "Nada importante."

# game/script-a3-emi.rpy:1256
translate es a3_emi_invisible_rock_2f62c72a:

    # "She's obviously not telling me something."
    "Obviamente no está diciéndome algo."

# game/script-a3-emi.rpy:1258
translate es a3_emi_invisible_rock_aaaf87c8:

    # "The question is, should I press the issue?"
    "La pregunta es, ¿debería de presionarla?"

# game/script-a3-emi.rpy:1260
translate es a3_emi_invisible_rock_145b86db:

    # "Something's clearly been bothering her for a while."
    "Claramente algo la ha estado molestando desde hace un tiempo."

# game/script-a3-emi.rpy:1262
translate es a3_emi_invisible_rock_11265078:

    # "I want to help her, but would it just come off as me being nosy?"
    "Quiero ayudarla, pero, ¿parecería que solo soy un entrometido?"

# game/script-a3-emi.rpy:1264
translate es a3_emi_invisible_rock_cc73f35c:

    # "She's got to know I care about her, though."
    "Aun así, debería de saber que me preocupo por ella."

# game/script-a3-emi.rpy:1266
translate es a3_emi_invisible_rock_cf490c98:

    # hi "Are you sure?"
    hi "¿Estás segura?"

# game/script-a3-emi.rpy:1268
translate es a3_emi_invisible_rock_342852e7:

    # hi "If something's bothering you, I'm here to help you sort it out."
    hi "Si algo te molesta, estoy aquí para ayudarte a resolverlo."

# game/script-a3-emi.rpy:1273
translate es a3_emi_invisible_rock_e11dcbea:

    # "Emi laughs then, but it's not her usual laugh. There's an edge to it that seems almost bitter."
    "Emi entonces se ríe, pero no es la risa de siempre. Tiene un tono que parece casi amargo."

# game/script-a3-emi.rpy:1278
translate es a3_emi_invisible_rock_772cc45f:

    # emi "Sort it out?"
    emi "¿Resolverlo?"

# game/script-a3-emi.rpy:1280
translate es a3_emi_invisible_rock_23c11ec4:

    # emi "I'm not sure it can be sorted out, Hisao."
    emi "No estoy segura de que pueda resolverse, Hisao."

# game/script-a3-emi.rpy:1282
translate es a3_emi_invisible_rock_a7313e8b:

    # "An almost grim smile crosses her lips."
    "Una sonrisa casi siniestra cruza sus labios."

# game/script-a3-emi.rpy:1284
translate es a3_emi_invisible_rock_c3d29ec8:

    # "It's like a smile of resignation."
    "Es como una sonrisa de resignación."

# game/script-a3-emi.rpy:1289
translate es a3_emi_invisible_rock_1cb7c597:

    # emi "I don't think you could help me, anyway."
    emi "Y no creo que puedas ayudarme, de todas maneras."

# game/script-a3-emi.rpy:1291
translate es a3_emi_invisible_rock_ead401c2:

    # "That hurts."
    "Eso duele."

# game/script-a3-emi.rpy:1293
translate es a3_emi_invisible_rock_80db5263:

    # "I don't want to say that it hurts to her, but it does."
    "No quiero decirle que me duele, pero lo hace."

# game/script-a3-emi.rpy:1295
translate es a3_emi_invisible_rock_00b8ebe3:

    # "Doesn't she realize I want to be there for her when things go wrong?"
    "¿No se da cuenta de que quiero estar ahí para ella cuando las cosas anden mal?"

# game/script-a3-emi.rpy:1297
translate es a3_emi_invisible_rock_6978d4ff:

    # hi "Well, I won't push you on the matter."
    hi "Bueno, no te presionaré con eso."

# game/script-a3-emi.rpy:1299
translate es a3_emi_invisible_rock_7b6ea813:

    # hi "But I'm here for you if you decide later that you'd like to talk about it."
    hi "Pero estoy aquí para ti si luego decides que te gustaría hablarlo."

# game/script-a3-emi.rpy:1301
translate es a3_emi_invisible_rock_e818da8c:

    # hi "It might help."
    hi "Podría ayudar."

# game/script-a3-emi.rpy:1306
translate es a3_emi_invisible_rock_7f48c783:

    # "I can see the debate raging behind Emi's eyes."
    "Puedo ver el furioso debate tras los ojos de Emi."

# game/script-a3-emi.rpy:1308
translate es a3_emi_invisible_rock_8f5561e4:

    # "It seems like she wants to tell me, but she's not sure whether or not she can."
    "Parece que quiere contármelo, pero no está segura de si puede hacerlo o no."

# game/script-a3-emi.rpy:1310
translate es a3_emi_invisible_rock_8a983934:

    # hi "Hey, forget about it for now, okay?"
    hi "Oye, olvídate de ello por ahora, ¿de acuerdo?"

# game/script-a3-emi.rpy:1312
translate es a3_emi_invisible_rock_7a92df35:

    # hi "We've got running to do."
    hi "Tenemos que correr."

# game/script-a3-emi.rpy:1314
translate es a3_emi_invisible_rock_69251a59:

    # "The mention of running, something that she can handle, brings Emi back to her usual self."
    "Con la mención de correr, algo que puede controlar, Emi regresa a ser la misma de siempre."

# game/script-a3-emi.rpy:1319
translate es a3_emi_invisible_rock_5acebdd5:

    # emi "Right!"
    emi "¡Cierto!"

# game/script-a3-emi.rpy:1324
translate es a3_emi_invisible_rock_3814f468:

    # emi "Hurry up and stretch out, Hisao!"
    emi "¡Apresúrate y estira, Hisao!"

# game/script-a3-emi.rpy:1329
translate es a3_emi_invisible_rock_22042dd1:

    # emi "We've got to get moving!"
    emi "¡Tenemos que empezar a movernos!"

# game/script-a3-emi.rpy:1340
translate es a3_emi_invisible_rock_15b3fec5:

    # "She takes off like a shot, far quicker than I'm used to."
    "Ella sale disparada, mucho más rápido de lo que estoy acostumbrado."

# game/script-a3-emi.rpy:1348
translate es a3_emi_invisible_rock_4640892e:

    # "Still, I try to keep pace with her, recklessly testing my limits."
    "Aun así, intento mantener su ritmo, probando tercamente mis límites."

# game/script-a3-emi.rpy:1350
translate es a3_emi_invisible_rock_20b2ad9e:

    # "It gives me a feeling of freedom, like my heart is no longer important."
    "Eso me deja una sensación de libertad, como si mi corazón ya no importara más."

# game/script-a3-emi.rpy:1352
translate es a3_emi_invisible_rock_becb3940:

    # "I find myself wanting to laugh, filled with the feeling of moving beyond what I once called my boundaries."
    "Me dan ganas de reír, lleno de la sensación de moverme afuera de lo que una vez llamé mis límites."

# game/script-a3-emi.rpy:1354
translate es a3_emi_invisible_rock_23581080:

    # "The nurse's warnings to not overdo things echo in my mind, and I disregard them."
    "Las advertencias del enfermero de no pasarme de la raya hacen eco en mi cabeza, y yo las ignoro."

# game/script-a3-emi.rpy:1356
translate es a3_emi_invisible_rock_d9542541:

    # "This feeling I have, this willingness to risk a heart attack for something so trivial as a morning run, feels out of character for me."
    "Esta sensación que tengo, esta disposición a arriesgar un ataque cardiaco por algo tan trivial como una carrera matutina, se siente atípico de mí."

# game/script-a3-emi.rpy:1358
translate es a3_emi_invisible_rock_2a012b8c:

    # "But is it?"
    "Pero, ¿lo es?"

# game/script-a3-emi.rpy:1360
translate es a3_emi_invisible_rock_19a19681:

    # "Or rather, should it be?"
    "O más bien, ¿debería de serlo?"

# game/script-a3-emi.rpy:1362
translate es a3_emi_invisible_rock_433dc596:

    # "I've got a weak heart, sure."
    "Tengo un corazón débil, claro."

# game/script-a3-emi.rpy:1364
translate es a3_emi_invisible_rock_27dc76f9:

    # "It'll never be capable of the kind of speed and endurance Emi's capable of."
    "Uno que nunca será capaz de tener la velocidad ni la resistencia que tiene el de Emi."

# game/script-a3-emi.rpy:1366
translate es a3_emi_invisible_rock_08042c25:

    # "Though I probably wouldn't be able to get that good even if I had a healthy heart."
    "Pero probablemente yo no sería capaz de llegar a ser tan bueno aun teniendo un corazón saludable."

# game/script-a3-emi.rpy:1370
translate es a3_emi_invisible_rock_e72ef4d1:

    # "As we round the final bend, I feel my legs screaming in protest, but for the first time, I ignore them."
    "Mientras cruzamos el último recodo, siento mis piernas gritando en protesta, pero, por primera vez, las ignoro."

# game/script-a3-emi.rpy:1372
translate es a3_emi_invisible_rock_87493504:

    # "I accelerate to finish at a sprint, nearly catching up to Emi."
    "Acelero para terminar con un sprint, casi alcanzando a Emi."

# game/script-a3-emi.rpy:1374
translate es a3_emi_invisible_rock_47657e10:

    # "That was never going to happen, of course."
    "Eso nunca iba a pasar, por supuesto."

# game/script-a3-emi.rpy:1376
translate es a3_emi_invisible_rock_09175dd3:

    # "Still, I feel surprisingly good."
    "Aun así, me siento sorprendentemente bien."

# game/script-a3-emi.rpy:1378
translate es a3_emi_invisible_rock_cf646b99:

    # "Oh sure, my legs feel like they're about to catch fire, and I'm having trouble staying upright."
    "Oh, claro, siento como si mis piernas estuvieran a punto de prenderse fuego, y estoy teniendo problemas para mantenerme derecho."

# game/script-a3-emi.rpy:1380
translate es a3_emi_invisible_rock_6ecfa003:

    # "But there's been a shift of some sort today."
    "Pero ha habido una especie de cambio hoy."

# game/script-a3-emi.rpy:1382
translate es a3_emi_invisible_rock_a8cf9d5a:

    # "And it's all thanks to the girl grinning at the finish line, waiting for me."
    "Y es todo gracias a la chica sonriendo en la línea de meta, esperando por mí."

# game/script-a3-emi.rpy:1390
translate es a3_emi_invisible_rock_2ebf6676:

    # hi "That felt a little faster than usual."
    hi "Eso se sintió un poco más rápido de lo habitual."

# game/script-a3-emi.rpy:1392
translate es a3_emi_invisible_rock_ca88186b:

    # "My comment is met with a grin and a shrug."
    "Mi comentario es recibido con una sonrisa y un encogimiento de hombros."

# game/script-a3-emi.rpy:1397
translate es a3_emi_invisible_rock_3ca7e76e:

    # emi "Can't have you think I was going to go soft on you, now can I?"
    emi "No puedo dejar que pienses que voy a ser blanda contigo, ¿no?"

# game/script-a3-emi.rpy:1402
translate es a3_emi_invisible_rock_749c30b1:

    # emi "But you managed to handle it just fine."
    emi "Pero te las arreglaste para superarlo sin problemas."

# game/script-a3-emi.rpy:1404
translate es a3_emi_invisible_rock_15bb78a3:

    # hi "Well, I couldn't have done it without you."
    hi "Bueno, no podría haberlo hecho sin ti."

# game/script-a3-emi.rpy:1409
translate es a3_emi_invisible_rock_dd4b83fd:

    # "Still feeling the high from the run and moved by a surge of gratitude, I seize Emi in a hug."
    "Todavía sintiendo el éxtasis de la carrera y motivado por una oleada de gratitud, atrapo a Emi en un abrazo."

# game/script-a3-emi.rpy:1411
translate es a3_emi_invisible_rock_9d5a9627:

    # hi "Thanks."
    hi "Gracias."

# game/script-a3-emi.rpy:1413
translate es a3_emi_invisible_rock_00989c04:

    # hi "Really, I'm not just saying that."
    hi "En serio, no lo digo por decirlo."

# game/script-a3-emi.rpy:1415
translate es a3_emi_invisible_rock_ac84899e:

    # hi "I'm in your debt."
    hi "Estoy en deuda contigo."

# game/script-a3-emi.rpy:1420
translate es a3_emi_invisible_rock_98b9ac44:

    # "Emi seems flustered by my words, squirming uncomfortably."
    "Emi parece agitada por mis palabras, retorciéndose con incomodidad."

# game/script-a3-emi.rpy:1422
translate es a3_emi_invisible_rock_e2937a71:

    # emi "Don't be silly, Hisao."
    emi "No seas tonto, Hisao."

# game/script-a3-emi.rpy:1427
translate es a3_emi_invisible_rock_a9b8ebe9:

    # emi "Someone had to haul you out here, didn't they?"
    emi "Alguien tenía que arrastrarte hasta acá afuera, ¿verdad?"

# game/script-a3-emi.rpy:1432
translate es a3_emi_invisible_rock_66dc4e43:

    # emi "And it's not like you're not doing anything for me, right?"
    emi "Y no es como si tú no estuvieras haciendo nada por mí, ¿cierto?"

# game/script-a3-emi.rpy:1437
translate es a3_emi_invisible_rock_53649164:

    # emi "I needed a running partner, remember?"
    emi "Necesitaba un compañero para correr, ¿recuerdas?"

# game/script-a3-emi.rpy:1442
translate es a3_emi_invisible_rock_97616939:

    # "I shake my head, still pointedly not letting go of Emi, who stops squirming and merely looks up at me with a quickly deepening blush that almost seems out of character."
    "Sacudo la cabeza, todavía sin ninguna intención de dejar ir a Emi, quien deja de retorcerse y se limita a mirarme desde abajo con un rubor que se intensifica rápidamente y que casi parece impropio de ella."

# game/script-a3-emi.rpy:1444
translate es a3_emi_invisible_rock_afdf5ae8:

    # hi "No, that's not true."
    hi "No, eso no es verdad."

# game/script-a3-emi.rpy:1446
translate es a3_emi_invisible_rock_723e3459:

    # hi "You wanted a running partner, but you didn't need one."
    hi "Querías un compañero para correr, pero no necesitabas uno."

# game/script-a3-emi.rpy:1448
translate es a3_emi_invisible_rock_74aebe84:

    # hi "If I hadn't shown up the day after the festival, you would still run, right?"
    hi "Si no me hubiera presentado el día después del festival, aún seguirías corriendo, ¿cierto?"

# game/script-a3-emi.rpy:1450
translate es a3_emi_invisible_rock_5172d36e:

    # hi "But it doesn't work the other way around."
    hi "Pero no funciona al revés."

# game/script-a3-emi.rpy:1452
translate es a3_emi_invisible_rock_c458705e:

    # hi "I only managed to make it out a few times before the festival."
    hi "Solo conseguí hacerlo unas pocas veces antes del festival."

# game/script-a3-emi.rpy:1454
translate es a3_emi_invisible_rock_38b5c26b:

    # hi "And without you, I probably wouldn't have made it out at all after that."
    hi "Y sin ti, probablemente no habría conseguido hacer nada después de eso."

# game/script-a3-emi.rpy:1459
translate es a3_emi_invisible_rock_adbe0b75:

    # "Emi smiles at me and prods my chest with one finger."
    "Emi me sonríe y me clava un dedo en el pecho."

# game/script-a3-emi.rpy:1464
translate es a3_emi_invisible_rock_d1177a2d:

    # emi "You are pretty lazy, Hisao."
    emi "Eres bastante perezoso, Hisao."

# game/script-a3-emi.rpy:1466
translate es a3_emi_invisible_rock_c43e88f5:

    # hi "Hey, I was giving you a compliment!"
    hi "¡Oye, yo te estaba halagando!"

# game/script-a3-emi.rpy:1471
translate es a3_emi_invisible_rock_e23d3eec:

    # emi "Well… you're welcome, I guess."
    emi "Bueno… de nada, supongo."

# game/script-a3-emi.rpy:1473
translate es a3_emi_invisible_rock_7a8ef15b:

    # hi "I'll pay you back somehow."
    hi "Te lo recompensaré de algún modo."

# game/script-a3-emi.rpy:1478
translate es a3_emi_invisible_rock_37d4c9b5:

    # emi "Oh, uh, well…"
    emi "Oh, uh, bueno…"

# game/script-a3-emi.rpy:1483
translate es a3_emi_invisible_rock_4287f81c:

    # emi "That's not necessary, you know."
    emi "Sabes, eso no es necesario."

# game/script-a3-emi.rpy:1488
translate es a3_emi_invisible_rock_1b13520b:

    # emi "I mean I kinda like you, Hisao."
    emi "Digo, me gustas un poco, Hisao."

# game/script-a3-emi.rpy:1493
translate es a3_emi_invisible_rock_02923efb:

    # emi "And being able to run with you in the mornings isn't exactly a bad deal for me either, so…"
    emi "Y poder correr contigo en las mañanas no es exactamente un mal trato para mí, así que…"

# game/script-a3-emi.rpy:1495
translate es a3_emi_invisible_rock_f5e1391f:

    # "For someone who gets so much praise, she seems unused to gratitude."
    "Para alguien que recibe tantas alabanzas, ella no parece acostumbrada a la gratitud."

# game/script-a3-emi.rpy:1497
translate es a3_emi_invisible_rock_129ca422:

    # "I can't think of anything else to say, so we fall silent."
    "No puedo pensar en nada más que decir, así que nos quedamos en silencio."

# game/script-a3-emi.rpy:1499
translate es a3_emi_invisible_rock_ca539ec6:

    # "I become aware of Emi's breathing, of the dampness of her clothing, and of the scent of her."
    "Me vuelvo consciente de la respiración de Emi, de la humedad de su ropa, y de su olor."

# game/script-a3-emi.rpy:1501
translate es a3_emi_invisible_rock_cf12642b:

    # "Coming off of anyone else, it would stink."
    "Viniendo de alguien más, apestaría."

# game/script-a3-emi.rpy:1503
translate es a3_emi_invisible_rock_613f8473:

    # "Coming off of Emi, it fits her in a way nothing else could."
    "Viniendo de Emi, se ajusta a ella de una forma única."

# game/script-a3-emi.rpy:1505
translate es a3_emi_invisible_rock_e3d3b0f7:

    # "Her skin is cool, slick with sweat, and a breeze causes goosebumps to rise."
    "Su piel está fría, cubierta de sudor, y una brisa le provoca escalofríos."

# game/script-a3-emi.rpy:1510
translate es a3_emi_invisible_rock_a5b4f599:

    # "Almost without thinking about it, I lean down and meet Emi's mouth which has already moved to meet my own."
    "Casi sin pensarlo, me inclino y me encuentro con la boca de Emi que ya se ha movido para encontrarse con la mía."

# game/script-a3-emi.rpy:1512
translate es a3_emi_invisible_rock_a272b8b7:

    # "Her lips are soft, and she hums happily as we kiss, sending vibrations from her mouth to mine."
    "Sus labios son suaves, y ella ronronea felizmente mientras nos besamos, enviando vibraciones de su boca a la mía."

# game/script-a3-emi.rpy:1514
translate es a3_emi_invisible_rock_e658f3ec:

    # "There's a startling rightness to everything about this moment. We fit one another perfectly."
    "Hay una alarmante exactitud en este momento. Encajamos el uno con el otro a la perfección."

# game/script-a3-emi.rpy:1519
translate es a3_emi_invisible_rock_fcb1facf:

    # "The kiss ends, and I finally let my arms drop back to my sides."
    "El beso termina, y finalmente dejo caer mis brazos a mis costados."

# game/script-a3-emi.rpy:1524
translate es a3_emi_invisible_rock_e4dca0cc:

    # "Emi is smiling warmly at me and giggles again."
    "Emi está sonriéndome cálidamente, y suelta una risita de nuevo."

# game/script-a3-emi.rpy:1529
translate es a3_emi_invisible_rock_a3b71fb2:

    # emi "Come on Hisao, we'd better go see the nurse."
    emi "Vamos, Hisao, será mejor que veamos al enfermero."

# game/script-a3-emi.rpy:1533
translate es a3_emi_invisible_rock_e2673aea:

    # "Then it happens."
    "Entonces ocurre."

# game/script-a3-emi.rpy:1543
translate es a3_emi_invisible_rock_5ec5d16a:

    # "As she turns to begin walking, she gives out a tiny yelp and stumbles forward."
    "Cuando ella se da vuelta para caminar, suelta un pequeño quejido y tropieza hacia adelante."

# game/script-a3-emi.rpy:1545
translate es a3_emi_invisible_rock_e1a06dae:

    # hi "Emi!"
    hi "¡Emi!"

# game/script-a3-emi.rpy:1552
translate es a3_emi_invisible_rock_141a2ebf:

    # "I leap to steady her and notice with some concern that she's favoring the same leg as last night."
    "Salto hacia adelante para estabilizarla y noto con algo de preocupación que está favoreciendo a la misma pierna de ayer."

# game/script-a3-emi.rpy:1554
translate es a3_emi_invisible_rock_f9d20be3:

    # hi "Your leg…"
    hi "Tu pierna…"

# game/script-a3-emi.rpy:1559
translate es a3_emi_invisible_rock_27cf0d6b:

    # "Emi seems panicked and pushes away from me."
    "Emi parece ser presa del pánico y me empuja para alejarse."

# game/script-a3-emi.rpy:1561
translate es a3_emi_invisible_rock_86cd46fa:

    # emi "It's fine!"
    emi "¡Está bien!"

# game/script-a3-emi.rpy:1563
translate es a3_emi_invisible_rock_26ed0b8f:

    # "My expression must seem hurt, because she hastens to apologize."
    "Mi expresión debe verse dolida, porque se apresura en disculparse."

# game/script-a3-emi.rpy:1568
translate es a3_emi_invisible_rock_0a9d6829:

    # emi "Sorry! Sorry!"
    emi "¡Perdón! ¡Perdón!"

# game/script-a3-emi.rpy:1570
translate es a3_emi_invisible_rock_b4c1f904:

    # emi "Didn't mean to push you like that!"
    emi "¡No quise empujarte así!"

# game/script-a3-emi.rpy:1575
translate es a3_emi_invisible_rock_02f134f4:

    # emi "I was just…"
    emi "Solo estaba…"

# game/script-a3-emi.rpy:1577
translate es a3_emi_invisible_rock_1a2a9bfc:

    # "She stumbles for something to say."
    "Ella tartamudea buscando algo que decir."

# game/script-a3-emi.rpy:1582
translate es a3_emi_invisible_rock_94b0f674:

    # emi "It's nothing, really."
    emi "No es nada, en serio."

# game/script-a3-emi.rpy:1584
translate es a3_emi_invisible_rock_90398009:

    # hi "Hey, don't worry about it."
    hi "Oye, no te preocupes por eso."

# game/script-a3-emi.rpy:1586
translate es a3_emi_invisible_rock_167b95bc:

    # "She's so flustered, I decide to shrug the whole thing off."
    "Ella está muy agitada, decido hacer de lado todo el asunto."

# game/script-a3-emi.rpy:1588
translate es a3_emi_invisible_rock_b9088bc1:

    # "But there's a cold feeling in the pit of my stomach now that won't go away."
    "Pero ahora hay una sensación gélida en el fondo de mi estómago que no se irá."

# game/script-a3-emi.rpy:1590
translate es a3_emi_invisible_rock_0c973c39:

    # "I tried to step in and help her, and she pushed me away."
    "Intenté acercarme y ayudarla, pero ella me empujó lejos."

# game/script-a3-emi.rpy:1592
translate es a3_emi_invisible_rock_f843ace5:

    # "Smiling, I shove those thoughts to the back of my mind and concentrate on Emi."
    "Sonriendo, entierro esos pensamientos en el fondo de mi mente y me concentro en Emi."

# game/script-a3-emi.rpy:1594
translate es a3_emi_invisible_rock_e2f62d4a:

    # hi "I just don't want you getting hurt, that's all."
    hi "Es solo que no quiero que te lastimes, nada más."

# game/script-a3-emi.rpy:1599
translate es a3_emi_invisible_rock_2ceab32d:

    # emi "You don't have to worry about me, honest."
    emi "No tienes que preocuparte por mí, en serio."

# game/script-a3-emi.rpy:1604
translate es a3_emi_invisible_rock_fd4b5979:

    # emi "I'm fine!"
    emi "¡Estoy bien!"

# game/script-a3-emi.rpy:1606
translate es a3_emi_invisible_rock_fabf8dd1:

    # "Yes, you say that, but I don't believe you."
    "Sí, tú dices eso, pero no te creo."

# game/script-a3-emi.rpy:1609
translate es a3_emi_invisible_rock_1b54cfe8:

    # "Why won't you tell me what's wrong?"
    "¿Por qué no me dices qué anda mal?"

# game/script-a3-emi.rpy:1611
translate es a3_emi_invisible_rock_b248864b:

    # "It's like she gets offended by my trying to help."
    "Es como si la ofendieran mis intentos de ayudarla."

# game/script-a3-emi.rpy:1613
translate es a3_emi_invisible_rock_9024f87e:

    # "What am I supposed to make of that?"
    "¿Qué se supone que haga entonces?"

# game/script-a3-emi.rpy:1615
translate es a3_emi_invisible_rock_bc1049e1:

    # "I keep worrying about you regardless, and not saying anything yesterday just makes me feel guilty about today."
    "A pesar de eso sigo preocupándome por ti, y no haber dicho nada ayer solo me hace sentir culpable por lo de hoy."

# game/script-a3-emi.rpy:1617
translate es a3_emi_invisible_rock_a3c7fe52:

    # "I should have at least asked."
    "Al menos debería haber preguntado."

# game/script-a3-emi.rpy:1619
translate es a3_emi_invisible_rock_fb5249f9:

    # "Would she have reacted the same way last night?"
    "¿Habría reaccionado ella del mismo modo anoche?"

# game/script-a3-emi.rpy:1621
translate es a3_emi_invisible_rock_ba96e530:

    # "Guess I'll never know now."
    "Supongo que ya nunca lo sabré."

# game/script-a3-emi.rpy:1628
translate es a3_emi_invisible_rock_09777475:

    # "I'm still trying to sort out what happened on the track as we arrive in front of the nurse's office."
    "Aún estoy intentando superar lo que ocurrió en la pista cuando llegamos frente a la oficina del enfermero."

# game/script-a3-emi.rpy:1630
translate es a3_emi_invisible_rock_67c339fb:

    # "Emi raises her hand to knock, hesitates and turns to me smiling guiltily."
    "Emi levanta su mano para tocar, vacila y se vuelve hacia mí con una sonrisa de culpabilidad."

# game/script-a3-emi.rpy:1639
translate es a3_emi_invisible_rock_7e11096c:

    # emi "Hey, can you do me a favor?"
    emi "Oye, ¿me puedes hacer un favor?"

# game/script-a3-emi.rpy:1641
translate es a3_emi_invisible_rock_2f6a93ba:

    # hi "Of course."
    hi "Claro."

# game/script-a3-emi.rpy:1646
translate es a3_emi_invisible_rock_8961c540:

    # emi "Can you tell the nurse that I'll see him later?"
    emi "¿Puedes decirle al enfermero que lo veré más tarde?"

# game/script-a3-emi.rpy:1651
translate es a3_emi_invisible_rock_df55753c:

    # emi "I just remembered that I've got some… stuff to take care of before class."
    emi "Acabo de recordar que tengo algunas… cosas de las que encargarme antes de clases."

# game/script-a3-emi.rpy:1656
translate es a3_emi_invisible_rock_b52715c4:

    # emi "So I really need to get moving."
    emi "Así que realmente necesito apresurarme."

# game/script-a3-emi.rpy:1661
translate es a3_emi_invisible_rock_f88d761d:

    # "I peer at her closely, and she fidgets under my stare."
    "La miro fijamente, y ella se inquieta bajo mi mirada."

# game/script-a3-emi.rpy:1663
translate es a3_emi_invisible_rock_8262e3dc:

    # "Yeah, she's clearly just avoiding the nurse."
    "Sí, claramente está evitando al enfermero."

# game/script-a3-emi.rpy:1665
translate es a3_emi_invisible_rock_3632eef3:

    # "That leg of hers…"
    "Esa pierna suya…"

# game/script-a3-emi.rpy:1667
translate es a3_emi_invisible_rock_99936ab0:

    # "Well, whatever. I said I'd help, and so I will."
    "Bueno, como sea. Dije que ayudaría, y así lo haré."

# game/script-a3-emi.rpy:1669
translate es a3_emi_invisible_rock_38357576:

    # "But I'll make damn sure she sees the nurse before the day's out."
    "Pero me aseguraré completamente de que vea al enfermero antes de que termine el día."

# game/script-a3-emi.rpy:1671
translate es a3_emi_invisible_rock_7c52ab3e:

    # hi "Yeah, okay. I'll let him know."
    hi "Sí, de acuerdo. Le haré saber."

# game/script-a3-emi.rpy:1676
translate es a3_emi_invisible_rock_c7b18935:

    # "Emi looks like I've just given her a pony on Christmas."
    "Emi se ve como si yo acabara de darle un poni para Navidad."

# game/script-a3-emi.rpy:1681
translate es a3_emi_invisible_rock_d0352384:

    # emi "Thank you so much!"
    emi "¡Muchísimas gracias!"

# game/script-a3-emi.rpy:1686
translate es a3_emi_invisible_rock_f78e62a9:

    # emi "You're the best, Hisao!"
    emi "¡Eres el mejor, Hisao!"

# game/script-a3-emi.rpy:1691
translate es a3_emi_invisible_rock_474501db:

    # "I am rewarded for my complicity in her lie by a kiss that makes it all worth it, or so I tell myself."
    "Soy recompensado por mi complicidad en su mentira con un beso que hace que todo valga la pena, o eso me digo a mí mismo."

# game/script-a3-emi.rpy:1696
translate es a3_emi_invisible_rock_4908513c:

    # "As Emi heads out of the building, trying hard not to let her limp show, I knock on the door of the office."
    "Mientras Emi sale del edificio, esforzándose por no evidenciar su cojera, toco la puerta de la oficina."

# game/script-a3-emi.rpy:1698
translate es a3_emi_invisible_rock_4b2b8e3e:

    # nk "Ah, Hisao. Come on in."
    nk "Ah, Hisao. Entra."

# game/script-a3-emi.rpy:1706
translate es a3_emi_invisible_rock_06312b56:

    # nk "I don't see Emi with you."
    nk "No veo a Emi contigo."

# game/script-a3-emi.rpy:1711
translate es a3_emi_invisible_rock_b55b4654:

    # nk "She's not sick again, is she?"
    nk "No está enferma de nuevo, ¿o sí?"

# game/script-a3-emi.rpy:1713
translate es a3_emi_invisible_rock_7dad4c7a:

    # "From the tone of his voice, I don't think the nurse is expecting me to say 'Yes, she's ill.'"
    "Por el tono de su voz, no creo que el enfermero esté esperando que yo diga “Sí, lo está”."

# game/script-a3-emi.rpy:1715
translate es a3_emi_invisible_rock_6a5f4a17:

    # hi "Er, she said that she'd forgotten to do something, and so she had to skip out, but she'll see you later today."
    hi "Eh, ella dijo que olvidó hacer algo, entonces tuvo que irse, pero te verá más tarde."

# game/script-a3-emi.rpy:1720
translate es a3_emi_invisible_rock_9960e747:

    # "The nurse heaves an exasperated sigh."
    "El enfermero suelta un suspiro de exasperación."

# game/script-a3-emi.rpy:1722
translate es a3_emi_invisible_rock_1fc9f566:

    # nk "Honestly, that girl…"
    nk "En serio, esa chica…"

# game/script-a3-emi.rpy:1724
translate es a3_emi_invisible_rock_34d1d59d:

    # hi "Hmm?"
    hi "¿Hmm?"

# game/script-a3-emi.rpy:1729
translate es a3_emi_invisible_rock_5688bfcb:

    # nk "She's been avoiding me."
    nk "Me ha estado evitando."

# game/script-a3-emi.rpy:1731
translate es a3_emi_invisible_rock_febad22e:

    # nk "Yesterday she was in and out of here without even taking off her prosthetics. And now this."
    nk "Ayer entró y salió de aquí sin siquiera quitarse sus prótesis. Y ahora esto."

# game/script-a3-emi.rpy:1733
translate es a3_emi_invisible_rock_ba0fe8b2:

    # "Well, at least it's not just me Emi doesn't want worrying."
    "Bueno, al menos no soy el único al que Emi no quiere tener preocupado."

# game/script-a3-emi.rpy:1735
translate es a3_emi_invisible_rock_86721cf1:

    # "That's a… comfort, I guess."
    "Eso es… reconfortante, supongo."

# game/script-a3-emi.rpy:1737
translate es a3_emi_invisible_rock_805ad0bd:

    # "Still, I feel like I should say something about her leg. I said I'd lie for her, but she really needs to see him."
    "Aun así, siento que debería decir algo sobre su pierna. Dije que mentiría por ella, pero realmente necesita ver al enfermero."

# game/script-a3-emi.rpy:1739
translate es a3_emi_invisible_rock_698345c4:

    # hi "Now that you mention it, she was limping pretty badly today."
    hi "Ahora que lo mencionas, hoy estaba cojeando bastante."

# game/script-a3-emi.rpy:1741
translate es a3_emi_invisible_rock_72ffa9e6:

    # hi "And last night as well."
    hi "Y anoche también."

# game/script-a3-emi.rpy:1746
translate es a3_emi_invisible_rock_005cf11b:

    # "The nurse's eyes narrow at the words 'last night.'"
    "Los ojos del enfermero se entrecierran con la palabra “anoche”."

# game/script-a3-emi.rpy:1748
translate es a3_emi_invisible_rock_e0ec98e6:

    # nk "And what exactly were you two doing last night?"
    nk "¿Y exactamente qué estaban haciendo ustedes dos anoche?"

# game/script-a3-emi.rpy:1750
translate es a3_emi_invisible_rock_98249085:

    # hi "We were uh, on a date."
    hi "Estábamos eh, en una cita."

# game/script-a3-emi.rpy:1755
translate es a3_emi_invisible_rock_b9ff7403:

    # "The nurse raises his eyebrows as if surprised."
    "El enfermero alza las cejas como si estuviera sorprendido."

# game/script-a3-emi.rpy:1757
translate es a3_emi_invisible_rock_291e7cdb:

    # nk "Really? Interesting."
    nk "¿En serio? Interesante."

# game/script-a3-emi.rpy:1759
translate es a3_emi_invisible_rock_0f638bea:

    # hi "Huh?"
    hi "¿Huh?"

# game/script-a3-emi.rpy:1764
translate es a3_emi_invisible_rock_084c8476:

    # nk "Oh, nothing."
    nk "Oh, nada."

# game/script-a3-emi.rpy:1766
translate es a3_emi_invisible_rock_670e4e02:

    # "His gaze turns thoughtful, and then he grins at me."
    "Su mirada se torna pensativa, y luego me sonríe."

# game/script-a3-emi.rpy:1771
translate es a3_emi_invisible_rock_fbbefeef:

    # nk "You don't think you could use some of that boyfriend charm to get her to come see me today, could you?"
    nk "No crees que puedes usar algo de ese encanto de novio para hacer que ella venga a verme hoy, ¿o sí?"

# game/script-a3-emi.rpy:1773
translate es a3_emi_invisible_rock_9aa89fc3:

    # hi "Of course!"
    hi "¡Claro!"

# game/script-a3-emi.rpy:1775
translate es a3_emi_invisible_rock_53229ca0:

    # hi "I was planning on doing that anyway."
    hi "Estaba pensando hacer eso de todos modos."

# game/script-a3-emi.rpy:1777
translate es a3_emi_invisible_rock_58436db5:

    # hi "I think she's really hurt and just pretending she isn't."
    hi "Creo que está realmente lastimada y solo pretende no estarlo."

# game/script-a3-emi.rpy:1782
translate es a3_emi_invisible_rock_68110dcc:

    # nk "Hmm, yes. She does that."
    nk "Hmm, sí. Ella hace eso."

# game/script-a3-emi.rpy:1784
translate es a3_emi_invisible_rock_d39a1758:

    # nk "Afraid I'll make her stop running."
    nk "Teme que yo le ponga un alto a las carreras."

# game/script-a3-emi.rpy:1786
translate es a3_emi_invisible_rock_13ff59c8:

    # hi "Will you?"
    hi "¿Lo harías?"

# game/script-a3-emi.rpy:1791
translate es a3_emi_invisible_rock_ee58f0df:

    # nk "I don't like to, but if it's bad enough that she's been limping, well…"
    nk "No me gusta, pero si es lo suficientemente malo como para que ella haya estado cojeando, bueno…"

# game/script-a3-emi.rpy:1793
translate es a3_emi_invisible_rock_d5e70109:

    # nk "I guess I'll have to see what's wrong for myself before I make that call."
    nk "Supongo que tendré que ver cuál es el problema antes de hacer esa llamada."

# game/script-a3-emi.rpy:1795
translate es a3_emi_invisible_rock_e1f8a5e6:

    # hi "I see."
    hi "Ya veo."

# game/script-a3-emi.rpy:1797
translate es a3_emi_invisible_rock_17be382d:

    # "Emi, not allowed to run? Perish the thought."
    "¿Emi sin permiso para correr? Que el cielo nos libre."

# game/script-a3-emi.rpy:1799
translate es a3_emi_invisible_rock_d638283d:

    # "I don't know if she'd be able to function without running."
    "No sé si ella podría seguir funcionando sin correr."

# game/script-a3-emi.rpy:1801
translate es a3_emi_invisible_rock_05703add:

    # "No wonder she's reluctant to admit anything's wrong."
    "Con razón es renuente a admitir que algo anda mal."

# game/script-a3-emi.rpy:1803
translate es a3_emi_invisible_rock_aefacdbb:

    # hi "Well, I'll make sure she sees you."
    hi "Bueno, me aseguraré de que te vea."

# game/script-a3-emi.rpy:1808
translate es a3_emi_invisible_rock_1a83d320:

    # nk "Good. Oh, and before I forget…"
    nk "Bien. Oh, y antes de que lo olvide…"

# game/script-a3-emi.rpy:1813
translate es a3_emi_invisible_rock_11ecfa14:

    # "He grins at me again in what feels like a vaguely threatening manner."
    "Me sonríe de nuevo de una manera que se siente levemente amenazadora."

# game/script-a3-emi.rpy:1815
translate es a3_emi_invisible_rock_adb0c5e7:

    # nk "Don't forget that I know what medications you're on."
    nk "No olvides que sé cuáles medicinas estás tomando."

# game/script-a3-emi.rpy:1820
translate es a3_emi_invisible_rock_73a21d81:

    # nk "You be careful around Emi, got it?"
    nk "Sé cuidadoso cuando estés con Emi, ¿entendido?"

# game/script-a3-emi.rpy:1822
translate es a3_emi_invisible_rock_5dbef0c1:

    # "Wow. He looks serious, too."
    "Guau. También se ve serio."

# game/script-a3-emi.rpy:1824
translate es a3_emi_invisible_rock_641f55dc:

    # hi "Got it."
    hi "Entendido."

# game/script-a3-emi.rpy:1826
translate es a3_emi_invisible_rock_73920ea6:

    # hi "Don't hurt Emi. Wouldn't dream of it."
    hi "No lastimar a Emi. Ni lo soñaría."

# game/script-a3-emi.rpy:1831
translate es a3_emi_invisible_rock_60e22ae0:

    # nk "Grand!"
    nk "¡Grandioso!"

# game/script-a3-emi.rpy:1836
translate es a3_emi_invisible_rock_46925b52:

    # nk "I'd hate for you to be late."
    nk "No me gustaría ver que te pasaras."

# game/script-a3-emi.rpy:1838
translate es a3_emi_invisible_rock_0f638bea_1:

    # hi "Huh?"
    hi "¿Huh?"

# game/script-a3-emi.rpy:1843
translate es a3_emi_invisible_rock_ce04b9eb:

    # nk "Late, as in the late Hisao Nakai."
    nk "Pasarte, en el sentido de “pasarte a mejor vida”."

# game/script-a3-emi.rpy:1848
translate es a3_emi_invisible_rock_9a0c2b05:

    # "He frowns briefly, dissatisfied."
    "Él frunce el ceño por un instante, insatisfecho."

# game/script-a3-emi.rpy:1850
translate es a3_emi_invisible_rock_b3d88ff6:

    # nk "Sounded better in my head…"
    nk "Sonaba mejor en mi cabeza…"

# game/script-a3-emi.rpy:1855
translate es a3_emi_invisible_rock_c1b99db7:

    # nk "Well, at any rate."
    nk "Bueno, en fin."

# game/script-a3-emi.rpy:1860
translate es a3_emi_invisible_rock_4335a79e:

    # nk "Get out of here before you miss your first class!"
    nk "¡Vete de aquí antes de que pierdas tu primera clase!"

# game/script-a3-emi.rpy:1862
translate es a3_emi_invisible_rock_f9605776:

    # nk "You've got things to do, I'm sure. Shoo!"
    nk "Tienes cosas que hacer, estoy seguro. ¡Fuera!"

# game/script-a3-emi.rpy:1866
translate es a3_emi_invisible_rock_1f20723d:

    # "As I leave, I notice the nurse pulling out his phone and dialing a number."
    "Mientras me voy, noto cómo el enfermero saca su teléfono y marca un número."

# game/script-a3-emi.rpy:1871
translate es a3_emi_invisible_rock_6de0fce7:

    # nk "Meiko, your daughter's being a pain in the ass again…"
    nk "Meiko, tu hija me está dando dolores de cabeza otra vez…"

# game/script-a3-emi.rpy:1873
translate es a3_emi_invisible_rock_9e6b0b35:

    # "I'd better head back to my room, or I really will be late."
    "Mejor me dirijo de vuelta a mi habitación, o en serio llegaré tarde."

# game/script-a3-emi.rpy:1875
translate es a3_emi_invisible_rock_2df2d14a:

    # "Hey, wasn't he supposed to check my heart rate?"
    "Oye, ¿no se supone que él tenía que revisar mi ritmo cardiaco?"

# game/script-a3-emi.rpy:1886
translate es a3_emi_lunch_and_science_18c45eb9:

    # "The lunch bell sounds, and I bring myself out of the stupor I slipped into during the morning's classes."
    "Suena la campanada del almuerzo, y salgo del estupor en el cual caí durante las clases de la mañana."

# game/script-a3-emi.rpy:1888
translate es a3_emi_lunch_and_science_220064c0:

    # "My lack of sleep last night, coupled with the increased pace of this morning's run, has left me a little exhausted."
    "La falta de sueño de anoche, sumada al acelerado ritmo de la carrera de esta mañana, me ha dejado algo exhausto."

# game/script-a3-emi.rpy:1896
translate es a3_emi_lunch_and_science_6c38ef20:

    # "Despite that, I find myself skipping stairs up to the roof."
    "A pesar de eso, estoy saltándome los escalones que van hacia la azotea."

# game/script-a3-emi.rpy:1898
translate es a3_emi_lunch_and_science_736a3d00:

    # "There's a thrill of excitement now, in addition to the pleasure one gets from eating lunch with one's friends."
    "Ahora hay un entusiasmante suspenso, además del placer que tienes al comer con tus amigos."

# game/script-a3-emi.rpy:1906
translate es a3_emi_lunch_and_science_aa3fb5ee:

    # "True, both Emi and Rin are still my friends, but Emi has become more than that now."
    "Cierto, Emi y Rin aún son mis amigas, pero Emi se ha vuelto más que eso."

# game/script-a3-emi.rpy:1908
translate es a3_emi_lunch_and_science_9dd35d0c:

    # "Rin is back in her usual spot on the roof, almost as if she'd never been absent."
    "Rin está de vuelta en su lugar habitual en la azotea, casi como si nunca se hubiera ausentado."

# game/script-a3-emi.rpy:1914
translate es a3_emi_lunch_and_science_2de01dbb:

    # hi "Feeling better, I take it?"
    hi "¿Supongo que te sientes mejor?"

# game/script-a3-emi.rpy:1919
translate es a3_emi_lunch_and_science_d5735622:

    # "A raised eyebrow is my reward for speaking."
    "Una ceja levantada es mi recompensa por hablar."

# game/script-a3-emi.rpy:1921
translate es a3_emi_lunch_and_science_c9377f7d:

    # rin "Better than what?"
    rin "¿Mejor que cuándo?"

# game/script-a3-emi.rpy:1925
translate es a3_emi_lunch_and_science_8bf01b97:

    # hi "Er, better than you felt yesterday."
    hi "Eh, mejor de lo que te sentías ayer."

# game/script-a3-emi.rpy:1930
translate es a3_emi_lunch_and_science_2d2f7278:

    # "Rin gives my question some serious thought."
    "Rin le dedica a mi pregunta algo de seria reflexión."

# game/script-a3-emi.rpy:1932
translate es a3_emi_lunch_and_science_0efef7a5:

    # rin "I'm not sure."
    rin "No estoy segura."

# game/script-a3-emi.rpy:1934
translate es a3_emi_lunch_and_science_36b8cd8c:

    # rin "I think I might have felt rather good for some of yesterday, but it's all fuzzy."
    rin "Creo que pude haberme sentido bastante bien durante una parte de ayer, pero todo está borroso."

# game/script-a3-emi.rpy:1936
translate es a3_emi_lunch_and_science_83f88214:

    # hi "Too much cold medicine?"
    hi "¿Demasiada medicina para la gripe?"

# game/script-a3-emi.rpy:1941
translate es a3_emi_lunch_and_science_c0b826db:

    # rin "Well, I was asleep. And that usually is pretty good."
    rin "Bueno, estaba dormida. Y eso por lo general es bueno."

# game/script-a3-emi.rpy:1946
translate es a3_emi_lunch_and_science_a5ec6de1:

    # rin "But I can't remember what it feels like to be asleep, because I'm not conscious for it."
    rin "Pero no puedo recordar cómo se siente estar dormida, porque no soy consciente de ello."

# game/script-a3-emi.rpy:1948
translate es a3_emi_lunch_and_science_8130d836:

    # rin "It's a real problem."
    rin "Es un verdadero problema."

# game/script-a3-emi.rpy:1953
translate es a3_emi_lunch_and_science_853bfa3f:

    # rin "Then again, if I knew how good it felt I might not sleep any more."
    rin "Por otra parte, si supiera qué tan bien se sintió podría no volver a dormir más."

# game/script-a3-emi.rpy:1955
translate es a3_emi_lunch_and_science_74ee5dfe:

    # rin "But this way I keep trying so I guess that's how I can keep from being overtired."
    rin "Pero de esta forma sigo intentándolo, entonces supongo que así es como evito estar demasiado cansada."

# game/script-a3-emi.rpy:1957
translate es a3_emi_lunch_and_science_a7fe4f70:

    # hi "An eternal mystery to keep you sleeping at night?"
    hi "¿Un misterio eterno para mantenerte durmiendo por las noches?"

# game/script-a3-emi.rpy:1962
translate es a3_emi_lunch_and_science_f69117cf:

    # rin "Maybe mystery's the wrong word. Intangibility might be the proper way to describe it."
    rin "Tal vez misterio es la palabra incorrecta. Intangibilidad podría ser la manera adecuada para describirlo."

# game/script-a3-emi.rpy:1964
translate es a3_emi_lunch_and_science_e1f8a5e6:

    # hi "I see."
    hi "Ya veo."

# game/script-a3-emi.rpy:1966
translate es a3_emi_lunch_and_science_6a0622ec:

    # "No, I don't see at all. I have no idea what she's talking about, but that's okay, since I rarely do."
    "No, no lo hago, en absoluto. No tengo ni idea de lo que está hablando, pero está bien, dado que rara vez lo hago."

# game/script-a3-emi.rpy:1971
translate es a3_emi_lunch_and_science_0a5c1d17:

    # rin "Do you remember what sleeping feels like?"
    rin "¿Tú recuerdas cómo se siente dormir?"

# game/script-a3-emi.rpy:1973
translate es a3_emi_lunch_and_science_c068b37f:

    # rin "Like yesterday, do you remember what you felt like sleeping yesterday?"
    rin "Ayer por ejemplo, ¿recuerdas cómo te sentías ayer al dormir?"

# game/script-a3-emi.rpy:1975
translate es a3_emi_lunch_and_science_5e4e10fc:

    # hi "Well, I actually didn't get a lot of sleep yesterday."
    hi "Bueno, en realidad ayer no pude dormir mucho."

# game/script-a3-emi.rpy:1980
translate es a3_emi_lunch_and_science_81383cfa:

    # rin "Hmm."
    rin "Hmm."

# game/script-a3-emi.rpy:1982
translate es a3_emi_lunch_and_science_f63e2621:

    # rin "Maybe that's because you remember subconsciously."
    rin "Tal vez es porque recuerdas inconscientemente."

# game/script-a3-emi.rpy:1984
translate es a3_emi_lunch_and_science_99a91115:

    # hi "Actually, I think I was worrying about Emi."
    hi "De hecho, creo que estaba preocupado por Emi."

# game/script-a3-emi.rpy:1989
translate es a3_emi_lunch_and_science_cb5bb5f5:

    # rin "Doesn't Emi worry enough about herself?"
    rin "¿Acaso Emi no se preocupa lo suficiente por sí misma?"

# game/script-a3-emi.rpy:1991
translate es a3_emi_lunch_and_science_3deea67e:

    # "I hadn't considered that, but it gives me pause."
    "No había considerado eso, pero me detiene por un momento."

# game/script-a3-emi.rpy:1993
translate es a3_emi_lunch_and_science_2c15b782:

    # hi "True, but would she ask for help if she needed it?"
    hi "Cierto, pero si ella necesitara ayuda, ¿la pediría?"

# game/script-a3-emi.rpy:1998
translate es a3_emi_lunch_and_science_e6b98c91:

    # "Rin frowns, and I raise an eyebrow. Will I get a proper answer?"
    "Rin frunce el ceño, y yo levanto una ceja. ¿Recibiré una respuesta apropiada?"

# game/script-a3-emi.rpy:2000
translate es a3_emi_lunch_and_science_ce9b6a8a:

    # rin "Probably not. Is there something she should be asking for help with?"
    rin "Probablemente no. ¿Hay algo por lo que ella debería estar pidiendo ayuda?"

# game/script-a3-emi.rpy:2002
translate es a3_emi_lunch_and_science_66624620:

    # hi "Her leg, for starters."
    hi "Su pierna, para empezar."

# game/script-a3-emi.rpy:2004
translate es a3_emi_lunch_and_science_be49126a:

    # "This seems to catch Rin's interest."
    "Esto parece captar el interés de Rin."

# game/script-a3-emi.rpy:2009
translate es a3_emi_lunch_and_science_2bf9a425:

    # rin "Leg?"
    rin "¿Pierna?"

# game/script-a3-emi.rpy:2011
translate es a3_emi_lunch_and_science_71c4e75f:

    # hi "It's hurt, but she won't see the nurse about it."
    hi "Está herida, pero no verá al enfermero por eso."

# game/script-a3-emi.rpy:2013
translate es a3_emi_lunch_and_science_e68abdb5:

    # "Rin shakes her head in disapproval."
    "Rin sacude la cabeza en desaprobación."

# game/script-a3-emi.rpy:2018
translate es a3_emi_lunch_and_science_bc6b1019:

    # rin "You have to make her."
    rin "Debes hacer que vaya."

# game/script-a3-emi.rpy:2023
translate es a3_emi_lunch_and_science_9144013e:

    # rin "Like she makes me go to class. For her own good."
    rin "Como ella me hace ir a clases. Por su propio bien."

# game/script-a3-emi.rpy:2025
translate es a3_emi_lunch_and_science_540af557:

    # rin "Otherwise she could lose her legs again, and that's just too weird."
    rin "O podría perder sus piernas de nuevo, y eso es demasiado raro."

# game/script-a3-emi.rpy:2027
translate es a3_emi_lunch_and_science_4407fc97:

    # rin "Losing things twice."
    rin "Perder cosas dos veces."

# game/script-a3-emi.rpy:2032
translate es a3_emi_lunch_and_science_b923619f:

    # rin "Especially if you don't find them again to begin with."
    rin "Especialmente si no las encuentras antes, para empezar."

# game/script-a3-emi.rpy:2034
translate es a3_emi_lunch_and_science_2562663d:

    # rin "Unless prosthetics are the same as finding something."
    rin "A menos de que las prótesis sean lo mismo que encontrar algo."

# game/script-a3-emi.rpy:2039
translate es a3_emi_lunch_and_science_b9772af5:

    # rin "But that's a different kind of lost, isn't it?"
    rin "Pero esa es una pérdida diferente, ¿verdad?"

# game/script-a3-emi.rpy:2041
translate es a3_emi_lunch_and_science_d16776d9:

    # hi "I think so."
    hi "Creo."

# game/script-a3-emi.rpy:2046
translate es a3_emi_lunch_and_science_ad6ae9c2:

    # rin "Hmm. I wonder…"
    rin "Hmm. Me pregunto…"

# game/script-a3-emi.rpy:2053
translate es a3_emi_lunch_and_science_eadfb775:

    # emi "Wonder what?"
    emi "¿Qué cosa?"

# game/script-a3-emi.rpy:2059
translate es a3_emi_lunch_and_science_c93faa9d:

    # "Emi seems to have snuck up on Rin and me, though Rin doesn't seem especially surprised. Which is itself unsurprising, I suppose."
    "Emi parece haberse escabullido hasta nosotros, aunque Rin no parece especialmente sorprendida. Lo que en sí mismo es poco sorprendente, supongo."

# game/script-a3-emi.rpy:2075
translate es a3_emi_lunch_and_science_1b4134ec:

    # "Rin manages to sit herself upright quite expertly, throwing her upper body forward and using her momentum to right herself."
    "Rin se las arregla para ponerse de pie con bastante pericia, empujando la parte superior del cuerpo hacia adelante y utilizando el impulso para enderezarse."

# game/script-a3-emi.rpy:2080
translate es a3_emi_lunch_and_science_36aa4b24:

    # hi "Your leg. How's it feel?"
    hi "Tu pierna. ¿Cómo está?"

# game/script-a3-emi.rpy:2086
translate es a3_emi_lunch_and_science_f642633c:

    # "That earns me a frown and a bit of a glare."
    "Eso me gana un ceño fruncido y una breve mirada feroz."

# game/script-a3-emi.rpy:2088
translate es a3_emi_lunch_and_science_35aa46d7:

    # emi "It's okay, I think."
    emi "Está bien, creo."

# game/script-a3-emi.rpy:2093
translate es a3_emi_lunch_and_science_53f4a770:

    # emi "Not worth worrying about."
    emi "No vale la pena preocuparse por eso."

# game/script-a3-emi.rpy:2098
translate es a3_emi_lunch_and_science_353ecd77:

    # hi "Tell that to the nurse."
    hi "Díselo al enfermero."

# game/script-a3-emi.rpy:2100
translate es a3_emi_lunch_and_science_20c95936:

    # hi "He's quite insistent that you visit him, you know."
    hi "Insiste en que lo visites, sabes."

# game/script-a3-emi.rpy:2106
translate es a3_emi_lunch_and_science_72abd5b7:

    # "Emi pouts like I've just told her she's been grounded."
    "Emi hace pucheros como si yo acabara de decirle que está castigada."

# game/script-a3-emi.rpy:2108
translate es a3_emi_lunch_and_science_051b59b0:

    # emi "He worries too much."
    emi "Él se preocupa demasiado."

# game/script-a3-emi.rpy:2113
translate es a3_emi_lunch_and_science_53761694:

    # emi "It's not a big deal, just a little soreness."
    emi "No es gran cosa, solo un poco de inflamación."

# game/script-a3-emi.rpy:2115
translate es a3_emi_lunch_and_science_77ac14a3:

    # "I try to resist rolling my eyes in exasperation."
    "Intento resistir poner los ojos en blanco por la exasperación."

# game/script-a3-emi.rpy:2120
translate es a3_emi_lunch_and_science_0f0f40ee:

    # hi "If it's nothing, then you should have no problem seeing him, right?"
    hi "Si no es nada, entonces no deberías tener ningún problema en ir a verlo, ¿cierto?"

# game/script-a3-emi.rpy:2126
translate es a3_emi_lunch_and_science_4b76edec:

    # "Emi narrows her eyes suspiciously."
    "Emi entrecierra los ojos con sospecha."

# game/script-a3-emi.rpy:2128
translate es a3_emi_lunch_and_science_be029d42:

    # emi "Did he put you up to this?"
    emi "¿Él te metió en esto?"

# game/script-a3-emi.rpy:2133
translate es a3_emi_lunch_and_science_dc3737ab:

    # hi "Well, maybe. A little."
    hi "Bueno, tal vez. Un poco."

# game/script-a3-emi.rpy:2135
translate es a3_emi_lunch_and_science_a866f927:

    # hi "But that's not the point. I would have reminded you to see him anyway."
    hi "Pero ese no es el punto. Te habría recordado que fueras a verlo de todos modos."

# game/script-a3-emi.rpy:2137
translate es a3_emi_lunch_and_science_3424a3dc:

    # hi "It would be terrible to see you really hurt and not doing anything about it."
    hi "Sería horrible ver cómo estando realmente herida no haces nada al respecto."

# game/script-a3-emi.rpy:2139
translate es a3_emi_lunch_and_science_3bc9404f:

    # hi "That would make it worse, and I don't really want to see you hurt, you know?"
    hi "Eso lo haría peor, y realmente no quiero verte herida, ¿sabes?"

# game/script-a3-emi.rpy:2141
translate es a3_emi_lunch_and_science_f1074a87:

    # hi "Call me crazy, but I kinda would prefer to see you happy and healthy."
    hi "Llámame loco, pero creo que prefiero verte sana y feliz."

# game/script-a3-emi.rpy:2147
translate es a3_emi_lunch_and_science_978d9142:

    # "With each statement, Emi's frown fades a little more, until eventually she's grinning, albeit a little shyly."
    "Con cada línea, el ceño de Emi se desvanece un poco, hasta que eventualmente está sonriendo, aunque con algo de timidez."

# game/script-a3-emi.rpy:2151
translate es a3_emi_lunch_and_science_51deaf76:

    # emi "Well, if you're going to put it that way, then I guess I'll have to see him."
    emi "Bueno, si lo vas a poner así, entonces supongo que tendré que verlo."

# game/script-a3-emi.rpy:2156
translate es a3_emi_lunch_and_science_7d35482f:

    # emi "Otherwise you'll keep worrying, and then I'll never hear the end of it, right?"
    emi "O continuarás preocupándote, y entonces será un cuento de nunca acabar, ¿no?"

# game/script-a3-emi.rpy:2161
translate es a3_emi_lunch_and_science_371aacb0:

    # hi "That's right. I'll keep bugging you about it, and that might put a damper on our dates."
    hi "Correcto. Seguiré insistiendo, y eso podría arruinar nuestras citas."

# game/script-a3-emi.rpy:2163
translate es a3_emi_lunch_and_science_9a0a32fa:

    # hi "'How's the food, Hisao?' 'Talk to the nurse, Emi.'"
    hi "“¿Cómo está la comida, Hisao?” “Habla con el enfermero, Emi”."

# game/script-a3-emi.rpy:2165
translate es a3_emi_lunch_and_science_51810e90:

    # hi "'How was your day, Hisao?' 'Talk to the nurse, Emi.'"
    hi "“¿Cómo estuvo tu día, Hisao?” “Habla con el enfermero, Emi”."

# game/script-a3-emi.rpy:2167
translate es a3_emi_lunch_and_science_ae00dc1c:

    # hi "'Hisao, I think I'm ready to go all the w—' '{b}Talk to the nurse, Emi.{/b}'"
    hi "“Hisao, creo que estoy lista para que m—” “{b}Habla con el enfermero, Emi{/b}”."

# game/script-a3-emi.rpy:2169
translate es a3_emi_lunch_and_science_0b653a27:

    # hi "See? It doesn't work that well."
    hi "¿Ves? No funciona muy bien."

# game/script-a3-emi.rpy:2175
translate es a3_emi_lunch_and_science_28bb042c:

    # "Emi giggles at my high-pitched rendition of her own voice and gives me an affectionate shove."
    "Emi se ríe infantilmente con mi chillona interpretación de su propia voz y me da un empujón cariñoso."

# game/script-a3-emi.rpy:2180
translate es a3_emi_lunch_and_science_45de7127:

    # emi "My voice isn't that high, jerk."
    emi "Mi voz no es tan aguda, idiota."

# game/script-a3-emi.rpy:2186
translate es a3_emi_lunch_and_science_ced5db4b:

    # rin "I thought it was pretty accurate."
    rin "Yo creo que fue bastante acertada."

# game/script-a3-emi.rpy:2190
translate es a3_emi_lunch_and_science_4647c070:

    # "Emi and I stare at Rin for a while before I burst into laughter."
    "Emi y yo observamos a Rin por unos instantes antes de echarme a reír."

# game/script-a3-emi.rpy:2196
translate es a3_emi_lunch_and_science_62cae0a9:

    # "Emi crosses her arms and huffs, mock-offended."
    "Emi cruza sus brazos y resopla, fingiendo estar ofendida."

# game/script-a3-emi.rpy:2201
translate es a3_emi_lunch_and_science_fbc12dc6:

    # emi "You're both jerks."
    emi "Los dos son unos idiotas."

# game/script-a3-emi.rpy:2206
translate es a3_emi_lunch_and_science_8dbef685:

    # hi "Such vile calumnies from you, young woman."
    hi "Tales calumnias viles provenientes de ti, joven dama."

# game/script-a3-emi.rpy:2208
translate es a3_emi_lunch_and_science_e1d8ff1e:

    # hi "I'm stunned that you would call me, of all people, a jerk."
    hi "Estoy sorprendido de que me llames, entre todas las personas, un idiota."

# game/script-a3-emi.rpy:2210
translate es a3_emi_lunch_and_science_ce320337:

    # hi "Honestly, I just… I don't know what to think."
    hi "Honestamente, simplemente… no sé qué pensar."

# game/script-a3-emi.rpy:2216
translate es a3_emi_lunch_and_science_ec3f0841:

    # "Emi sticks her tongue out at me."
    "Emi me saca la lengua."

# game/script-a3-emi.rpy:2218
translate es a3_emi_lunch_and_science_ef3c74ef:

    # emi "You ass."
    emi "Imbécil."

# game/script-a3-emi.rpy:2223
translate es a3_emi_lunch_and_science_79d22f7a:

    # emi "So Rin, how's the art club these days?"
    emi "Entonces, Rin, ¿qué tal el club de arte estos días?"

# game/script-a3-emi.rpy:2228
translate es a3_emi_lunch_and_science_715072b3:

    # "Rin, seemingly as surprised by this sudden change of topic as I am, takes a minute to think before answering."
    "Rin, viéndose tan sorprendida como yo por este repentino cambio de tema, se toma un minuto para pensar antes de responder."

# game/script-a3-emi.rpy:2233
translate es a3_emi_lunch_and_science_80474c21:

    # rin "I believe it is okay."
    rin "Creo que está bien."

# game/script-a3-emi.rpy:2238
translate es a3_emi_lunch_and_science_95d1778d:

    # rin "Although Nomiya keeps telling me to work harder."
    rin "Aunque Nomiya sigue diciéndome que trabaje más duro."

# game/script-a3-emi.rpy:2243
translate es a3_emi_lunch_and_science_3521bde4:

    # rin "But I don't think he understands my methods."
    rin "Pero no creo que entienda mis métodos."

# game/script-a3-emi.rpy:2248
translate es a3_emi_lunch_and_science_5b0327fe:

    # emi "He always struck me as slightly creepy."
    emi "Él siempre me ha parecido algo escalofriante."

# game/script-a3-emi.rpy:2253
translate es a3_emi_lunch_and_science_03c9cfbd:

    # "Rin ponders this statement for a while."
    "Rin sopesa esta declaración por un instante."

# game/script-a3-emi.rpy:2258
translate es a3_emi_lunch_and_science_31b4a7e7:

    # rin "I've never really noticed."
    rin "Nunca lo noté."

# game/script-a3-emi.rpy:2263
translate es a3_emi_lunch_and_science_6a83d423:

    # rin "But I don't pay much attention to him most days, so maybe that's why."
    rin "Pero no le presto mucha atención la mayor parte de los días, así que tal vez sea eso."

# game/script-a3-emi.rpy:2265
translate es a3_emi_lunch_and_science_0b167f69:

    # hi "How often do you meet?"
    hi "¿Qué tan seguido se encuentran?"

# game/script-a3-emi.rpy:2270
translate es a3_emi_lunch_and_science_35d43b8d:

    # emi "Thinking of joining, Hisao?"
    emi "¿Piensas unirte, Hisao?"

# game/script-a3-emi.rpy:2275
translate es a3_emi_lunch_and_science_77010f7a:

    # hi "What? Nah, I've already decided to join a club."
    hi "¿Qué? Nah, ya he decidido unirme a un club."

# game/script-a3-emi.rpy:2281
translate es a3_emi_lunch_and_science_301af26b:

    # emi "Really? Which one?"
    emi "¿En serio? ¿Cuál?"

# game/script-a3-emi.rpy:2286
translate es a3_emi_lunch_and_science_45fdb502:

    # hi "Well, it's not really much of a club, to be honest…"
    hi "Bueno, no es realmente un club, para ser honesto…"

# game/script-a3-emi.rpy:2292
translate es a3_emi_lunch_and_science_71981731:

    # emi "Oh, you joined the tea club?"
    emi "Oh, ¿te uniste al club del té?"

# game/script-a3-emi.rpy:2297
translate es a3_emi_lunch_and_science_c77824a7:

    # hi "No, I uh… joined the science club… I think."
    hi "No, yo, eh… me uní al club de ciencia… creo."

# game/script-a3-emi.rpy:2303
translate es a3_emi_lunch_and_science_af05e59e:

    # "Emi looks highly confused."
    "Emi se ve muy confundida."

# game/script-a3-emi.rpy:2305
translate es a3_emi_lunch_and_science_6d26b033:

    # emi "We have a science club?"
    emi "¿Tenemos un club de ciencia?"

# game/script-a3-emi.rpy:2310
translate es a3_emi_lunch_and_science_64500638:

    # hi "Er, not really. It's just me."
    hi "Eh, no realmente. Solo soy yo."

# game/script-a3-emi.rpy:2316
translate es a3_emi_lunch_and_science_1c01093a:

    # emi "Hisao, that's not a club. That's sitting in your room reading books."
    emi "Hisao, eso no es un club. Eso es sentarte en tu habitación a leer libros."

# game/script-a3-emi.rpy:2318
translate es a3_emi_lunch_and_science_82665fdb:

    # hi "No, I mean it's just me and Mutou."
    hi "No, quiero decir, solo somos Mutou y yo."

# game/script-a3-emi.rpy:2320
translate es a3_emi_lunch_and_science_2f09f5db:

    # hi "I'm just the only student so far."
    hi "Soy el único estudiante hasta este momento."

# game/script-a3-emi.rpy:2326
translate es a3_emi_lunch_and_science_2c0454a0:

    # emi "Mutou? Really?"
    emi "¿Mutou? ¿En serio?"

# game/script-a3-emi.rpy:2328
translate es a3_emi_lunch_and_science_45d22bf3:

    # "A thought strikes her."
    "La asalta un pensamiento."

# game/script-a3-emi.rpy:2333
translate es a3_emi_lunch_and_science_65fca8b8:

    # emi "Oh, is that what you were talking about yesterday? Your meeting with Mutou?"
    emi "Oh, ¿de eso estabas hablando ayer? ¿Tu reunión con Mutou?"

# game/script-a3-emi.rpy:2335
translate es a3_emi_lunch_and_science_9c40978e:

    # hi "Yeah, that was our first meeting, I guess."
    hi "Sí, esa fue nuestra primera reunión, supongo."

# game/script-a3-emi.rpy:2340
translate es a3_emi_lunch_and_science_7af30aaa:

    # "Emi giggles."
    "Emi suelta una risita."

# game/script-a3-emi.rpy:2345
translate es a3_emi_lunch_and_science_0e5d260e:

    # emi "Nerd."
    emi "Nerd."

# game/script-a3-emi.rpy:2347
translate es a3_emi_lunch_and_science_516a31fb:

    # hi "Hey, I can't help being clever."
    hi "Oye, no puedo evitar ser listo."

# game/script-a3-emi.rpy:2352
translate es a3_emi_lunch_and_science_59e4a769:

    # emi "You know, I could have used your help years ago."
    emi "Sabes, podría haber usado tu ayuda hace unos años."

# game/script-a3-emi.rpy:2354
translate es a3_emi_lunch_and_science_0ee33054:

    # emi "You should've had your heart attack earlier in life, Hisao."
    emi "Deberías de haber sufrido tu ataque al corazón más joven, Hisao."

# game/script-a3-emi.rpy:2356
translate es a3_emi_lunch_and_science_92ea2b3f:

    # "I laugh, and then realize this is probably one of the very rare times I've laughed about my heart attack."
    "Me río, y luego me doy cuenta de que es probablemente una de las muy raras ocasiones en las que me he reído de mi ataque cardiaco."

# game/script-a3-emi.rpy:2358
translate es a3_emi_lunch_and_science_c9a5e7a4:

    # hi "Hindsight…"
    hi "En retrospectiva…"

# game/script-a3-emi.rpy:2363
translate es a3_emi_lunch_and_science_c711e092:

    # emi "Yeah…"
    emi "Sí…"

# game/script-a3-emi.rpy:2367
translate es a3_emi_lunch_and_science_81fb92f7:

    # "The ringing of the bell ends our conversation."
    "El sonido de la campana finaliza nuestra conversación."

# game/script-a3-emi.rpy:2369
translate es a3_emi_lunch_and_science_ca4fce7f:

    # hi "Hmm, guess we'd better go."
    hi "Hmm, creo que mejor nos vamos."

# game/script-a3-emi.rpy:2374
translate es a3_emi_lunch_and_science_b88352b2:

    # emi "Yeah, I guess so."
    emi "Sí, supongo que sí."

# game/script-a3-emi.rpy:2381
translate es a3_emi_lunch_and_science_01bf50ff:

    # emi "Come on Rin, you too."
    emi "Vamos, Rin, tú también."

# game/script-a3-emi.rpy:2386
translate es a3_emi_lunch_and_science_b30e972c:

    # "Rin has apparently begun to doze off, so Emi gives her a sharp bump."
    "Al parecer Rin ha empezado a dormitar, así que Emi le da un sonoro golpe."

# game/script-a3-emi.rpy:2391
translate es a3_emi_lunch_and_science_48e45611:

    # rin "I almost had it."
    rin "Casi lo tenía."

# game/script-a3-emi.rpy:2396
translate es a3_emi_lunch_and_science_07446fe4:

    # emi "Sorry, but you need to go to class."
    emi "Perdona, pero tienes que ir a clases."

# game/script-a3-emi.rpy:2401
translate es a3_emi_lunch_and_science_fb4ae925:

    # rin "I disagree, but maybe if I nap in class I'll get it this time."
    rin "No opino lo mismo, pero tal vez si duermo en clases lo conseguiré."

# game/script-a3-emi.rpy:2406
translate es a3_emi_lunch_and_science_05949d42:

    # rin "Changing location is sometimes helpful for that kind of thing."
    rin "Cambiar de lugar a veces es útil para esa clase de cosas."

# game/script-a3-emi.rpy:2408
translate es a3_emi_lunch_and_science_1d0b6747:

    # "Neither Emi or I bother asking what 'it' is."
    "Ni Emi ni yo nos preocupamos en preguntar qué cosas son."

# game/script-a3-emi.rpy:2415
translate es a3_emi_lunch_and_science_23a739de:

    # "As we arrive at my classroom, Emi gives me a quick kiss and heads down the hallway, Rin in tow."
    "Cuando llegamos a mi aula, Emi me da un beso rápido y se va por el pasillo, Rin remolcada detrás."

# game/script-a3-emi.rpy:2430
translate es a3_emi_lunch_and_science_cb2eadf0:

    # "I turn to enter the classroom, to be met by the duo of Shizune and Misha."
    "Me vuelvo para entrar al aula, y me encuentro con el dúo de Shizune y Misha."

# game/script-a3-emi.rpy:2437
translate es a3_emi_lunch_and_science_ef2b123c:

    # shi "…"
    shi "…"

# game/script-a3-emi.rpy:2439
translate es a3_emi_lunch_and_science_c0ed9402:

    # "Misha seems to be fighting a losing battle to keep from breaking into a fit of giggles while she translates Shizune's latest rant."
    "Misha parece estar luchando una batalla perdida para evitar romper en un ataque de risa mientras traduce el último arranque de Shizune."

# game/script-a3-emi.rpy:2444
translate es a3_emi_lunch_and_science_73443101:

    # mi "While we are pleased, nay thrilled, to see how well you've managed to make new friends and forge relationships - and with such a cutie too, Hicchan~…"
    mi "Aun si estamos encantadas, y hasta emocionadas, de ver lo bien que te las has arreglado para hacer nuevos amigos y forjar relaciones —y con semejante lindura también, Hicchan~…"

# game/script-a3-emi.rpy:2446
translate es a3_emi_lunch_and_science_d1accfeb:

    # "I think that last part was probably Misha."
    "Creo que la última parte fue probablemente Misha."

# game/script-a3-emi.rpy:2451
translate es a3_emi_lunch_and_science_ef2b123c_1:

    # shi "…"
    shi "…"

# game/script-a3-emi.rpy:2456
translate es a3_emi_lunch_and_science_079c0b5d:

    # mi "We nevertheless feel compelled to politely remind you that public displays of affection are strictly forbidden - really? That's disappointing, Shicchan - by section eight of the code of conduct laid out in the student handbook."
    mi "No obstante, nos sentimos obligadas a recordarte amablemente que las muestras públicas de afecto están estrictamente prohibidas — ¿en serio? Eso es decepcionante, Shicchan —  por la sección ocho del código de conducta establecido en el manual del estudiante"

# game/script-a3-emi.rpy:2461
translate es a3_emi_lunch_and_science_ef2b123c_2:

    # shi "…"
    shi "…"

# game/script-a3-emi.rpy:2466
translate es a3_emi_lunch_and_science_5c024ff3:

    # mi "In this case, however, ignorance of the law may be your excuse, as we are feeling lenient…"
    mi "En este caso, sin embargo, la ignorancia de la ley puede ser tu excusa, ya que nos sentimos clementes…"

# game/script-a3-emi.rpy:2471
translate es a3_emi_lunch_and_science_ef2b123c_3:

    # shi "…"
    shi "…"

# game/script-a3-emi.rpy:2476
translate es a3_emi_lunch_and_science_d2f8011b:

    # mi "…and the paperwork required to punish the both of you would only add to the already mountainous volume of work which confronts us, the sole members of the Student Council - and besides, you two are adorable together~!"
    mi "… Y el papeleo requerido para castigarlos a ambos solo se sumaría al ya colosal volúmen de trabajo que nos confronta, a los únicos miembros del consejo estudiantil, además, ¡ustedes dos son adorables juntos~!"

# game/script-a3-emi.rpy:2481
translate es a3_emi_lunch_and_science_ef2b123c_4:

    # shi "…"
    shi "…"

# game/script-a3-emi.rpy:2486
translate es a3_emi_lunch_and_science_9b7ea9ad:

    # mi "Therefore consider this a formal warning, and please refrain from such displays in the future. At least when Shizune can see you, Hicchan~!"
    mi "Por consiguiente considera esto una advertencia formal, y por favor abstente de tales exhibiciones en el futuro. ¡Por lo menos cuando Shizune pueda verte, Hicchan~!"

# game/script-a3-emi.rpy:2488
translate es a3_emi_lunch_and_science_b0463726:

    # "This whole spiel is so patently ridiculous that I can't help but reply in the same pompous manner."
    "Todo este asunto es tan patentemente ridículo que no puedo evitar responder de la misma manera pomposa."

# game/script-a3-emi.rpy:2490
translate es a3_emi_lunch_and_science_01c0e5d0:

    # hi "Well, I for one feel enlightened."
    hi "Bueno, yo, por mi parte, me siento ilustrado."

# game/script-a3-emi.rpy:2492
translate es a3_emi_lunch_and_science_33f4e90f:

    # hi "I apologize profusely for my rash actions and will strive to contain my baser impulses which, I fear, impel me toward such inappropriate displays of public affection."
    hi "Me disculpo profusamente por mis imprudentes acciones y me esforzaré en contener mis instintivos impulsos que, me temo, me empujan hacia tan inapropiadas exhibiciones públicas de afección."

# game/script-a3-emi.rpy:2494
translate es a3_emi_lunch_and_science_152bada1:

    # hi "It is hardly my wish to burden an already overworked Student Council with such petty matters, and will do my best to make your lives easier in this matter in the future."
    hi "Ni por asomo se me ocurriría agobiar a un consejo estudiantil que ya está sobrecargado de trabajo con tan insignificantes asuntos, y haré todo lo posible para facilitar sus vidas en este tema de ahora en adelante."

# game/script-a3-emi.rpy:2496
translate es a3_emi_lunch_and_science_75512ce3:

    # hi "At least, when Shizune's watching."
    hi "Al menos cuando Shizune esté mirando."

# game/script-a3-emi.rpy:2498
translate es a3_emi_lunch_and_science_a60ddf0f:

    # "This last line is delivered with a wink to Misha, who finally loses control of her laughter."
    "Esta última línea la acompaño con un guiño a Misha, quien finalmente pierde el control de su risa."

# game/script-a3-emi.rpy:2503
translate es a3_emi_lunch_and_science_e39fcd2d:

    # mi "Wahaha~!"
    mi "¡Guajaja~!"

# game/script-a3-emi.rpy:2508
translate es a3_emi_lunch_and_science_3978c3f8:

    # mi "Well said, Hicchan~!"
    mi "¡Bien dicho, Hicchan~!"

# game/script-a3-emi.rpy:2510
translate es a3_emi_lunch_and_science_e1dab0c0:

    # "Chuckling a little myself, we enter the classroom."
    "Mientras me río entre dientes, entramos al aula."

# game/script-a3-emi.rpy:2516
translate es a3_emi_lunch_and_science_e831e126:

    # "Class is uneventful, and after the final bell rings, I find myself alone with Mutou again."
    "La clase transcurre sin pormenores, y luego de que suena la campanada final, me encuentro de nuevo a solas con Mutou."

# game/script-a3-emi.rpy:2521
translate es a3_emi_lunch_and_science_291fa12c:

    # mu "So, it looks like we've all assembled for the second meeting of the Science Club."
    mu "Entonces, parece que estamos listos para la segunda reunión del club de ciencia."

# game/script-a3-emi.rpy:2527
translate es a3_emi_lunch_and_science_072cd73e:

    # mu "Or is it the first? What do you think, should we count yesterday as a meeting?"
    mu "¿O es la primera? Qué piensas, ¿deberíamos de contar lo de ayer como una reunión?"

# game/script-a3-emi.rpy:2529
translate es a3_emi_lunch_and_science_11579085:

    # hi "Well, we did form the club yesterday, didn't we?"
    hi "Bueno, formamos el club ayer, ¿no es verdad?"

# game/script-a3-emi.rpy:2531
translate es a3_emi_lunch_and_science_1f820112:

    # hi "That seems like club business, so we can safely call yesterday a meeting."
    hi "Eso parece asunto del club, entonces sin duda podemos llamarla una reunión."

# game/script-a3-emi.rpy:2536
translate es a3_emi_lunch_and_science_a4296f0b:

    # "Mutou smiles in his usual stilted and awkward way. I wonder if the muscles in his face are just not shaped correctly to let him smile naturally."
    "Mutou sonríe de forma torpe y rebuscada, como es típico de él. Me pregunto si los músculos de su cara no tienen la forma correcta como para dejarlo sonreír con naturalidad."

# game/script-a3-emi.rpy:2538
translate es a3_emi_lunch_and_science_a69070b4:

    # mu "You really do have a knack for this, I think. Logical thought processes, that is."
    mu "Creo que realmente tienes un don para esto. Procesos de pensamiento lógico, eso es."

# game/script-a3-emi.rpy:2540
translate es a3_emi_lunch_and_science_38fa48f6:

    # hi "I guess so?"
    hi "¿Supongo?"

# game/script-a3-emi.rpy:2545
translate es a3_emi_lunch_and_science_557cbd21:

    # mu "A scientist speaks with authority, Hisao. The answer here is 'Yes, I do.'"
    mu "Un científico responde con autoridad, Hisao. La respuesta aquí es “Sí, lo hago”."

# game/script-a3-emi.rpy:2547
translate es a3_emi_lunch_and_science_d40f499f:

    # mu "When the world wants to know how it works, we tell it. Even if all we've got is a decent hypothesis."
    mu "Cuando el mundo quiere saber cómo funciona algo, les decimos. Incluso si todo lo que tenemos es una hipótesis decente."

# game/script-a3-emi.rpy:2552
translate es a3_emi_lunch_and_science_4a07af6a:

    # mu "But we must sound certain anyway, because we're the authorities on the subject, right?"
    mu "Pero aun así debemos sonar seguros, porque somos las autoridades en el tema, ¿cierto?"

# game/script-a3-emi.rpy:2554
translate es a3_emi_lunch_and_science_5e089a2c:

    # "He chuckles, to go along with his awkward smile at his awkward joke. I'm doing my best not to grimace, but I don't think I'm being too successful."
    "Él se ríe entre dientes, acompañando a su incómodo chiste con su incómoda sonrisa. Hago todo lo posible por no hacer alguna mueca, pero creo que no estoy teniendo mucho éxito."

# game/script-a3-emi.rpy:2559
translate es a3_emi_lunch_and_science_0d74b4e3:

    # mu "That's entirely false, of course."
    mu "Eso es completamente falso, por supuesto."

# game/script-a3-emi.rpy:2561
translate es a3_emi_lunch_and_science_f1eb0b77:

    # mu "We know a lot, sure, but nobody's an expert on how the world works, if only because nobody can be sure. With no certainty, there are no experts."
    mu "Sabemos mucho, claro, pero nadie es un experto en cómo funciona el mundo, aunque solo sea porque nadie puede estar seguro. Sin certeza, no hay expertos."

# game/script-a3-emi.rpy:2563
translate es a3_emi_lunch_and_science_7b49fe21:

    # mu "But we like to pretend, sometimes."
    mu "Pero nos gusta pretender, a veces."

# game/script-a3-emi.rpy:2565
translate es a3_emi_lunch_and_science_2035d2c3:

    # hi "There's some things we can be certain of, right?"
    hi "Hay algunas cosas de las que podemos estar seguros, ¿cierto?"

# game/script-a3-emi.rpy:2567
translate es a3_emi_lunch_and_science_738ce96d:

    # mu "Yes… but no."
    mu "Sí… pero no."

# game/script-a3-emi.rpy:2569
translate es a3_emi_lunch_and_science_da989aa2:

    # mu "We know gravity's there, for example."
    mu "Sabemos que la gravedad está ahí, por ejemplo."

# game/script-a3-emi.rpy:2571
translate es a3_emi_lunch_and_science_9fb46c72:

    # "To illustrate, Mutou picks up a pencil and drops it."
    "Para ilustrar, Mutou levanta un lápiz y lo suelta."

# game/script-a3-emi.rpy:2573
translate es a3_emi_lunch_and_science_39deda6c:

    # mu "See? Still there. But it's good to check every once in a while."
    mu "¿Ves? Aún está ahí. Pero es bueno revisar de vez en cuando."

# game/script-a3-emi.rpy:2575
translate es a3_emi_lunch_and_science_18e27b07:

    # mu "That's why you'll still see researchers mucking about with gravity."
    mu "Es por eso que todavía verás investigadores curioseando con la gravedad."

# game/script-a3-emi.rpy:2580
translate es a3_emi_lunch_and_science_6b768a7a:

    # mu "We're pretty sure we know how it works, but there's always a chance that something isn't how we think it is."
    mu "Estamos bastante seguros de cómo funciona, pero siempre existe la posibilidad de que algo no sea como creemos que es."

# game/script-a3-emi.rpy:2582
translate es a3_emi_lunch_and_science_bc2adbbc:

    # mu "So you check, and check, and check. That's science in a nutshell, Hisao."
    mu "Así que revisamos, y revisamos, y revisamos. Eso es ciencia en pocas palabras, Hisao."

# game/script-a3-emi.rpy:2584
translate es a3_emi_lunch_and_science_511b3722:

    # "The whole time I've listened feeling rather spellbound. Mutou seems to really be passionate about this stuff… I think. It's hard to tell, sometimes."
    "Todo este tiempo he estado escuchando con cierto embelesamiento. Mutou parece estar realmente apasionado con estas cosas… creo. A veces es difícil decirlo."

# game/script-a3-emi.rpy:2586
translate es a3_emi_lunch_and_science_74ea04e9:

    # "How the world works…"
    "Cómo funciona el mundo…"

# game/script-a3-emi.rpy:2588
translate es a3_emi_lunch_and_science_65fabe47:

    # "How humans work."
    "Cómo funcionan los humanos."

# game/script-a3-emi.rpy:2590
translate es a3_emi_lunch_and_science_d98c7895:

    # "How the universe works."
    "Cómo funciona el universo."

# game/script-a3-emi.rpy:2592
translate es a3_emi_lunch_and_science_a3ad946a:

    # "All these questions to be answered."
    "Todas estas preguntas sin respuesta aún."

# game/script-a3-emi.rpy:2594
translate es a3_emi_lunch_and_science_3252cd69:

    # "And, depending on what I go into, maybe I could even figure out a way to fix my heart. That said, I don't think that's a real priority for me."
    "Y, dependiendo de lo que decida, quizá hasta podría encontrar una manera de arreglar mi corazón. Con esto dicho, no creo que esa sea una verdadera prioridad para mí."

# game/script-a3-emi.rpy:2596
translate es a3_emi_lunch_and_science_29ecac11:

    # "Besides, as we start discussing the book he gave me yesterday, I find myself more and more interested in that than my heart condition."
    "Además, cuando empezamos a discutir el libro que me dio ayer, me encuentro más y más interesado en eso que en mi condición cardiaca."

# game/script-a3-emi.rpy:2601
translate es a3_emi_lunch_and_science_7d7b286b:

    # "Before we even realize it, an hour's gone by."
    "Antes de darnos cuenta, ha pasado una hora."

# game/script-a3-emi.rpy:2603
translate es a3_emi_lunch_and_science_5c890fc6:

    # mu "Well, let's call this meeting over for now, okay?"
    mu "Bueno, terminaremos esta reunión por ahora, ¿de acuerdo?"

# game/script-a3-emi.rpy:2605
translate es a3_emi_lunch_and_science_0e87bc90:

    # mu "We'll have another meeting… tomorrow, or uh… the day after."
    mu "Tendremos otra reunión… mañana, o eh… pasado mañana."

# game/script-a3-emi.rpy:2607
translate es a3_emi_lunch_and_science_8529385d:

    # "He considers this for a moment."
    "Considera esto por un momento."

# game/script-a3-emi.rpy:2609
translate es a3_emi_lunch_and_science_46d767c2:

    # mu "Call it the day after. I've got a lot of grading to do."
    mu "Mejor pasado mañana. Tengo que poner un montón de notas."

# game/script-a3-emi.rpy:2611
translate es a3_emi_lunch_and_science_18f6c519:

    # hi "Okay. See you then."
    hi "De acuerdo. Te veré entonces."

# game/script-a3-emi.rpy:2618
translate es a3_emi_lunch_and_science_7fb92804:

    # "As I exit the classroom, I realize that I don't really have anything to do tonight."
    "Mientras salgo del aula, me doy cuenta de que en realidad no tengo nada que hacer esta noche."

# game/script-a3-emi.rpy:2620
translate es a3_emi_lunch_and_science_afdb6e99:

    # "Emi and I didn't make plans, so…"
    "Emi y yo no tenemos planes, así que…"

# game/script-a3-emi.rpy:2622
translate es a3_emi_lunch_and_science_1af65376:

    # "I guess I'll go to the library. It beats doing homework in my room, anyway."
    "Supongo que iré a la biblioteca. La verdad, es mejor que hacer la tarea en mi habitación."

# game/script-a3-emi.rpy:2635
translate es a3_emi_up_down_and_up_again_5f961ec5:

    # "The library always seems cooler than the rest of the building."
    "La biblioteca siempre parece más fresca que el resto del edificio."

# game/script-a3-emi.rpy:2637
translate es a3_emi_up_down_and_up_again_55ce66bb:

    # "Probably to keep the books from getting damaged by excessive heat and humidity."
    "Probablemente para evitar que los libros se dañen por exceso de humedad y calor."

# game/script-a3-emi.rpy:2639
translate es a3_emi_up_down_and_up_again_43c7ec40:

    # "Books are sturdy objects, but if you want to keep them in good condition it takes a little effort."
    "Los libros son objetos robustos, pero requiere un pequeño esfuerzo mantenerlos en buen estado."

# game/script-a3-emi.rpy:2641
translate es a3_emi_up_down_and_up_again_de27fee6:

    # "I've got several books that are so well-worn the pages are barely clinging to the spine."
    "Tengo varios libros que están tan gastados que las páginas apenas se sostienen del lomo."

# game/script-a3-emi.rpy:2643
translate es a3_emi_up_down_and_up_again_e98ac8bd:

    # "It seems impossible for them to still be usable, but if you handle them with care…"
    "Parece imposible que todavía sean utilizables, pero si los manejas con cuidado…"

# game/script-a3-emi.rpy:2645
translate es a3_emi_up_down_and_up_again_1a935ea8:

    # "I make my way to the main desk, where I spot Yuuko busying herself with something or another."
    "Camino hacia el escritorio principal, donde veo a Yuuko ocupada con esto y aquello."

# game/script-a3-emi.rpy:2650
translate es a3_emi_up_down_and_up_again_626a644c:

    # "She smiles at me as I enter and waves."
    "Me sonríe cuando entro y saluda con la mano."

# game/script-a3-emi.rpy:2655
translate es a3_emi_up_down_and_up_again_56bd504b:

    # yu "Hello, Hisao."
    yu "Hola, Hisao."

# game/script-a3-emi.rpy:2660
translate es a3_emi_up_down_and_up_again_060f7c77:

    # yu "Good to see you again! What are you looking for this time?"
    yu "¡Es bueno verte de nuevo! ¿Qué estás buscando esta vez?"

# game/script-a3-emi.rpy:2662
translate es a3_emi_up_down_and_up_again_f444bec9:

    # hi "Nothing in particular, I guess. I just didn't really feel like going back to my room, is all."
    hi "Nada en particular, supongo. Simplemente no quería regresar a mi habitación, es todo."

# game/script-a3-emi.rpy:2667
translate es a3_emi_up_down_and_up_again_2b4678bf:

    # "Yuuko nods."
    "Yuuko asiente."

# game/script-a3-emi.rpy:2672
translate es a3_emi_up_down_and_up_again_cee22333:

    # yu "Well, if you're unoccupied, maybe you could help me look for something?"
    yu "Bueno, si estás desocupado, ¿tal vez podrías ayudarme a buscar algo?"

# game/script-a3-emi.rpy:2674
translate es a3_emi_up_down_and_up_again_44dac545:

    # hi "Sure, what do you need?"
    hi "Claro, ¿qué necesitas?"

# game/script-a3-emi.rpy:2681
translate es a3_emi_up_down_and_up_again_80cb4d04:

    # "Yuuko brings a finger to her lips and looks around furtively."
    "Yuuko se lleva un dedo a los labios y mira alrededor furtivamente."

# game/script-a3-emi.rpy:2683
translate es a3_emi_up_down_and_up_again_50c87fed:

    # "She seems to be looking for eavesdroppers."
    "Parece estar buscando espías."

# game/script-a3-emi.rpy:2685
translate es a3_emi_up_down_and_up_again_321f3d99:

    # yu "Come closer."
    yu "Acércate."

# game/script-a3-emi.rpy:2690
translate es a3_emi_up_down_and_up_again_e4f6dd11:

    # "I take a few hesitant steps forward while feeling distinctly unnerved."
    "Doy unos pasos inseguros hacia adelante sintiéndome distintivamente nervioso."

# game/script-a3-emi.rpy:2692
translate es a3_emi_up_down_and_up_again_596eb3dd:

    # "Yuuko lowers her voice to a confidential whisper."
    "Yuuko baja su voz hasta un susurro confidencial."

# game/script-a3-emi.rpy:2697
translate es a3_emi_up_down_and_up_again_5b382376:

    # yu "I'm on the trail of the Yamaku Cat Burglar."
    yu "Estoy siguiéndole la pista al Gato Ladrón de Yamaku."

# game/script-a3-emi.rpy:2701
translate es a3_emi_up_down_and_up_again_a7d6fb56:

    # hi "The what?"
    hi "¿Al qué?"

# game/script-a3-emi.rpy:2706
translate es a3_emi_up_down_and_up_again_4485dacd:

    # yu "Shh! The walls have ears, Hisao!"
    yu "¡Shh! ¡Las paredes oyen, Hisao!"

# game/script-a3-emi.rpy:2708
translate es a3_emi_up_down_and_up_again_82e2f54b:

    # yu "Or they might."
    yu "O podrían hacerlo."

# game/script-a3-emi.rpy:2713
translate es a3_emi_up_down_and_up_again_04017c4c:

    # yu "But listen! Those missing books, remember them?"
    yu "¡Pero escucha! Esos libros perdidos, ¿los recuerdas?"

# game/script-a3-emi.rpy:2715
translate es a3_emi_up_down_and_up_again_5f71bc52:

    # hi "Er, yeah?"
    hi "Eh, ¿sí?"

# game/script-a3-emi.rpy:2720
translate es a3_emi_up_down_and_up_again_480cf535:

    # yu "Well, they weren't missing! They were stolen!"
    yu "Bueno, ¡no se perdieron! ¡Se los robaron!"

# game/script-a3-emi.rpy:2722
translate es a3_emi_up_down_and_up_again_0472c156:

    # yu "I'm convinced of it!"
    yu "¡Estoy convencida de ello!"

# game/script-a3-emi.rpy:2724
translate es a3_emi_up_down_and_up_again_023b44dc:

    # hi "I remember you saying something of the sort earlier, but how do you know?"
    hi "Recuerdo que decías algo por el estilo antes, ¿pero cómo lo sabes?"

# game/script-a3-emi.rpy:2726
translate es a3_emi_up_down_and_up_again_3b291e19:

    # "Yuuko leans in closer and, if possible, whispers even lower."
    "Yuuko se acerca más y, si es posible, susurra más bajo."

# game/script-a3-emi.rpy:2731
translate es a3_emi_up_down_and_up_again_9be36150:

    # yu "Because I found one of his hiding places!"
    yu "¡Porque encontré uno de sus escondites!"

# game/script-a3-emi.rpy:2733
translate es a3_emi_up_down_and_up_again_167efa4b:

    # hi "You did what?"
    hi "¿Que hiciste qué?"

# game/script-a3-emi.rpy:2735
translate es a3_emi_up_down_and_up_again_9bd867eb:

    # "Yuuko looks triumphant."
    "Yuuko se ve triunfante."

# game/script-a3-emi.rpy:2740
translate es a3_emi_up_down_and_up_again_d9418d9c:

    # yu "Found one of his stashes! It was under one of the stairwells in the boy's dorm!"
    yu "¡Encontré uno de sus escondites! ¡Estaba bajo uno de los escalones en el dormitorio de los chicos!"

# game/script-a3-emi.rpy:2742
translate es a3_emi_up_down_and_up_again_a153032d:

    # yu "Three books I'd been looking for, all there!"
    yu "Tres libros que había estado buscando, ¡todos allí!"

# game/script-a3-emi.rpy:2747
translate es a3_emi_up_down_and_up_again_7fd0012a:

    # yu "I'd suspected a thief before, but this proves it!"
    yu "Sospechaba de un ladrón antes, ¡pero esto lo prueba!"

# game/script-a3-emi.rpy:2749
translate es a3_emi_up_down_and_up_again_2c4f463e:

    # hi "So did you take back the books?"
    hi "¿Entonces tomaste los libros?"

# game/script-a3-emi.rpy:2754
translate es a3_emi_up_down_and_up_again_b8489dd9:

    # "Yuuko looks as if I've just suggested she walk around naked."
    "Yuuko se ve como si yo acabara de sugerirle que camine desnuda."

# game/script-a3-emi.rpy:2756
translate es a3_emi_up_down_and_up_again_6241567a:

    # yu "Are you nuts?"
    yu "¿Estás loco?"

# game/script-a3-emi.rpy:2761
translate es a3_emi_up_down_and_up_again_4125826c:

    # yu "He can't know I'm on to him! He might go to ground and evade capture!"
    yu "¡No puede saber que estoy tras él! ¡Podría escabullirse y evadir la captura!"

# game/script-a3-emi.rpy:2763
translate es a3_emi_up_down_and_up_again_1e332c0c:

    # hi "Uh… huh. So what do you need my help with, then?"
    hi "A… já. Entonces, ¿para qué necesitas mi ayuda?"

# game/script-a3-emi.rpy:2765
translate es a3_emi_up_down_and_up_again_dc57b6a1:

    # "Yuuko casts another glance around the library and leans in closer."
    "Yuuko lanza otra mirada hacia la biblioteca y se aproxima más."

# game/script-a3-emi.rpy:2770
translate es a3_emi_up_down_and_up_again_95a25071:

    # yu "You've got to spy for me."
    yu "Tienes que espiar por mí."

# game/script-a3-emi.rpy:2772
translate es a3_emi_up_down_and_up_again_349104a9:

    # hi "Spy?"
    hi "¿Espiar?"

# game/script-a3-emi.rpy:2774
translate es a3_emi_up_down_and_up_again_948e16ab:

    # yu "Yeah, like when you're in the dorms, you know."
    yu "Sí, cuando estés en los dormitorios, ya sabes."

# game/script-a3-emi.rpy:2779
translate es a3_emi_up_down_and_up_again_4c481194:

    # yu "Keep an eye out for suspicious activity."
    yu "Vigila cualquier actividad sospechosa."

# game/script-a3-emi.rpy:2781
translate es a3_emi_up_down_and_up_again_3b99d883:

    # "What constitutes suspicious, anyway?"
    "De todas formas, ¿qué constituye “sospechosa”?"

# game/script-a3-emi.rpy:2783
translate es a3_emi_up_down_and_up_again_19e51202:

    # "I mean Kenji's a pretty suspicious dude, but I'll wager he barely goes to class, much less sneaks into the library to pilfer books."
    "Quiero decir, Kenji es un tipo bastante sospechoso, pero apostaría a que apenas asiste a clases, mucho menos entraría a hurtadillas a la biblioteca para robar libros."

# game/script-a3-emi.rpy:2785
translate es a3_emi_up_down_and_up_again_4057f6e5:

    # "Still, what's the harm in saying yes? At the least it'll get me out of this weird conversation."
    "Aun así, ¿qué daño haría decir que sí? Al menos me sacaría de esta extraña conversación."

# game/script-a3-emi.rpy:2787
translate es a3_emi_up_down_and_up_again_44cab39a:

    # hi "Yeah, I can do that. No problem."
    hi "Seguro, puedo hacer eso. Sin problema."

# game/script-a3-emi.rpy:2792
translate es a3_emi_up_down_and_up_again_b6c633ae:

    # "Yuuko straightens up and claps excitedly."
    "Yuuko se endereza y aplaude felizmente."

# game/script-a3-emi.rpy:2794
translate es a3_emi_up_down_and_up_again_6fe05fee:

    # yu "Great!"
    yu "¡Genial!"

# game/script-a3-emi.rpy:2799
translate es a3_emi_up_down_and_up_again_fb2d785a:

    # yu "Now, hurry up and talk about something else in case someone comes in!"
    yu "Ahora, rápido, ¡habla de alguna otra cosa en caso de que alguien entre!"

# game/script-a3-emi.rpy:2806
translate es a3_emi_up_down_and_up_again_caf16bd4:

    # yu "How's the school treating you?"
    yu "¿Cómo te ha estado tratando la escuela?"

# game/script-a3-emi.rpy:2808
translate es a3_emi_up_down_and_up_again_2fe6c1c4:

    # hi "Er, pretty well, actually."
    hi "Eh, bastante bien, en realidad."

# game/script-a3-emi.rpy:2810
translate es a3_emi_up_down_and_up_again_6d19ae65:

    # hi "I've been running in the mornings with—"
    hi "He estado corriendo por las mañanas con—"

# game/script-a3-emi.rpy:2815
translate es a3_emi_up_down_and_up_again_449125d7:

    # yu "Emi Ibarazaki, right?"
    yu "Emi Ibarazaki, ¿correcto?"

# game/script-a3-emi.rpy:2819
translate es a3_emi_up_down_and_up_again_2d3cf25d:

    # hi "Uh, yeah."
    hi "Uh, sí."

# game/script-a3-emi.rpy:2821
translate es a3_emi_up_down_and_up_again_da2bccd9:

    # hi "How'd you know?"
    hi "¿Cómo sabes?"

# game/script-a3-emi.rpy:2826
translate es a3_emi_up_down_and_up_again_e7d8e8e7:

    # yu "I served you two in the teahouse, remember?"
    yu "Los atendí en la casa de té, ¿recuerdas?"

# game/script-a3-emi.rpy:2831
translate es a3_emi_up_down_and_up_again_08db766c:

    # yu "I deduced that if you were going to run with anyone, it would probably be her."
    yu "Deduje que si ibas a correr con alguien, sería probablemente con ella."

# game/script-a3-emi.rpy:2833
translate es a3_emi_up_down_and_up_again_f6ab9b8c:

    # "She looks pleased with herself."
    "Se ve complacida consigo misma."

# game/script-a3-emi.rpy:2835
translate es a3_emi_up_down_and_up_again_05823cbc:

    # hi "Impressive."
    hi "Impresionante."

# game/script-a3-emi.rpy:2837
translate es a3_emi_up_down_and_up_again_7371112f:

    # hi "Anyway, yes. We've been running in the mornings."
    hi "De todas formas, sí. Hemos estado corriendo por las mañanas."

# game/script-a3-emi.rpy:2839
translate es a3_emi_up_down_and_up_again_f9f7e4ff:

    # hi "And uh, we kinda started dating."
    hi "Y eh, en algún momento empezamos a salir."

# game/script-a3-emi.rpy:2844
translate es a3_emi_up_down_and_up_again_3b22f75a:

    # "Yuuko claps her hands together excitedly."
    "Yuuko aplaude con emoción."

# game/script-a3-emi.rpy:2846
translate es a3_emi_up_down_and_up_again_723ae3be:

    # yu "Really? That's great!"
    yu "¿En serio? ¡Eso es grandioso!"

# game/script-a3-emi.rpy:2848
translate es a3_emi_up_down_and_up_again_b8a7502d:

    # yu "I'll bet you two are great together!"
    yu "¡Apuesto a que ustedes dos se ven genial juntos!"

# game/script-a3-emi.rpy:2853
translate es a3_emi_up_down_and_up_again_f657a023:

    # yu "I love seeing people find one another like that, you know?"
    yu "Me encanta ver cómo las personas se encuentras las unas a las otras así, ¿sabes?"

# game/script-a3-emi.rpy:2855
translate es a3_emi_up_down_and_up_again_4fcbfa89:

    # yu "I even thought to myself when you walked into the Shanghai that one time, 'I wonder if that kid will wind up with one of those girls.'"
    yu "Incluso aquella vez cuando entraste en el Shanghái pensé, “me pregunto si ese chico terminará con alguna de esas chicas”."

# game/script-a3-emi.rpy:2857
translate es a3_emi_up_down_and_up_again_9456f068:

    # hi "…Really?"
    hi "… ¿En serio?"

# game/script-a3-emi.rpy:2859
translate es a3_emi_up_down_and_up_again_fb2b7134:

    # "Yuuko doesn't seem to notice my somewhat weirded out tone and nods affirmatively."
    "Yuuko no parece notar mi tono de extrañeza y asiente afirmativamente."

# game/script-a3-emi.rpy:2864
translate es a3_emi_up_down_and_up_again_00ac9931:

    # yu "Yup! I could tell that you'd wind up with one of them, you know."
    yu "¡Síp! Estaba segura de que acabarías con una de ellas, sabes."

# game/script-a3-emi.rpy:2869
translate es a3_emi_up_down_and_up_again_fabbb612:

    # yu "I've got an eye for that sort of thing."
    yu "Tengo buen ojo para este tipo de cosas."

# game/script-a3-emi.rpy:2874
translate es a3_emi_up_down_and_up_again_fc547a64:

    # yu "Of course…"
    yu "Claro…"

# game/script-a3-emi.rpy:2876
translate es a3_emi_up_down_and_up_again_39e41517:

    # "Her expression droops slightly."
    "Su expresión decae ligeramente."

# game/script-a3-emi.rpy:2878
translate es a3_emi_up_down_and_up_again_5715a674:

    # yu "I'm not so good at it myself."
    yu "No soy tan buena cuando se trata de mí misma."

# game/script-a3-emi.rpy:2880
translate es a3_emi_up_down_and_up_again_54264c5d:

    # hi "Aw, I'm sure that's not true."
    hi "Oh, estoy seguro de que eso no es cierto."

# game/script-a3-emi.rpy:2885
translate es a3_emi_up_down_and_up_again_127e7dfd:

    # yu "Oh, it's true."
    yu "Sí, es cierto."

# game/script-a3-emi.rpy:2887
translate es a3_emi_up_down_and_up_again_89eaedb3:

    # yu "I met this guy once…"
    yu "Una vez conocí a un chico…"

# game/script-a3-emi.rpy:2892
translate es a3_emi_up_down_and_up_again_db47b7ec:

    # yu "We got along really great, but it turned out he was younger than me."
    yu "Nos llevábamos realmente bien, pero resulta que era más joven que yo."

# game/script-a3-emi.rpy:2897
translate es a3_emi_up_down_and_up_again_15fb4b26:

    # yu "And that was kinda weird, but not terribly so."
    yu "Y eso era un poco raro, pero no terriblemente raro."

# game/script-a3-emi.rpy:2899
translate es a3_emi_up_down_and_up_again_7b0067a0:

    # yu "What was really weird was that he disappeared one day, and I've not seen him since then."
    yu "Lo más raro es que un día desapareció, y no lo he visto desde entonces."

# game/script-a3-emi.rpy:2901
translate es a3_emi_up_down_and_up_again_2f18048b:

    # hi "Huh. That does seem kind of odd."
    hi "Oh. Eso sí parece raro."

# game/script-a3-emi.rpy:2906
translate es a3_emi_up_down_and_up_again_7773c4ef:

    # yu "Doesn't it?"
    yu "¿Verdad?"

# game/script-a3-emi.rpy:2911
translate es a3_emi_up_down_and_up_again_553af8a3:

    # yu "I hope it wasn't something I did…"
    yu "Espero que no haya sido algo que yo hice…"

# game/script-a3-emi.rpy:2913
translate es a3_emi_up_down_and_up_again_7c0b7cb1:

    # "I feel compelled to reassure her."
    "Me siento obligado a tranquilizarla."

# game/script-a3-emi.rpy:2915
translate es a3_emi_up_down_and_up_again_3c90741a:

    # hi "I'm sure it wasn't."
    hi "Estoy seguro de que no lo fue."

# game/script-a3-emi.rpy:2924
translate es a3_emi_up_down_and_up_again_c3c50419:

    # "I intend to try and calm her down further, but the both of us jump in surprise at the ringing suddenly coming from my pocket."
    "Mi intención es calmarla un poco más, pero ambos saltamos de sorpresa por el repentino zumbido proveniente de mi bolsillo."

# game/script-a3-emi.rpy:2929
translate es a3_emi_up_down_and_up_again_225cae1f:

    # "Yuuko sighs to steady herself as I pull the phone from my pocket. I feel a little sheepish for indirectly causing the incident."
    "Yuuko suspira para calmarse mientras yo saco el teléfono de mi bolsillo. Me siento un poco avergonzado por haber causado el suceso indirectamente."

# game/script-a3-emi.rpy:2939
translate es a3_emi_up_down_and_up_again_58c69509:

    # hi "Emi? What's up?"
    hi "¿Emi? ¿Qué pasa?"

# game/script-a3-emi.rpy:2941
translate es a3_emi_up_down_and_up_again_a08228e1:

    # emi "Oh thank God I haven't called your phone before so I didn't know if this number would work or whether you would pick up and I can't—"
    emi "Oh, gracias a Dios, no había llamado a tu número antes así que no sabía si funcionaría o si no contestarías y no podía—"

# game/script-a3-emi.rpy:2946
translate es a3_emi_up_down_and_up_again_354ab1d0:

    # hi "Woah there Emi, slow down."
    hi "Guau, Emi, tranquilízate."

# game/script-a3-emi.rpy:2948
translate es a3_emi_up_down_and_up_again_2331cad8:

    # hi "What's wrong?"
    hi "¿Qué sucede?"

# game/script-a3-emi.rpy:2950
translate es a3_emi_up_down_and_up_again_ab5d124e:

    # "There's a pause on the other side of the line, during which I can hear Emi trying to control her breathing in order to calm down."
    "Hay una pausa al otro lado de la línea, durante la cual puedo escuchar a Emi tratando de controlar su respiración para calmarse."

# game/script-a3-emi.rpy:2952
translate es a3_emi_up_down_and_up_again_6355dcf2:

    # "Something's got her terribly agitated, and it's starting to agitate me."
    "Algo la ha agitado terriblemente, y está empezando a agitarme a mí."

# game/script-a3-emi.rpy:2954
translate es a3_emi_up_down_and_up_again_9282edca:

    # emi "Can you just…"
    emi "Podrías solo…"

# game/script-a3-emi.rpy:2956
translate es a3_emi_up_down_and_up_again_1c75e1c7:

    # emi "Can you stop by?"
    emi "¿Podrías pasarte por acá?"

# game/script-a3-emi.rpy:2958
translate es a3_emi_up_down_and_up_again_54cadd34:

    # emi "Like, now? Or shortly after now?"
    emi "Algo así como, ¿ya? ¿O en un ratito?"

# game/script-a3-emi.rpy:2960
translate es a3_emi_up_down_and_up_again_74556757:

    # emi "I really, really need to talk to you."
    emi "En serio, en serio necesito hablar contigo."

# game/script-a3-emi.rpy:2962
translate es a3_emi_up_down_and_up_again_5f247174:

    # "There's a tone of pleading in the last sentence that I don't think I've ever heard from her."
    "Hay un tono de súplica en su última oración que no creo haber escuchado nunca en ella."

# game/script-a3-emi.rpy:2964
translate es a3_emi_up_down_and_up_again_e6a9e9f2:

    # hi "Of course, I'll be right there."
    hi "Claro, estaré allí enseguida."

# game/script-a3-emi.rpy:2966
translate es a3_emi_up_down_and_up_again_d8776315:

    # hi "Hold steady, okay?"
    hi "Quédate tranquila, ¿de acuerdo?"

# game/script-a3-emi.rpy:2968
translate es a3_emi_up_down_and_up_again_5fc7ae54:

    # "In my increasingly agitated state I've apparently started saying things that don't quite make sense."
    "En mi creciente estado de agitación aparentemente he empezado a decir cosas que no tienen mucho sentido."

# game/script-a3-emi.rpy:2970
translate es a3_emi_up_down_and_up_again_26a89abd:

    # emi "Okay. I'll be okay."
    emi "De acuerdo. Estaré bien."

# game/script-a3-emi.rpy:2972
translate es a3_emi_up_down_and_up_again_6482a22f:

    # hi "See you soon."
    hi "Te veo pronto."

# game/script-a3-emi.rpy:2990
translate es a3_emi_up_down_and_up_again_7c8735bd:

    # "I press the button to end the call before slipping the phone back into my pocket, apologize to Yuuko for running off, and run off."
    "Presiono el botón para finalizar la llamada antes de deslizar el teléfono de vuelta en mi bolsillo, me disculpo con Yuuko por correr, y salgo corriendo."

# game/script-a3-emi.rpy:2995
translate es a3_emi_up_down_and_up_again_862cd951:

    # "Perhaps at some point I would have stopped to think about the time, or how suspicious it looks for a guy to enter the girls' dorm at this hour."
    "Tal vez en algún punto me habría detenido a pensar en la hora, o en qué tan sospechoso se vería un chico entrando al dormitorio de las chicas a esta hora."

# game/script-a3-emi.rpy:2997
translate es a3_emi_up_down_and_up_again_0d2e1ca9:

    # "Except right now I'm just concerned with getting to Emi and finding out what's wrong and how I can help her."
    "Excepto que justo ahora solo me preocupa ir con Emi y averiguar qué anda mal y cómo puedo ayudarla."

# game/script-a3-emi.rpy:3001
translate es a3_emi_up_down_and_up_again_19e19650:

    # "I knock on the door and am greeted with a subdued 'Come in.'"
    "Toco la puerta y soy recibido con un suave “Entra”."

# game/script-a3-emi.rpy:3006
translate es a3_emi_up_down_and_up_again_e83f36b7:

    # "Something is very wrong as I stare at the scene before me."
    "Algo está bastante mal cuando observo la escena frente a mí."

# game/script-a3-emi.rpy:3008
translate es a3_emi_up_down_and_up_again_1df8148b:

    # "Emi's there, yes."
    "Emi está ahí, sí."

# game/script-a3-emi.rpy:3010
translate es a3_emi_up_down_and_up_again_7f3269a5:

    # "But she's in a wheelchair."
    "Pero está en silla de ruedas."

# game/script-a3-emi.rpy:3012
translate es a3_emi_up_down_and_up_again_c096bfdd:

    # "And her legs are missing. I glance around the room and see them sitting in a corner, looking like they've been thrown there."
    "Y sus piernas no están. Miro por la habitación y las veo en una esquina, parecen haber sido arrojadas allí."

# game/script-a3-emi.rpy:3017
translate es a3_emi_up_down_and_up_again_04fb99bc:

    # "Emi responds to my entrance with a lopsided grin that is both pleased to see me and completely, utterly heartbroken."
    "Emi reacciona cuando entro con una sonrisa torcida que se ve tanto complacida de verme como completamente, absolutamente descorazonada."

# game/script-a3-emi.rpy:3019
translate es a3_emi_up_down_and_up_again_61b0a565:

    # emi "Hi, Hisao."
    emi "Hola, Hisao."

# game/script-a3-emi.rpy:3021
translate es a3_emi_up_down_and_up_again_2e17aadc:

    # "It looks like she's been crying, but if she was, she's stopped now."
    "Parece que ha estado llorando, pero si lo hacía, ya se detuvo."

# game/script-a3-emi.rpy:3023
translate es a3_emi_up_down_and_up_again_313bfa8b:

    # "I notice that I'm a little out of breath, having taken the stairs two at a time in order to get here."
    "Noto que estoy algo agitado, habiendo saltado escalones de dos en dos para llegar aquí."

# game/script-a3-emi.rpy:3025
translate es a3_emi_up_down_and_up_again_a1e7bbdd:

    # "My heart doesn't seem to mind the strain, though. I file this happy fact away for later consideration."
    "Sin embargo, mi corazón no parece preocuparse por la tensión. Archivo este agradable hecho para considerarlo más tarde."

# game/script-a3-emi.rpy:3027
translate es a3_emi_up_down_and_up_again_73c68489:

    # "Like when I am not staring somewhat dumbstruck at my girlfriend in a wheelchair."
    "Como para cuando no esté mirando estupefacto a mi novia en silla de ruedas."

# game/script-a3-emi.rpy:3029
translate es a3_emi_up_down_and_up_again_ae88f535:

    # "Realizing I've still not responded to her greeting, my brain lurches into gear."
    "Dándose cuenta de que aún no he respondido a su saludo, mi cerebro se enciende bruscamente."

# game/script-a3-emi.rpy:3031
translate es a3_emi_up_down_and_up_again_0886498e:

    # hi "Emi? What happened?"
    hi "¿Emi? ¿Qué sucedió?"

# game/script-a3-emi.rpy:3036
translate es a3_emi_up_down_and_up_again_912043c9:

    # emi "Guess I should've listened to you, Hisao."
    emi "Supongo que debería haberte escuchado, Hisao."

# game/script-a3-emi.rpy:3041
translate es a3_emi_up_down_and_up_again_643c5b95:

    # emi "My leg's got a nasty infection. I'm not allowed to run on it for at least a couple of weeks."
    emi "Mi pierna tiene una infección desagradable. No me permitirán correr por lo menos durante un par de semanas."

# game/script-a3-emi.rpy:3043
translate es a3_emi_up_down_and_up_again_623fe27f:

    # "She gives a bitter laugh that shouldn't be coming from her."
    "Suelta una risa amarga que no debería provenir de ella."

# game/script-a3-emi.rpy:3048
translate es a3_emi_up_down_and_up_again_7fd644de:

    # emi "Heh, I can't even walk on it."
    emi "Je, ni siquiera puedo caminar."

# game/script-a3-emi.rpy:3050
translate es a3_emi_up_down_and_up_again_fb111d79:

    # emi "I could have used a crutch and kept one of my legs, but I didn't see the point."
    emi "Podía haber usado una muleta y conservar una de mis piernas, pero no veo el punto."

# game/script-a3-emi.rpy:3055
translate es a3_emi_up_down_and_up_again_d07732f2:

    # emi "Why hop? You can't run on one leg."
    emi "¿Por qué cojear? No puedes correr con una pierna."

# game/script-a3-emi.rpy:3060
translate es a3_emi_up_down_and_up_again_4e339765:

    # emi "At least this way I can still, I dunno, roll fast or something."
    emi "Al menos de esta forma puedo, no sé, rodar más rápido o algo."

# game/script-a3-emi.rpy:3062
translate es a3_emi_up_down_and_up_again_1324f439:

    # hi "Y-yeah, that's good, right?"
    hi "S-sí, eso es bueno, ¿cierto?"

# game/script-a3-emi.rpy:3064
translate es a3_emi_up_down_and_up_again_2886ce29:

    # "My awkward attempt to look on the bright side seems appreciated, but not really effective."
    "Mi torpe intento de ver el lado bueno parece ser apreciado, pero no efectivo."

# game/script-a3-emi.rpy:3066
translate es a3_emi_up_down_and_up_again_a29e2378:

    # "Emi shrugs again."
    "Emi se encoge de hombros de nuevo."

# game/script-a3-emi.rpy:3071
translate es a3_emi_up_down_and_up_again_4c3645e5:

    # emi "It's just… kind of a nuisance."
    emi "Es solo… una molestia."

# game/script-a3-emi.rpy:3076
translate es a3_emi_up_down_and_up_again_8478c5fa:

    # emi "I mean, we can't even eat up on the roof now. No wheelchair access."
    emi "Digo, ahora ni siquiera podemos comer en el techo. No tiene acceso para sillas de ruedas."

# game/script-a3-emi.rpy:3078
translate es a3_emi_up_down_and_up_again_cb952baf:

    # hi "Yeah, but that's not a big deal, right?"
    hi "Sí, pero eso no es gran cosa, ¿cierto?"

# game/script-a3-emi.rpy:3080
translate es a3_emi_up_down_and_up_again_595e0f07:

    # hi "I mean we can still eat together, and that's the important thing."
    hi "Quiero decir, aún podemos comer juntos, y eso es lo que importa."

# game/script-a3-emi.rpy:3085
translate es a3_emi_up_down_and_up_again_786ac2e7:

    # "That lopsided grin again. It hurts to look at."
    "Esa sonrisa torcida de nuevo. Duele mirarla."

# game/script-a3-emi.rpy:3087
translate es a3_emi_up_down_and_up_again_ebdc6fd1:

    # emi "I suppose so, yeah."
    emi "Supongo que sí."

# game/script-a3-emi.rpy:3092
translate es a3_emi_up_down_and_up_again_56bff2ff:

    # emi "But like I said, it's a nuisance."
    emi "Pero como dije, es una molestia."

# game/script-a3-emi.rpy:3097
translate es a3_emi_up_down_and_up_again_36be3a30:

    # emi "I mean, I haven't really used a wheelchair in…"
    emi "Digo, no he usado una silla de ruedas en…"

# game/script-a3-emi.rpy:3101
translate es a3_emi_up_down_and_up_again_ca92b07c:

    # "She thinks for a minute."
    "Lo piensa por un momento."

# game/script-a3-emi.rpy:3106
translate es a3_emi_up_down_and_up_again_80041d64:

    # emi "Maybe seven years? Something like that, anyway."
    emi "¿Siete años quizá? En fin, algo así."

# game/script-a3-emi.rpy:3108
translate es a3_emi_up_down_and_up_again_5b1bee7b:

    # emi "A long time."
    emi "Un largo tiempo."

# game/script-a3-emi.rpy:3113
translate es a3_emi_up_down_and_up_again_5bca6df3:

    # emi "I'm afraid I'm a bit out of practice."
    emi "Me temo que he perdido algo de práctica."

# game/script-a3-emi.rpy:3115
translate es a3_emi_up_down_and_up_again_a6c7d5e8:

    # hi "Well, fortunately it's only temporary, right?"
    hi "Bueno, afortunadamente solo es temporal, ¿cierto?"

# game/script-a3-emi.rpy:3117
translate es a3_emi_up_down_and_up_again_61cabdac:

    # "Emi nods."
    "Emi asiente."

# game/script-a3-emi.rpy:3122
translate es a3_emi_up_down_and_up_again_5602bd8f:

    # emi "Oh yeah, of course."
    emi "Oh, sí, por supuesto."

# game/script-a3-emi.rpy:3124
translate es a3_emi_up_down_and_up_again_3f683ac8:

    # emi "It's not like I've lost 'em permanently."
    emi "No es como si las hubiera perdido permanentemente."

# game/script-a3-emi.rpy:3129
translate es a3_emi_up_down_and_up_again_22b6e828:

    # emi "But it's a pain in the ass all the same."
    emi "Pero igual es un dolor de cabeza."

# game/script-a3-emi.rpy:3131
translate es a3_emi_up_down_and_up_again_9e4b9fb6:

    # "I nod sympathetically."
    "Asiento comprensivamente."

# game/script-a3-emi.rpy:3133
translate es a3_emi_up_down_and_up_again_7198b845:

    # "There's not much else I can do, after all."
    "No hay mucho más que pueda hacer, después de todo."

# game/script-a3-emi.rpy:3135
translate es a3_emi_up_down_and_up_again_725bb9e9:

    # "What am I gonna do, say 'I told you so?'"
    "¿Qué voy a hacer? ¿Decir “te lo dije”?"

# game/script-a3-emi.rpy:3137
translate es a3_emi_up_down_and_up_again_152cb06b:

    # "Although I {b}did{/b} tell her to get that leg looked at."
    "Aunque {b}sí{/b} le dije que debían revisarle esa pierna."

# game/script-a3-emi.rpy:3139
translate es a3_emi_up_down_and_up_again_b07c63d4:

    # "But by the time I noticed, it was too late anyway."
    "Pero cuando me di cuenta, ya era demasiado tarde."

# game/script-a3-emi.rpy:3141
translate es a3_emi_up_down_and_up_again_a699921a:

    # hi "Do you need help with anything?"
    hi "¿Necesitas ayuda con algo?"

# game/script-a3-emi.rpy:3143
translate es a3_emi_up_down_and_up_again_eb8b0b0c:

    # hi "Er, that is, can I help with anything?"
    hi "Eh, digo, ¿puedo ayudar con algo?"

# game/script-a3-emi.rpy:3148
translate es a3_emi_up_down_and_up_again_f0a24398:

    # "Emi shakes her head and there's a bit of her usual grin back."
    "Emi sacude la cabeza y vuelve un poco de su sonrisa habitual."

# game/script-a3-emi.rpy:3150
translate es a3_emi_up_down_and_up_again_0b2bf549:

    # emi "Nah, I can manage fine by myself."
    emi "Nah, me las puedo arreglar sola."

# game/script-a3-emi.rpy:3155
translate es a3_emi_up_down_and_up_again_0b3c19ed:

    # emi "Although if you want to help me over to my bed, it would save me the trouble of rolling over there myself."
    emi "Aunque si quieres ayudarme a alcanzar mi cama, me ahorrarías la molestia de rodar hasta allá por mí misma."

# game/script-a3-emi.rpy:3157
translate es a3_emi_up_down_and_up_again_43500873:

    # "I blush, in spite of myself."
    "Me sonrojo, a mi pesar."

# game/script-a3-emi.rpy:3159
translate es a3_emi_up_down_and_up_again_7af30aaa:

    # "Emi giggles."
    "Emi suelta una risita."

# game/script-a3-emi.rpy:3166
translate es a3_emi_up_down_and_up_again_8f5d9fb9:

    # emi "You're such a prude, Hisao."
    emi "Eres un mojigato, Hisao."

# game/script-a3-emi.rpy:3168
translate es a3_emi_up_down_and_up_again_85dcbb34:

    # hi "I'm not a prude! I just wouldn't want to take advantage of a young woman such as yourself."
    hi "¡No lo soy! Solo que no me gustaría aprovecharme de una joven dama como tú."

# game/script-a3-emi.rpy:3170
translate es a3_emi_up_down_and_up_again_6c3fea9b:

    # hi "It's ungentlemanly."
    hi "No es caballeroso."

# game/script-a3-emi.rpy:3178
translate es a3_emi_up_down_and_up_again_1903eb2e:

    # "I wheel Emi's chair to her bed, and easily scoop her up and deposit her there. She quickly sorts herself out and sits on the side."
    "Empujo la silla hasta la cama, alzo a Emi con facilidad y la dejo allí. Ella se acomoda con rapidez y se sienta en el borde."

# game/script-a3-emi.rpy:3189
translate es a3_emi_up_down_and_up_again_f36e85d9:

    # "She's actually a little heavier than she looks. It would be rude of me to observe this aloud, of course."
    "La verdad es que ella es un poco más pesada de lo que parece. Sería grosero de mi parte observar esto en voz alta, por supuesto."

# game/script-a3-emi.rpy:3191
translate es a3_emi_up_down_and_up_again_da9afd7e:

    # hi "Man, you're kind of heavy."
    hi "Vaya, eres bastante pesada."

# game/script-a3-emi.rpy:3205
translate es a3_emi_up_down_and_up_again_172369cb:

    # "Emi hits me with a pillow."
    "Emi me golpea con una almohada."

# game/script-a3-emi.rpy:3210
translate es a3_emi_up_down_and_up_again_da07edab:

    # emi "Ass."
    emi "Idiota."

# game/script-a3-emi.rpy:3212
translate es a3_emi_up_down_and_up_again_9a5576ff:

    # hi "Just sayin', is all."
    hi "Nada más lo decía, es todo."

# game/script-a3-emi.rpy:3214
translate es a3_emi_up_down_and_up_again_2a757153:

    # hi "Must be all that running."
    hi "Debe ser por correr tanto."

# game/script-a3-emi.rpy:3219
translate es a3_emi_up_down_and_up_again_834c68f7:

    # "At the mention of running Emi's grin falters slightly."
    "Al mencionar las carreras la sonrisa de Emi se tambalea un poco."

# game/script-a3-emi.rpy:3224
translate es a3_emi_up_down_and_up_again_bb6e20d8:

    # emi "Heh, well I guess I won't have to worry about that for a bit, huh?"
    emi "Je, bueno, supongo que no tendré que preocuparme por eso durante un tiempo, ¿eh?"

# game/script-a3-emi.rpy:3229
translate es a3_emi_up_down_and_up_again_2ac88e1c:

    # emi "Maybe I'll lose some weight."
    emi "Tal vez pierda algo de peso."

# game/script-a3-emi.rpy:3231
translate es a3_emi_up_down_and_up_again_47f78638:

    # hi "That's what you do to lose weight, right? Cease physical activity?"
    hi "Eso es lo que haces para perder peso, ¿cierto? ¿Cesar la actividad física?"

# game/script-a3-emi.rpy:3236
translate es a3_emi_up_down_and_up_again_e6ca5e7a:

    # emi "I'm pretty sure that's what the nurse would recommend."
    emi "Estoy segura de que eso es lo que recomendaría el enfermero."

# game/script-a3-emi.rpy:3238
translate es a3_emi_up_down_and_up_again_fd636600:

    # hi "Speaking of which, are you going to still be showing up in the mornings?"
    hi "Hablando de eso, ¿aún irás por las mañanas?"

# game/script-a3-emi.rpy:3240
translate es a3_emi_up_down_and_up_again_e1cab925:

    # hi "I'd hate to run alo—"
    hi "No me gustaría correr so—"

# game/script-a3-emi.rpy:3245
translate es a3_emi_up_down_and_up_again_37db3ebc:

    # emi "Ah, shit…"
    emi "Ah, mierda…"

# game/script-a3-emi.rpy:3247
translate es a3_emi_up_down_and_up_again_754a249f:

    # "Emi's sudden interjection, more a disquieted muttering than anything too profane, causes me to look over in shock."
    "La repentina interjección de Emi, más un murmullo inquieto que algo muy profano, me obliga a mirarla, sorprendido."

# game/script-a3-emi.rpy:3249
translate es a3_emi_up_down_and_up_again_f5964946:

    # "She's leaning forward, trying to cover the fact that she's crying by covering her eyes with a hand."
    "Ella está inclinada hacia adelante, intentando ocultar el hecho de que está llorando cubriéndose los ojos con una mano."

# game/script-a3-emi.rpy:3251
translate es a3_emi_up_down_and_up_again_d7f53b59:

    # "Of course, the subdued sobbing makes it pretty obvious that she's crying."
    "Por supuesto, los suaves sollozos hacen que sea bastante evidente que está llorando."

# game/script-a3-emi.rpy:3253
translate es a3_emi_up_down_and_up_again_308a9253:

    # hi "Hey, I'm sorry."
    hi "Oye, lo siento."

# game/script-a3-emi.rpy:3255
translate es a3_emi_up_down_and_up_again_a3e8814d:

    # hi "Forget I said anything, okay?"
    hi "Olvida todo lo que dije, ¿de acuerdo?"

# game/script-a3-emi.rpy:3260
translate es a3_emi_up_down_and_up_again_a083cbe2:

    # "I place a hand gingerly around her and pull her close."
    "Con cautela, coloco una mano a su alrededor y la atraigo más hacia mí."

# game/script-a3-emi.rpy:3262
translate es a3_emi_up_down_and_up_again_a0eda459:

    # "I can think of nothing else to say or do. How do you comfort someone who's just lost their legs again?"
    "No puedo pensar en nada más que hacer o decir. ¿Cómo consuelas a alguien que acaba de perder sus piernas de nuevo?"

# game/script-a3-emi.rpy:3267
translate es a3_emi_up_down_and_up_again_1242f0b5:

    # "Emi wraps me in a hug and stays that way for a while."
    "Emi me atrapa en un abrazo y se queda así por un rato."

# game/script-a3-emi.rpy:3269
translate es a3_emi_up_down_and_up_again_eab1722c:

    # hi "Sorry."
    hi "Perdona."

# game/script-a3-emi.rpy:3271
translate es a3_emi_up_down_and_up_again_3ed8826c:

    # hi "I'm pretty bad at this whole comforting thing, I guess."
    hi "Creo que soy bastante malo consolándote."

# game/script-a3-emi.rpy:3273
translate es a3_emi_up_down_and_up_again_8407207d:

    # emi "Don't say that."
    emi "No digas eso."

# game/script-a3-emi.rpy:3275
translate es a3_emi_up_down_and_up_again_dfb5c13b:

    # emi "I'm fine, really."
    emi "Estoy bien, en serio."

# game/script-a3-emi.rpy:3277
translate es a3_emi_up_down_and_up_again_d0fd81a6:

    # "Her voice is slightly muffled by my chest. I pat her head reassuringly."
    "Su voz resulta ligeramente amortiguada por mi pecho. Le doy unas palmaditas en la cabeza para animarla."

# game/script-a3-emi.rpy:3279
translate es a3_emi_up_down_and_up_again_379787d6:

    # hi "That's the spirit, right?"
    hi "Ese es el espíritu, ¿cierto?"

# game/script-a3-emi.rpy:3281
translate es a3_emi_up_down_and_up_again_55eae125:

    # hi "You'll get through this fine, I know it."
    hi "Saldrás de esto sin problemas, lo sé."

# game/script-a3-emi.rpy:3283
translate es a3_emi_up_down_and_up_again_6b118916:

    # hi "Besides, I'm here to help you, remember?"
    hi "Además, estoy aquí para ayudarte, ¿recuerdas?"

# game/script-a3-emi.rpy:3288
translate es a3_emi_up_down_and_up_again_70c13975:

    # "Emi lifts her head and stares at me with tear-stained eyes."
    "Emi levanta su cabeza y me mira con los ojos llenos de lágrimas."

# game/script-a3-emi.rpy:3293
translate es a3_emi_up_down_and_up_again_0bafb5fd:

    # emi "Can you? Can you really?"
    emi "¿Puedes hacerlo? ¿En serio puedes hacerlo?"

# game/script-a3-emi.rpy:3295
translate es a3_emi_up_down_and_up_again_f73cde47:

    # "She's grinning lopsidedly, and something sparkles in her gaze."
    "Ella está sonriendo torcidamente, y algo brilla en su mirada."

# game/script-a3-emi.rpy:3297
translate es a3_emi_up_down_and_up_again_325a3b55:

    # "I can't tell if I'm being mocked or not."
    "No puedo saber si está bromeando o no."

# game/script-a3-emi.rpy:3299
translate es a3_emi_up_down_and_up_again_b3f25bd3:

    # hi "Of course. I mean sure, you're a bit heavy, but -{w=0.5}{nw}"
    hi "Claro. Digo, la verdad eres un poco pesada, pero—{w=0.5}{nw}"

# game/script-a3-emi.rpy:3306
translate es a3_emi_up_down_and_up_again_3f555334:

    # extend " mmph!"
    extend " ¡mmff!"

# game/script-a3-emi.rpy:3308
translate es a3_emi_up_down_and_up_again_c37dfd9b:

    # "My witty comment is cut off by the sudden press of Emi's lips on mine. I'm caught off guard, and am rewarded by hitting my head on the wall behind her bed."
    "Mi ocurrencia es interrumpida por la súbita presión de los labios de Emi sobre los míos. Me toma desprevenido, y soy recompensado golpeando mi cabeza contra la pared detrás de su cama."

# game/script-a3-emi.rpy:3310
translate es a3_emi_up_down_and_up_again_3b159a51:

    # hi "Ow."
    hi "Ay."

# game/script-a3-emi.rpy:3315
translate es a3_emi_up_down_and_up_again_e4868734:

    # "Emi pulls back, trying to look concerned rather than like she's about to laugh."
    "Emi retrocede, tratando de verse preocupada en vez de como si estuviera a punto de reír."

# game/script-a3-emi.rpy:3317
translate es a3_emi_up_down_and_up_again_f8744be2:

    # emi "Are you okay?"
    emi "¿Estás bien?"

# game/script-a3-emi.rpy:3322
translate es a3_emi_up_down_and_up_again_2b702141:

    # emi "Sorry!"
    emi "¡Perdón!"

# game/script-a3-emi.rpy:3324
translate es a3_emi_up_down_and_up_again_a12a61c9:

    # "I rub my head ruefully and grin back at her."
    "Me froto la cabeza apesadumbradamente y le sonrío de vuelta."

# game/script-a3-emi.rpy:3326
translate es a3_emi_up_down_and_up_again_b5b043a3:

    # hi "Caught me off guard, there."
    hi "Me tomaste desprevenido."

# game/script-a3-emi.rpy:3328
translate es a3_emi_up_down_and_up_again_91d2a934:

    # hi "Is that going to become a habit? Am I going to be lectured by Shizune and Misha more?"
    hi "¿Se va a volver costumbre? ¿Shizune y Misha van a pasar más tiempo regañándome?"

# game/script-a3-emi.rpy:3330
translate es a3_emi_up_down_and_up_again_b035ac35:

    # "At the mention of the duo, Emi giggles."
    "A la mención del dúo, Emi suelta una risita."

# game/script-a3-emi.rpy:3335
translate es a3_emi_up_down_and_up_again_8874a93a:

    # emi "Honestly, those two…"
    emi "En serio, esas dos…"

# game/script-a3-emi.rpy:3340
translate es a3_emi_up_down_and_up_again_1df67718:

    # emi "If I didn't know why, I'd be utterly confused as to why she hangs around with someone so bossy."
    emi "Si no supiera la razón, no entendería en absoluto por qué ella anda con alguien tan mandona."

# game/script-a3-emi.rpy:3342
translate es a3_emi_up_down_and_up_again_5cbfab81:

    # hi "Which one are we talking about?"
    hi "¿De cuál de las dos hablamos?"

# game/script-a3-emi.rpy:3347
translate es a3_emi_up_down_and_up_again_ab088914:

    # emi "You know exactly which one, Hisao. Misha's hardly bossy."
    emi "Sabes exactamente de cuál, Hisao. Misha difícilmente es mandona."

# game/script-a3-emi.rpy:3349
translate es a3_emi_up_down_and_up_again_cd3b68a6:

    # hi "So what's the reason, then?"
    hi "Entonces, ¿cuál es la razón?"

# game/script-a3-emi.rpy:3354
translate es a3_emi_up_down_and_up_again_d5e5048f:

    # emi "Huh?"
    emi "¿Huh?"

# game/script-a3-emi.rpy:3356
translate es a3_emi_up_down_and_up_again_27f9b373:

    # hi "The reason why Misha hangs around Shizune."
    hi "La razón por la que Misha anda con Shizune."

# game/script-a3-emi.rpy:3361
translate es a3_emi_up_down_and_up_again_7e1f9b4e:

    # "Emi waves my question off with a smile."
    "Emi evita mi pregunta con una sonrisa."

# game/script-a3-emi.rpy:3363
translate es a3_emi_up_down_and_up_again_ab9a83a6:

    # emi "No idea."
    emi "Ni idea."

# game/script-a3-emi.rpy:3365
translate es a3_emi_up_down_and_up_again_e1f8a5e6:

    # hi "I see."
    hi "Ya veo."

# game/script-a3-emi.rpy:3370
translate es a3_emi_up_down_and_up_again_b1367cf6:

    # emi "Anyway, you seem to be forgetting the original question, don't you?"
    emi "De todos modos, pareces estar olvidando la pregunta original, ¿no?"

# game/script-a3-emi.rpy:3372
translate es a3_emi_up_down_and_up_again_7592e090:

    # hi "Oh yeah, I guess I am."
    hi "Oh, sí, creo que sí."

# game/script-a3-emi.rpy:3374
translate es a3_emi_up_down_and_up_again_b3059c80:

    # hi "You wouldn't mind giving a guy a little warning, would you?"
    hi "No te molestaría darle al chico un pequeño aviso, ¿no crees?"

# game/script-a3-emi.rpy:3376
translate es a3_emi_up_down_and_up_again_3d4a1bf1:

    # hi "Otherwise I'm liable to wind up with a concussion."
    hi "De otra manera podría terminar con una conmoción."

# game/script-a3-emi.rpy:3378
translate es a3_emi_up_down_and_up_again_9578240e:

    # "I emphasize the point by rubbing at the back of my head."
    "Enfatizo el punto frotándome la parte trasera de la cabeza."

# game/script-a3-emi.rpy:3383
translate es a3_emi_up_down_and_up_again_f58d04f2:

    # "Emi giggles madly."
    "Emi suelta una risita malvada."

# game/script-a3-emi.rpy:3385
translate es a3_emi_up_down_and_up_again_a725e695:

    # emi "You could wear a helmet."
    emi "Podrías usar casco."

# game/script-a3-emi.rpy:3390
translate es a3_emi_up_down_and_up_again_0ff15731:

    # emi "Some kids here do, you know."
    emi "Algunos chicos de aquí lo hacen, sabes."

# game/script-a3-emi.rpy:3394
translate es a3_emi_up_down_and_up_again_092670d8:

    # hi "Or I could just take revenge!"
    hi "¡O podría cobrar venganza!"

# game/script-a3-emi.rpy:3401
translate es a3_emi_up_down_and_up_again_7669e4fc:

    # "I grab a pillow from beside me and whack Emi over the head."
    "Agarro una almohada de atrás y golpeo a Emi en la cabeza."

# game/script-a3-emi.rpy:3420
translate es a3_emi_up_down_and_up_again_c470ef6d:

    # "Emi topples off the bed and lands on the floor with a thump."
    "Emi cae de la cama y aterriza en el suelo con un golpe."

# game/script-a3-emi.rpy:3428
translate es a3_emi_up_down_and_up_again_644d14b5:

    # "Her arms promptly reappear on the bed, and she manages to pull herself back up."
    "Sus brazos reaparecen rápidamente en la cama, y se las arregla para subir de nuevo."

# game/script-a3-emi.rpy:3430
translate es a3_emi_up_down_and_up_again_f05aeb67:

    # "She really has a surprising amount of strength in that little body."
    "Realmente tiene una cantidad sorprendente de fuerza en ese pequeño cuerpo."

# game/script-a3-emi.rpy:3432
translate es a3_emi_up_down_and_up_again_64eb1e86:

    # "Her face is turned downwards and away from mine, making me think I might have accidentally hurt her."
    "Su cara está vuelta hacia abajo, mirando hacia el lado opuesto, lo que me hace pensar que podría haberla lastimado accidentalmente."

# game/script-a3-emi.rpy:3434
translate es a3_emi_up_down_and_up_again_9fd0eaa0:

    # hi "Emi? You okay?"
    hi "¿Emi? ¿Estás bien?"

# game/script-a3-emi.rpy:3436
translate es a3_emi_up_down_and_up_again_2721129b:

    # hi "You didn't hit your—{w=0.3}{nw}"
    hi "No te golpeaste la—{w=0.3}{nw}"

# game/script-a3-emi.rpy:3441
translate es a3_emi_up_down_and_up_again_dc0cfcde:

    # "A hand shoots up and grabs my collar. She pulls me in with a sharp tug, her face now barely an inch away from mine as she grins cheekily."
    "Una mano sale disparada y agarra el cuello de mi camisa. Ella me acerca con un fuerte tirón, ahora su rostro está apenas a un centímetro del mío mientras sonríe descaradamente."

# game/script-a3-emi.rpy:3443
translate es a3_emi_up_down_and_up_again_4932c45f:

    # hi "Emi…?"
    hi "¿Emi…?"

# game/script-a3-emi.rpy:3456
translate es a3_emi_up_down_and_up_again_1023b7e6:

    # "She gives me a sharp headbutt, our foreheads making quite a loud thud."
    "Ella me da un brusco cabezazo, y nuestras frentes producen un ruido bastante fuerte."

# game/script-a3-emi.rpy:3462
translate es a3_emi_up_down_and_up_again_5ef367cb:

    # "I sit back and rub my now sore head as Emi smirks victoriously."
    "Me siento y froto mi adolorida cabeza mientras Emi sonríe victoriosa."

# game/script-a3-emi.rpy:3467
translate es a3_emi_up_down_and_up_again_598e6989:

    # emi "How's {b}that{/b} for revenge?"
    emi "¿Qué tal {b}eso{/b} como venganza?"

# game/script-a3-emi.rpy:3471
translate es a3_emi_up_down_and_up_again_53fc202d:

    # hi "No fair!"
    hi "¡No es justo!"

# game/script-a3-emi.rpy:3473
translate es a3_emi_up_down_and_up_again_7ee18cae:

    # hi "You can't take revenge for revenge!"
    hi "¡No puedes vengarte de alguien que se está vengando!"

# game/script-a3-emi.rpy:3475
translate es a3_emi_up_down_and_up_again_dda94b2d:

    # "For someone missing most of her legs, Emi's surprisingly agile."
    "Para alguien que no tiene gran parte de sus piernas, Emi es sorprendentemente ágil."

# game/script-a3-emi.rpy:3488
translate es a3_emi_up_down_and_up_again_bbb1a491:

    # "I swipe at her, but she deftly rolls out of the way and lands a hit with her pillow."
    "Me deslizo hasta su posición, pero ella rueda hábilmente y acierta un golpe con su almohada."

# game/script-a3-emi.rpy:3490
translate es a3_emi_up_down_and_up_again_0e757448:

    # "Of course, the odds are against her. I can stand up, for starters."
    "Claro, las probabilidades están en su contra. Yo puedo ponerme en pie, para empezar."

# game/script-a3-emi.rpy:3495
translate es a3_emi_up_down_and_up_again_aae32818:

    # "Oof!"
    "¡Auch!"

# game/script-a3-emi.rpy:3504
translate es a3_emi_up_down_and_up_again_e27b1f19:

    # "Guess I can't, after all. Emi seems to have effectively tripped me up, and is now sitting primly astride me as I lay on my back. I'm not even sure how she managed it."
    "Supongo que no puedo, después de todo. Emi parece haberme tirado abajo eficazmente, y ahora está sentada a horcajadas sobre mí mientras yo estoy de espaldas al suelo. Ni siquiera estoy seguro de cómo lo consiguió."

# game/script-a3-emi.rpy:3506
translate es a3_emi_up_down_and_up_again_57acfbbb:

    # emi "I win!"
    emi "¡Yo gano!"

# game/script-a3-emi.rpy:3508
translate es a3_emi_up_down_and_up_again_67e6431b:

    # "Her eyes twinkle mischievously. I've been thoroughly defeated, and by a girl that's a fraction of my size, at that."
    "Sus ojos brillan traviesamente. He sido completamente derrotado por una chica que es de una fracción de mi tamaño, sin más."

# game/script-a3-emi.rpy:3510
translate es a3_emi_up_down_and_up_again_bd674f9b:

    # "Then again, being defeated doesn't seem quite so bad. Emi being positioned over my waist isn't something that I, or my body, can ignore easily."
    "Por otra parte, ser el perdedor no parece tan malo. Emi posicionada sobre mi cintura no es algo que yo, o mi cuerpo, podamos ignorar fácilmente."

# game/script-a3-emi.rpy:3515
translate es a3_emi_up_down_and_up_again_f06f210d:

    # "I open my lips to speak, but Emi's head darts downwards before I can get so much as a word out. I give no resistance as she presses her mouth to mine, not that I'd want to."
    "Abro mi boca para hablar, pero Emi baja su cabeza rápidamente antes de que yo pueda pronunciar media palabra. No opongo resistencia mientras ella presiona su boca contra la mía, tampoco es que quiera hacerlo."

# game/script-a3-emi.rpy:3517
translate es a3_emi_up_down_and_up_again_742be0e4:

    # "This is… different, somehow."
    "Esto es… diferente, de algún modo."

# game/script-a3-emi.rpy:3519
translate es a3_emi_up_down_and_up_again_999036c7:

    # "She pulls back, nips at my lower lip, and reinitiates the embrace. Her tongue darts inside my mouth, exploring. I can feel a warmth spreading through my body as my heart begins to beat faster."
    "Ella se detiene, mordisquea mi labio inferior, y continúa con su abrazo. Su lengua se apresura a explorar el interior de mi boca. Puedo sentir un calor extendiéndose a través de mi cuerpo mientras mi corazón comienza a latir más rápido."

# game/script-a3-emi.rpy:3521
translate es a3_emi_up_down_and_up_again_e35e85d0:

    # "My mind starts to go foggy, and I become vaguely aware of my hand traveling up Emi's blouse. Emi gasps as I reach a breast, then there's a giggle, and then—"
    "Mi mente empieza a enturbiarse, y me vuelvo vagamente consciente de mi mano viajando hacia la blusa de Emi. Emi suspira cuando alcanzo un pecho, luego hay una risita, y luego—"

# game/script-a3-emi.rpy:3526
translate es a3_emi_up_down_and_up_again_bcb72d98:

    # "I stare up at a grinning Emi."
    "Miro hacia arriba, hacia una Emi sonriente."

# game/script-a3-emi.rpy:3528
translate es a3_emi_up_down_and_up_again_09cca8be:

    # emi "Told you. That makes my second win, now."
    emi "Te lo dije. Ahora esa es mi segunda victoria."

# game/script-a3-emi.rpy:3530
translate es a3_emi_up_down_and_up_again_a451435b:

    # hi "What? That doesn't count; you used feminine wiles."
    hi "¿Qué? Eso no cuenta; usaste tus encantos femeninos."

# game/script-a3-emi.rpy:3535
translate es a3_emi_up_down_and_up_again_cdc611e5:

    # emi "'All's fair in love and war,' right?"
    emi "“En el amor y la guerra todo se vale”, ¿no?"

# game/script-a3-emi.rpy:3537
translate es a3_emi_up_down_and_up_again_2490623b:

    # emi "Ha, and you're even blushing! I didn't know you were a blusher, Hisao."
    emi "Ja, ¡hasta te estás sonrojando! No sabía que eras de los que se sonrojan, Hisao."

# game/script-a3-emi.rpy:3539
translate es a3_emi_up_down_and_up_again_e8594ea2:

    # hi "You were blushing too, you know. Probably because of your prudish ways."
    hi "Tú también te estabas sonrojando, sabes. Probablemente debido a tus actos indecentes."

# game/script-a3-emi.rpy:3541
translate es a3_emi_up_down_and_up_again_c3f7529a:

    # "Even I've got to admit this is a stupid thing to say to a woman who is currently straddling me and has been, up until a few seconds ago, playing tonsil hockey with me."
    "Incluso yo debo admitir que es una estupidez decirle esto a una mujer que está a horcajadas sobre mí y ha estado, hasta hace unos instantes, jugando a explorar nuestras gargantas."

# game/script-a3-emi.rpy:3546
translate es a3_emi_up_down_and_up_again_0f15a1f4:

    # emi "A prude, am I?"
    emi "Una indecente, ¿yo?"

# game/script-a3-emi.rpy:3548
translate es a3_emi_up_down_and_up_again_1d46142d:

    # emi "Well then, let's see who blushes first, shall we?"
    emi "Pues bien, veamos quién se sonroja primero, ¿de acuerdo?"

# game/script-a3-emi.rpy:3550
translate es a3_emi_up_down_and_up_again_8dc2e3a9:

    # "I'm not sure whether the tone of her voice terrifies or arouses me, but that question is quickly made rather moot."
    "No estoy seguro de si el tono de su voz me aterroriza o me provoca, pero esa pregunta se vuelve irrelevante con rapidez."

# game/script-a3-emi.rpy:3558
translate es a3_emi_up_down_and_up_again_9c28dc3e:

    # "In a motion of practiced ease, she peels her blouse off and tosses it carelessly aside. Her bra and skirt quickly follow it onto the floor."
    "En un movimiento de facilidad practicada, ella se quita su blusa y la arroja despreocupadamente a un lado. Su sujetador y falda la siguen rápidamente al suelo."

# game/script-a3-emi.rpy:3560
translate es a3_emi_up_down_and_up_again_2ca6399c:

    # emi "Ha!"
    emi "¡Ja!"

# game/script-a3-emi.rpy:3562
translate es a3_emi_up_down_and_up_again_c74a78a8:

    # "I fight the urge to blush. It's a rather hard task."
    "Resisto la urgencia de sonrojarme. Es una tarea sumamente difícil."

# game/script-a3-emi.rpy:3564
translate es a3_emi_up_down_and_up_again_cdc44a85:

    # hi "Escalation, is it?"
    hi "Poco a poco, ¿no?"

# game/script-a3-emi.rpy:3569
translate es a3_emi_up_down_and_up_again_58751ef0:

    # "My own shirt follows suit, albeit with some difficulty thanks to my position. Emi mock-yawns."
    "Mi camisa sigue el ejemplo, no sin algo de dificultad gracias a mi posición. Emi finge un bostezo."

# game/script-a3-emi.rpy:3571
translate es a3_emi_up_down_and_up_again_34dc0bc0:

    # emi "You'll have to try harder than th—"
    emi "Tendrás que hacer algo más que e—"

# game/script-a3-emi.rpy:3577
translate es a3_emi_up_down_and_up_again_ac06165e:

    # emi "Ah…!"
    emi "¡Ah…!"

# game/script-a3-emi.rpy:3579
translate es a3_emi_up_down_and_up_again_20dd5674:

    # "My hands gently caress Emi's bare skin, causing her to shiver. It would seem that my hands are acting on their own, again. If our position had let me, I'd probably have finished her undressing for her."
    "Mis manos acarician suavemente la piel desnuda de Emi, provocándole un escalofrío. Parece ser que mis manos actúan por sí mismas, de nuevo. Si nuestra posición me lo permitiera, probablemente terminaría de desnudarla."

# game/script-a3-emi.rpy:3581
translate es a3_emi_up_down_and_up_again_af6055c4:

    # "I start to say something about how Emi's starting to blush, but both of us are very rapidly reaching the edge of something very barely holding us back. Conversation grinds to a halt, and I feel my arms losing energy."
    "Empiezo a decir algo acerca de cómo Emi ha comenzado a sonrojarse, pero los dos estamos aproximándonos con rapidez a los límites de algo que apenas nos está conteniendo. Nuestra conversación se detiene por completo, y siento cómo mis brazos pierden energía."

# game/script-a3-emi.rpy:3585
translate es a3_emi_up_down_and_up_again_469be206:

    # "Neither of us, however, is prepared for this sudden new sensation."
    "Ninguno de los dos, sin embargo, está preparado para esta súbita y nueva sensación."

# game/script-a3-emi.rpy:3590
translate es a3_emi_up_down_and_up_again_45c99ed5:

    # "An indescribable heat surges through me, coming from both myself and, it seems, Emi as well."
    "Un indescriptible calor recorre mi cuerpo, viniendo tanto de mí como, al parecer, de Emi."

# game/script-a3-emi.rpy:3592
translate es a3_emi_up_down_and_up_again_0ffede01:

    # "With one hand on my chest to steady herself, and another holding mine to make sure that I can't have my way with her body again, she looks quite pleased with herself."
    "Con una mano en mi pecho para estabilizarse, y otra sosteniendo la mía para asegurarse de que no puedo abrirme paso por su cuerpo de nuevo, ella se ve bastante complacida consigo misma."

# game/script-a3-emi.rpy:3597
translate es a3_emi_up_down_and_up_again_e8e47fe9:

    # "And then, after a moment's hesitation, she moves."
    "Y entonces, luego de un momento de duda, se mueve."

# game/script-a3-emi.rpy:3599
translate es a3_emi_up_down_and_up_again_4bc641cf:

    # "And she moves again."
    "Y se mueve de nuevo."

# game/script-a3-emi.rpy:3601
translate es a3_emi_up_down_and_up_again_ded17c8a:

    # "And again."
    "Y de nuevo."

# game/script-a3-emi.rpy:3603
translate es a3_emi_up_down_and_up_again_7c902df3:

    # "As she moves, Emi's breath hitches. My breathing is starting to come faster, and more raggedly as well."
    "Mientras se mueve, su respiración sube y baja. Mi respiración también se está volviendo más rápida y entrecortada."

# game/script-a3-emi.rpy:3605
translate es a3_emi_up_down_and_up_again_88d57a24:

    # "Emi's body shivers and shudders against mine, and I can feel her starting to lose her balance. It must be harder for her to keep steady because she's missing her legs."
    "El cuerpo de Emi tiembla y se estremece contra el mío, y puedo sentir cómo ella empieza a perder el balance. Debe de ser más difícil para ella mantenerse estable porque no tiene piernas."

# game/script-a3-emi.rpy:3610
translate es a3_emi_up_down_and_up_again_baca9a18:

    # "I steady her as best as I can, cupping my hands around her backside. It's firm and taut."
    "La estabilizo lo mejor que puedo, posicionando mis manos sobre su trasero. Es firme y tenso."

# game/script-a3-emi.rpy:3612
translate es a3_emi_up_down_and_up_again_b0d32b34:

    # "Makes sense, considering how much she runs. The potential power in those muscles makes them flex as she responds to my touch."
    "Tiene sentido, considerando lo mucho que corre. El poder latente en esos músculos hace que se flexionen cuando ella responde a mi contacto."

# game/script-a3-emi.rpy:3614
translate es a3_emi_up_down_and_up_again_b93945fc:

    # "What I fail to take into account is the fact that my attempt to steady Emi kind of slides her forward and, well… It feels amazing."
    "Lo que fallo a tener en cuenta es que mi intento de estabilizarla hace que ella se deslice un poco hacia adelante y, bueno… Se siente increíble."

# game/script-a3-emi.rpy:3619
translate es a3_emi_up_down_and_up_again_2ec8f709:

    # "Her panties slide easily against my trousers, and it doesn't take us long to figure out a rhythm."
    "Su ropa interior se desliza fácilmente contra mis pantalones, y no nos toma mucho tiempo establecer un ritmo."

# game/script-a3-emi.rpy:3621
translate es a3_emi_up_down_and_up_again_dbaad5f5:

    # "But Emi refuses to keep to it, going now fast, now slow, now pausing for what feels like an eternity. I'm not sure whether she's doing this to toy with me, or if it's to make her feel better, but I'm well past caring."
    "Pero Emi se niega a seguirlo, yendo ahora rápido, ahora lento, ahora deteniéndose por lo que se siente como una eternidad. No estoy seguro de si lo hace por jugar conmigo, o si lo hace para sentirse mejor, pero no podría importarme menos."

# game/script-a3-emi.rpy:3623
translate es a3_emi_up_down_and_up_again_6a2cea84:

    # "The heat between us is growing more intense, and I can't hold back a gasp. The noise only seems to drive Emi along."
    "El calor entre nosotros está creciendo en intensidad, y no puedo retener un suspiro. El sonido solo parece provocar a Emi."

# game/script-a3-emi.rpy:3625
translate es a3_emi_up_down_and_up_again_b334fa84:

    # "I begin to punctuate her movements with some of my own, which causes her modest breasts to bounce in time with my movements. Her breath begins to come faster as we continue, my own breathing becoming equally quick."
    "Empiezo a remarcar sus movimientos con los míos, lo que causa que sus modestos pechos reboten con mis movimientos. Su respiración empieza a acelerar mientras continuamos, con mi propia respiración acelerando del mismo modo."

# game/script-a3-emi.rpy:3627
translate es a3_emi_up_down_and_up_again_dcb00269:

    # "With her eyes closed, her lips purse expectantly. I just manage to lift myself up for a few moments. Our mouths seeking one another, her chest sliding against mine as our sweat mingles."
    "Con los ojos cerrados, aprieta sus labios con expectación. Me las arreglo para levantarme por unos instantes. Nuestras bocas buscándose la una a la otra, su pecho deslizándose contra el mío mientras nuestro sudor se mezcla."

# game/script-a3-emi.rpy:3629
translate es a3_emi_up_down_and_up_again_7e4a802e:

    # "As I flop back down, my trousers are soaked with sweat. I would take them off if it didn't mean stopping what we're doing."
    "Cuando me acuesto de nuevo, mis pantalones están empapados con sudor. Me los quitaría si eso no significase detener lo que estamos haciendo."

# game/script-a3-emi.rpy:3631
translate es a3_emi_up_down_and_up_again_8288dda7:

    # "And I don't want to stop what we're doing, stop this growing pressure, this tickling in the back of my brain."
    "Y no quiero detener lo que estamos haciendo, detener esta presión cada vez mayor, este cosquilleo en lo recóndito de mi cerebro."

# game/script-a3-emi.rpy:3633
translate es a3_emi_up_down_and_up_again_f01303fb:

    # "Emi is sliding faster and faster, panting heavily, her voice seemingly unable to convey what she's feeling. Her body, on the other hand, is doing a fine job."
    "Emi está deslizándose más y más rápido, respirando pesadamente, con su voz aparentemente incapaz de expresar lo que está sintiendo. Su cuerpo, por otro lado, está haciendo un buen trabajo."

# game/script-a3-emi.rpy:3638
translate es a3_emi_up_down_and_up_again_4d6274e5:

    # "Suddenly she moves a little more erratically as my own breath hitches in my throat, ending in a final desperate thrust that sends me over the edge into a surging feeling I didn't know existed."
    "De repente, ella cambia un poco sus movimientos mientras mi respiración se enreda en mi garganta, terminando en una estocada desesperada que me envía sobre los límites hacia una sensación arrasadora que no sabía que existía."

# game/script-a3-emi.rpy:3643
translate es a3_emi_up_down_and_up_again_ec1d7169:

    # "My mind blanks, fills with white noise, and I succumb to the feeling of climax. For a few seconds, everything else in the world falls away except for this amazing feeling of Emi and I, together."
    "Mi mente se vacía, se llena de ruido blanco, y sucumbo a la sensación del clímax. Por unos segundos, todo lo demás en el mundo colapsa excepto por esta increíble sensación de Emi y yo, juntos."

# game/script-a3-emi.rpy:3648
translate es a3_emi_up_down_and_up_again_3cb3c649:

    # "And then… it passes. The white noise clears, and I am left staring up into the eyes of the girl atop me."
    "Y luego… termina. El ruido blanco se aclara, y me quedo mirando hacia los ojos de la chica encima de mí."

# game/script-a3-emi.rpy:3650
translate es a3_emi_up_down_and_up_again_2fa0f41e:

    # "For a few minutes, neither of us speaks. The sound of our breathing fills the room, our chests heaving from the experience."
    "Por unos minutos, ninguno de los dos habla. El sonido de nuestra respiración llena el lugar, nuestros pechos agitados por la experiencia."

# game/script-a3-emi.rpy:3652
translate es a3_emi_up_down_and_up_again_5e239e14:

    # "She eventually, reluctantly, shifts off of me and sits against the wall. I join her."
    "Eventualmente ella, de mala gana, se mueve de encima y se sienta contra la pared. Me uno a ella."

# game/script-a3-emi.rpy:3660
translate es a3_emi_up_down_and_up_again_28e20d40:

    # emi "So… did I blush?"
    emi "Entonces… ¿me sonrojé?"

# game/script-a3-emi.rpy:3662
translate es a3_emi_up_down_and_up_again_9b12a75d:

    # hi "I didn't notice."
    hi "No me fijé."

# game/script-a3-emi.rpy:3664
translate es a3_emi_up_down_and_up_again_5218fda5:

    # hi "Did I?"
    hi "¿Y yo?"

# game/script-a3-emi.rpy:3669
translate es a3_emi_up_down_and_up_again_73f37c8f:

    # "Emi shrugs, still breathing a little heavily."
    "Emi se encoge de hombros, todavía respirando pesadamente."

# game/script-a3-emi.rpy:3674
translate es a3_emi_up_down_and_up_again_4ea3d9a0:

    # emi "Didn't notice either."
    emi "Tampoco me fijé."

# game/script-a3-emi.rpy:3676
translate es a3_emi_up_down_and_up_again_dcecc211:

    # hi "Well, maybe we should—"
    hi "Bueno, tal vez deberíamos—"

# game/script-a3-emi.rpy:3691
translate es a3_emi_up_down_and_up_again_026d8715:

    # rin "I need to use your window."
    rin "Necesito usar tu ventana."

# game/script-a3-emi.rpy:3693
translate es a3_emi_up_down_and_up_again_2020e93b:

    # "My first instinct is to hide, but then I realize that I'm still utterly exhausted and sitting next to a topless Emi, so there's no running anyway."
    "Mi primera reacción es esconderme, pero luego me doy cuenta de que todavía estoy totalmente exhausto y sentado junto a una Emi con los pechos al aire, así que no hay escapatoria."

# game/script-a3-emi.rpy:3704
translate es a3_emi_up_down_and_up_again_598792d5:

    # "Rin's eyes pass over Emi, and me, and focus on the window."
    "Los ojos de Rin pasan sobre Emi, sobre mí, y se fijan en la ventana."

# game/script-a3-emi.rpy:3709
translate es a3_emi_up_down_and_up_again_c9140f13:

    # rin "There was a cloud."
    rin "Había una nube."

# game/script-a3-emi.rpy:3716
translate es a3_emi_up_down_and_up_again_6d92def0:

    # emi "A cloud?"
    emi "¿Una nube?"

# game/script-a3-emi.rpy:3721
translate es a3_emi_up_down_and_up_again_a14f4291:

    # "Rin nods."
    "Rin asiente."

# game/script-a3-emi.rpy:3726
translate es a3_emi_up_down_and_up_again_3aef4a3d:

    # rin "I was watching it from my window, but it didn't stay in my window."
    rin "La estaba observando desde mi ventana, pero no se quedó en mi ventana."

# game/script-a3-emi.rpy:3731
translate es a3_emi_up_down_and_up_again_ddbc276a:

    # rin "So I need to use your window."
    rin "Así que necesito usar tu ventana."

# game/script-a3-emi.rpy:3736
translate es a3_emi_up_down_and_up_again_96ee1481:

    # "Emi shifts a little, causing me to cough in order to cover up a giggle of my own."
    "Emi se retuerce un poco, obligándome a toser para ocultar mi propia risa."

# game/script-a3-emi.rpy:3738
translate es a3_emi_up_down_and_up_again_6fb9aa14:

    # emi "How long do you need the window for?"
    emi "¿Cuánto tiempo necesitas la ventana?"

# game/script-a3-emi.rpy:3740
translate es a3_emi_up_down_and_up_again_a60fd612:

    # emi "We're uh."
    emi "Estamos eh."

# game/script-a3-emi.rpy:3745
translate es a3_emi_up_down_and_up_again_ce17125c:

    # emi "Busy."
    emi "Ocupados."

# game/script-a3-emi.rpy:3747
translate es a3_emi_up_down_and_up_again_22a49f30:

    # "This time I can't contain my laughter."
    "Esta vez no puedo contener mi risa."

# game/script-a3-emi.rpy:3752
translate es a3_emi_up_down_and_up_again_f32d8185:

    # "Rin ignores both Emi and me and peers out the window."
    "Rin nos ignora a los dos y observa por la ventana."

# game/script-a3-emi.rpy:3757
translate es a3_emi_up_down_and_up_again_3afa2cce:

    # "Her shoulders slump, and she looks disappointed."
    "Ella deja caer los hombros de golpe, y se ve decepcionada."

# game/script-a3-emi.rpy:3759
translate es a3_emi_up_down_and_up_again_81383cfa:

    # rin "Hmm."
    rin "Hmm."

# game/script-a3-emi.rpy:3761
translate es a3_emi_up_down_and_up_again_66bb81ef:

    # rin "It changed into something else."
    rin "Cambió a otra cosa."

# game/script-a3-emi.rpy:3763
translate es a3_emi_up_down_and_up_again_320a131a:

    # rin "Disappointing."
    rin "Decepcionante."

# game/script-a3-emi.rpy:3768
translate es a3_emi_up_down_and_up_again_b5eceb3e:

    # "Emi is having trouble keeping a straight face."
    "Emi está teniendo problemas para mantener una cara seria."

# game/script-a3-emi.rpy:3770
translate es a3_emi_up_down_and_up_again_2063b8fe:

    # emi "Sorry to hear that, Rin."
    emi "Lamento escucharlo, Rin."

# game/script-a3-emi.rpy:3775
translate es a3_emi_up_down_and_up_again_f132638c:

    # emi "Could we have a little privacy now, please?"
    emi "¿Podríamos tener algo de privacidad ahora, por favor?"

# game/script-a3-emi.rpy:3791
translate es a3_emi_up_down_and_up_again_8195ae23:

    # "Rin shrugs, as if to say 'Can you?' and hooks her foot around the door, pulling it closed behind her."
    "Rin se encoge de hombros, como si dijera ¿“puedes”? y arrastra sus pies a través de la puerta, cerrándola tras ella."

# game/script-a3-emi.rpy:3796
translate es a3_emi_up_down_and_up_again_0f2a173e:

    # "We both dissolve into raucous laughter, unable to deal with Rin's bizarrely timed visit any other way."
    "Ambos soltamos una estridente carcajada, incapaces de lidiar con la extrañamente sincronizada visita de Rin de otra manera."

# game/script-a3-emi.rpy:3798
translate es a3_emi_up_down_and_up_again_d4335c11:

    # "After our laughter dies down, I look to Emi. We're both a total mess."
    "Luego de que se detienen nuestras carcajadas, miro a Emi. Somos un completo desastre."

# game/script-a3-emi.rpy:3802
translate es a3_emi_up_down_and_up_again_b1afde36:

    # hi "Well."
    hi "Entonces."

# game/script-a3-emi.rpy:3807
translate es a3_emi_up_down_and_up_again_3de31db1:

    # "Emi raises an eyebrow."
    "Emi alza una ceja."

# game/script-a3-emi.rpy:3809
translate es a3_emi_up_down_and_up_again_9ea2d850:

    # emi "Well?"
    emi "¿Entonces?"

# game/script-a3-emi.rpy:3811
translate es a3_emi_up_down_and_up_again_755b0e12:

    # hi "Again?"
    hi "¿Otra vez?"

# game/script-a3-emi.rpy:3816
translate es a3_emi_up_down_and_up_again_1b844624:

    # "Emi grins and laughs, and then she nods."
    "Emi suelta una carcajada, y luego asiente."

# game/script-a3-emi.rpy:3821
translate es a3_emi_up_down_and_up_again_ab45c742:

    # emi "We should probably ditch the clothes, this time."
    emi "Probablemente deberíamos de quitarnos la ropa esta vez."

# game/script-a3-emi.rpy:3839
translate es a3_emi_storage_space_b0a49044:

    # "The sunlight breaks through my window shortly before my alarm ruins the morning silence."
    "El resplandor del sol atraviesa mi ventana poco después de que mi alarma arruina el silencio de la mañana."

# game/script-a3-emi.rpy:3843
translate es a3_emi_storage_space_6441d2ec:

    # "I feel sore."
    "Me siento adolorido."

# game/script-a3-emi.rpy:3845
translate es a3_emi_storage_space_921e7b26:

    # "The events of the previous evening suddenly intrude upon my consciousness, and I find myself blushing."
    "Los eventos de la noche anterior invaden de repente mi conciencia, y termino sonrojándome."

# game/script-a3-emi.rpy:3847
translate es a3_emi_storage_space_2938d594:

    # "That was an eventful evening - and it explains perfectly the soreness in my lower back."
    "Fue una noche memorable, y explica perfectamente el dolor en la parte baja de mi espalda."

# game/script-a3-emi.rpy:3849
translate es a3_emi_storage_space_982d5491:

    # "The walk back, as I recall, had been rather tense."
    "Caminar de regreso, según recuerdo, fue bastante tenso."

# game/script-a3-emi.rpy:3851
translate es a3_emi_storage_space_5e17ace9:

    # "My trousers having been… soiled, I had washed them off in the bathroom before going back to my room."
    "Mis pantalones terminaron… empapados, los lavé en la ducha antes de regresar a mi habitación."

# game/script-a3-emi.rpy:3853
translate es a3_emi_storage_space_20d625c9:

    # "But there was still a fairly obvious-looking stain on the front."
    "Pero aún había una mancha considerablemente visible en el frente."

# game/script-a3-emi.rpy:3855
translate es a3_emi_storage_space_aa1576ff:

    # "Fortunately for me, the only person I ran into on my way back was Kenji."
    "Por fortuna para mí, Kenji fue la única persona con quien me encontré mientras regresaba."

# game/script-a3-emi.rpy:3857
translate es a3_emi_storage_space_b2f85952:

    # "And he didn't notice a thing."
    "Y él no notó nada."

# game/script-a3-emi.rpy:3859
translate es a3_emi_storage_space_ea286f07:

    # "Well, apart from my being in the general vicinity."
    "Bueno, aparte de mi presencia en su vecinidad general."

# game/script-a3-emi.rpy:3861
translate es a3_emi_storage_space_2d4fb63f:

    # "Of course he'd asked how the night went, and whether or not I'd learned anything of importance."
    "Claro que él me preguntó qué tal estuvo la noche, y si aprendí algo importante o no."

# game/script-a3-emi.rpy:3863
translate es a3_emi_storage_space_096d69be:

    # "I don't even know if I opened my mouth to answer; I was too tired to care."
    "Ni siquiera sé si abrí la boca para contestar; estaba demasiado cansado para que me importara."

# game/script-a3-emi.rpy:3865
translate es a3_emi_storage_space_73e005e2:

    # "And this morning, I'll admit that I'm feeling pretty worn out."
    "Y esta mañana, admito que me siento bastante desgastado."

# game/script-a3-emi.rpy:3867
translate es a3_emi_storage_space_08f7aeea:

    # "Still, Emi had promised to meet me at the track, and I'd hate to disappoint."
    "A pesar de todo, Emi prometió encontrarse conmigo en la pista, y no me gustaría defraudarla."

# game/script-a3-emi.rpy:3873
translate es a3_emi_storage_space_776a3a29:

    # "She is indeed waiting for me when I arrive."
    "Cuando llego, de verdad está esperándome."

# game/script-a3-emi.rpy:3875
translate es a3_emi_storage_space_720857e0:

    # "Doing her best to look cheery, despite the fact that she's sitting in a wheelchair."
    "Esforzándose por verse animada, a pesar del hecho de estar en silla de ruedas."

# game/script-a3-emi.rpy:3877
translate es a3_emi_storage_space_3a30e736:

    # "I wave to her and begin stretching."
    "La saludo y empiezo a estirar."

# game/script-a3-emi.rpy:3879
translate es a3_emi_storage_space_54901257:

    # hi "You're early."
    hi "Llegaste temprano."

# game/script-a3-emi.rpy:3884
translate es a3_emi_storage_space_f2c6c02e:

    # "Emi frowns and shakes her head."
    "Emi frunce el ceño y sacude la cabeza."

# game/script-a3-emi.rpy:3889
translate es a3_emi_storage_space_741054c6:

    # emi "Ridiculous."
    emi "Ridículo."

# game/script-a3-emi.rpy:3891
translate es a3_emi_storage_space_2301af8a:

    # emi "{b}You're{/b} late."
    emi "{b}Tú{/b} llegaste tarde."

# game/script-a3-emi.rpy:3896
translate es a3_emi_storage_space_7103fe05:

    # emi "Overslept, Hisao?"
    emi "¿Te quedaste dormido, Hisao?"

# game/script-a3-emi.rpy:3901
translate es a3_emi_storage_space_13f12191:

    # emi "All tuckered out?"
    emi "¿Todo machacado?"

# game/script-a3-emi.rpy:3903
translate es a3_emi_storage_space_2e19cb18:

    # "Well, at least she seems more like her old self."
    "Bueno, al menos ya se ve más como la misma de siempre."

# game/script-a3-emi.rpy:3905
translate es a3_emi_storage_space_6fcc4204:

    # "And as expected, she doesn't seem that shy about mentioning our… previous activities."
    "Y como era de esperarse, ella no parece muy cohibida al mencionar nuestras… actividades previas."

# game/script-a3-emi.rpy:3907
translate es a3_emi_storage_space_fc860247:

    # hi "Hey, you're lucky I could show up at all."
    hi "Oye, tienes suerte de que haya podido venir hoy."

# game/script-a3-emi.rpy:3909
translate es a3_emi_storage_space_ae6848bd:

    # hi "All that cardiovascular activity last night, I nearly thought I'd have to see the nurse afterwards."
    hi "Con toda esa actividad cardiovascular de anoche, casi llegué a pensar que tendría que ver al enfermero luego."

# game/script-a3-emi.rpy:3914
translate es a3_emi_storage_space_7c50dbf4:

    # "Emi laughs out loud, then her face suddenly becomes concerned."
    "Emi suelta una risotada, después su rostro se llena repentinamente de preocupación."

# game/script-a3-emi.rpy:3921
translate es a3_emi_storage_space_6921cc44:

    # emi "Hey, that's not uh…"
    emi "Oye, eso no es…"

# game/script-a3-emi.rpy:3923
translate es a3_emi_storage_space_5bfda2a6:

    # emi "I mean, you're not…"
    emi "Quiero decir, tú no…"

# game/script-a3-emi.rpy:3925
translate es a3_emi_storage_space_b7415c48:

    # hi "Go on, spit it out."
    hi "Adelante, dilo."

# game/script-a3-emi.rpy:3930
translate es a3_emi_storage_space_e036cc69:

    # emi "It's just that it would be hard to explain if you had an episode while we were…"
    emi "Es solo que sería difícil de explicar si tú hubieras tenido un episodio mientras nosotros…"

# game/script-a3-emi.rpy:3932
translate es a3_emi_storage_space_04be15a3:

    # hi "Oh."
    hi "Oh."

# game/script-a3-emi.rpy:3934
translate es a3_emi_storage_space_338867b4:

    # hi "{b}Oh.{/b}"
    hi "{b}Oh.{/b}"

# game/script-a3-emi.rpy:3936
translate es a3_emi_storage_space_98cbe744:

    # "Now that she mentions it, it really is a legitimate concern."
    "Ahora que ella lo menciona, de verdad es una preocupación legítima."

# game/script-a3-emi.rpy:3938
translate es a3_emi_storage_space_0e0ad784:

    # "I certainly hadn't thought of it last night, of course - other, more pressing concerns had been at hand."
    "Honestamente no pensé en eso anoche, por supuesto, estaba ocupado con otros asuntos más urgentes."

# game/script-a3-emi.rpy:3940
translate es a3_emi_storage_space_0abd664d:

    # hi "Well, I don't think anything we, er, {b}do{/b} is going to be any more of a strain than these morning runs, and I handle those fine, so…"
    hi "Bueno, no creo que nada de lo que, eh, {b}hagamos{/b} vaya a ser una tensión mayor que estas carreras matutinas, y me las arreglo bien con esas, así que…"

# game/script-a3-emi.rpy:3945
translate es a3_emi_storage_space_71c60631:

    # "Emi considers this point."
    "Emi considera esto."

# game/script-a3-emi.rpy:3950
translate es a3_emi_storage_space_e852e6ce:

    # "A devious light appears in her eyes."
    "Un brillo travieso aparece en sus ojos."

# game/script-a3-emi.rpy:3954
translate es a3_emi_storage_space_c4648bf1:

    # emi "Say…"
    emi "Digamos…"

# game/script-a3-emi.rpy:3956
translate es a3_emi_storage_space_34d1d59d:

    # hi "Hmm?"
    hi "¿Hmm?"

# game/script-a3-emi.rpy:3961
translate es a3_emi_storage_space_86d34890:

    # "The light vanishes, and Emi grins ruefully at me."
    "La luz se esfuma, y Emi me sonríe con tristeza."

# game/script-a3-emi.rpy:3963
translate es a3_emi_storage_space_05b9bacb:

    # "I can't help but feel vaguely suspicious."
    "No puedo evitar sentir una vaga sospecha."

# game/script-a3-emi.rpy:3968
translate es a3_emi_storage_space_ace437d0:

    # emi "I seem to have forgotten a pair of gloves."
    emi "Parece que olvidé un par de guantes."

# game/script-a3-emi.rpy:3970
translate es a3_emi_storage_space_6d1f773c:

    # hi "What do you need gloves for?"
    hi "¿Para qué necesitas guantes?"

# game/script-a3-emi.rpy:3975
translate es a3_emi_storage_space_cf91a250:

    # "Emi indicates the chair upon which she is seated."
    "Emi señala la silla sobre la cual está sentada."

# game/script-a3-emi.rpy:3977
translate es a3_emi_storage_space_b60d5737:

    # emi "For this, of course!"
    emi "¡Para esto, por supuesto!"

# game/script-a3-emi.rpy:3982
translate es a3_emi_storage_space_71da3265:

    # emi "Sure, regular moving around is all well and good without 'em, but I want to be able to get a good workout."
    emi "Bueno, no hacen falta para moverse normalmente por los alrededores, pero quiero sacarle buen provecho y ejercitarme."

# game/script-a3-emi.rpy:3987
translate es a3_emi_storage_space_e977dc2f:

    # emi "And to get that kind of speed, you gotta have gloves if you don't want blisters."
    emi "Y para ir así de rápido, debes usar guantes si no quieres terminar con llagas."

# game/script-a3-emi.rpy:3989
translate es a3_emi_storage_space_95703a7f:

    # hi "So what, are you wussing out on me then? Do I have to go it alone?"
    hi "Entonces qué, ¿te estás acobardando? ¿Tengo que hacerlo solo?"

# game/script-a3-emi.rpy:3994
translate es a3_emi_storage_space_d22f94dc:

    # "Emi thinks for a minute - or pretends to think."
    "Emi piensa por unos instantes, o pretende pensar."

# game/script-a3-emi.rpy:3999
translate es a3_emi_storage_space_074a017f:

    # emi "Hmm… if I remember right, there's a spare pair or two in the track shed."
    emi "Hmm… si mal no recuerdo, hay uno o dos pares que no se usan en el almacén de atletismo."

# game/script-a3-emi.rpy:4001
translate es a3_emi_storage_space_24f5aea1:

    # "So she does seriously want to do it, then."
    "Así que de verdad quiere hacerlo."

# game/script-a3-emi.rpy:4003
translate es a3_emi_storage_space_43394219:

    # "But in her normal school uniform? I'd have expected her to wear her gym outfit for something like this."
    "¿Pero con el uniforme normal de la escuela? Pensé que usaría el uniforme de gimnasia para algo como esto."

# game/script-a3-emi.rpy:4005
translate es a3_emi_storage_space_764f243d:

    # hi "Wait, what are they doing there?"
    hi "Espera, ¿qué están haciendo allí?"

# game/script-a3-emi.rpy:4010
translate es a3_emi_storage_space_b13bd09c:

    # "Emi looks askance at me."
    "Emi me mira de soslayo."

# game/script-a3-emi.rpy:4012
translate es a3_emi_storage_space_939aa7b6:

    # emi "Seriously? You can't think of why a shed full of track supplies at a school for the disabled would have racing gloves?"
    emi "¿En serio? ¿No se te ocurre una razón por la cual un almacén lleno de suministros de atletismo en una escuela de discapacitados tendría guantes para carreras?"

# game/script-a3-emi.rpy:4014
translate es a3_emi_storage_space_dda2dfd1:

    # "Well, when she puts it that way, I suppose that makes perfect sense."
    "Bueno, si lo pone de esa manera, supongo que tiene perfecto sentido."

# game/script-a3-emi.rpy:4016
translate es a3_emi_storage_space_619e3982:

    # hi "Hey, I'm still getting used to this place. Give me a break, huh?"
    hi "Oye, todavía estoy acostumbrándome a este lugar. Dame un respiro, ¿no?"

# game/script-a3-emi.rpy:4021
translate es a3_emi_storage_space_ec071e50:

    # emi "I guess I can let it slide this time."
    emi "Supongo que puedo dejarlo pasar esta vez."

# game/script-a3-emi.rpy:4026
translate es a3_emi_storage_space_ba2494ad:

    # emi "Now come on, I'll need your help."
    emi "Ahora vamos, voy a necesitar tu ayuda."

# game/script-a3-emi.rpy:4028
translate es a3_emi_storage_space_4920876b:

    # "I can't imagine what for, but then again I didn't have a clue why racing gloves would be in the shed, so I'm not willing to press the issue."
    "No imagino para qué, pero, por otra parte, tampoco sabía por qué hay guantes para carreras en el almacén, así que no quiero presionar más el asunto."

# game/script-a3-emi.rpy:4033
translate es a3_emi_storage_space_1f3d2374:

    # "Emi navigates her way to the shed easily enough, though I can hear her grumbling under her breath."
    "Emi dirige el camino hasta el almacén con suficiente facilidad, aunque puedo escuchar cómo se queja en voz baja."

# game/script-a3-emi.rpy:4035
translate es a3_emi_storage_space_4d69f9eb:

    # "It's actually kinda cute."
    "La verdad es que es algo tierno."

# game/script-a3-emi.rpy:4037
translate es a3_emi_storage_space_96f87b13:

    # "I hurry a little to reach the door first. Opening it will be easier for me than for her."
    "Me adelanto un poco para alcanzar la puerta primero. Abrirla será más fácil para mí que para ella."

# game/script-a3-emi.rpy:4049
translate es a3_emi_storage_space_f5938f8a:

    # "The door opens, and Emi starts to wheel inside, only to come to a sudden halt at the doorway."
    "La puerta se abre, y Emi comienza a empujar su silla hacia adentro, solo para detenerse repentinamente en la entrada."

# game/script-a3-emi.rpy:4051
translate es a3_emi_storage_space_b7750477:

    # "It seems the doorsill is slightly too high for her to get over by herself."
    "Parece que el peldaño es un poquito más alto de lo que ella puede subir por sí misma."

# game/script-a3-emi.rpy:4068
translate es a3_emi_storage_space_2d52f173:

    # "She makes a few runs at it, unsuccessfully, before crossing her eyes and glaring at the offending object."
    "Lo intenta unas cuantas veces, sin éxito, antes de fruncir el ceño y lanzar una mirada al objeto infractor."

# game/script-a3-emi.rpy:4073
translate es a3_emi_storage_space_c981799b:

    # emi "Stupid wheelchair."
    emi "Estúpida silla de ruedas."

# game/script-a3-emi.rpy:4078
translate es a3_emi_storage_space_e6123f1c:

    # emi "Hisao, can you give me a hand here?"
    emi "Hisao, ¿me podrías echar una mano con esto?"

# game/script-a3-emi.rpy:4080
translate es a3_emi_storage_space_40be391a:

    # hi "Sure, no problem."
    hi "Claro, no hay problema."

# game/script-a3-emi.rpy:4085
translate es a3_emi_storage_space_49db032d:

    # "It's a simple enough matter for me to bump Emi over the doorway, jostling her slightly."
    "Es suficientemente sencillo para mí empujar a Emi sobre el peldaño de la puerta, sacudiéndola un poco."

# game/script-a3-emi.rpy:4090
translate es a3_emi_storage_space_993b2aea:

    # emi "Hey, easy there!"
    emi "¡Oye, con cuidado!"

# game/script-a3-emi.rpy:4092
translate es a3_emi_storage_space_c1b82a2f:

    # hi "Whoops! Sorry."
    hi "¡Oops! Perdón."

# game/script-a3-emi.rpy:4094
translate es a3_emi_storage_space_b8b13aa6:

    # "It's at about this time that I fail to notice where I'm going and run Emi's chair into a mat."
    "Es en este momento cuando fallo en darme cuenta de mi dirección y estrello la silla de Emi contra una estera."

# game/script-a3-emi.rpy:4105
translate es a3_emi_storage_space_e29c2896:

    # "She gives a startled yelp and topples forward out of her chair."
    "Ella suelta un grito de sorpresa y se desploma fuera de su silla."

# game/script-a3-emi.rpy:4107
translate es a3_emi_storage_space_213dddc2:

    # "There's a moment of silence as I gaze in horror upon what I've done, and Emi glares at me."
    "Hay un momento de silencio mientras miro con horror lo que hice, y Emi me mira a mí."

# game/script-a3-emi.rpy:4109
translate es a3_emi_storage_space_d5796930:

    # emi "Hisao…"
    emi "Hisao…"

# game/script-a3-emi.rpy:4111
translate es a3_emi_storage_space_80452e63:

    # hi "Yes?"
    hi "¿Sí?"

# game/script-a3-emi.rpy:4113
translate es a3_emi_storage_space_33ea2d78:

    # emi "Promise me you'll never work at a hospital."
    emi "Prométeme que nunca trabajarás en un hospital."

# game/script-a3-emi.rpy:4115
translate es a3_emi_storage_space_04d2e458:

    # hi "Sorry! I didn't mean to!"
    hi "¡Lo lamento! ¡No quise hacerlo!"

# game/script-a3-emi.rpy:4117
translate es a3_emi_storage_space_47756de2:

    # "Emi giggles, and holds up a hand."
    "Emi suelta una risita, y alza una mano."

# game/script-a3-emi.rpy:4119
translate es a3_emi_storage_space_f70ee8d8:

    # emi "Would you kindly help me back into my chair, Hisao?"
    emi "¿Serías tan amable de ayudarme a regresar a mi silla, Hisao?"

# game/script-a3-emi.rpy:4129
translate es a3_emi_storage_space_efd25c7c:

    # "As I bend down to pick up Emi, she grins in triumph and pulls me into a kiss that quickly has us both entirely unconcerned about getting her back into the chair."
    "Mientras me agacho para ayudarla, ella sonríe triunfante y me jala hacia un beso que rápidamente provoca que nos olvidemos de regresarla a su silla."

# game/script-a3-emi.rpy:4133
translate es a3_emi_storage_space_3d0eeef3:

    # "In fact, as I move to a more comfortable position, I confess that the chair is pushed out the door, which, startled by the passage, swings shut."
    "De hecho, cuando me muevo a una posición más cómoda, confieso que la silla rueda a través de la puerta, la cual se cierra de golpe tras su paso."

# game/script-a3-emi.rpy:4141
translate es a3_emi_storage_space_4f0b0d6e:

    # "Well, at least we've got privacy now, which is a good thing as my hands work quickly to remove Emi's blouse and skirt."
    "Bueno, al menos ahora tenemos privacidad, y eso es bueno porque mis manos trabajan con velocidad para remover la camiseta y la falda de Emi."

# game/script-a3-emi.rpy:4143
translate es a3_emi_storage_space_fdacb416:

    # "I'm startled to discover that she's forgotten to put her bra on today. Did she plan this?"
    "Me asusta descubrir que ella olvidó ponerse su sujetador hoy. ¿Acaso planeó esto?"

# game/script-a3-emi.rpy:4148
translate es a3_emi_storage_space_a1aef55d:

    # "Her arms hook under mine and rest on my shoulders as I kiss my way down Emi's neck, pausing to give special attention to a spot right where the neck meets the shoulder that I'd found last night."
    "Sus brazos se acomodan bajo los míos y reposan en mis hombros mientras yo trazo un camino de besos en su cuello, deteniéndome para dedicarle especial atención a cierto punto que descubrí ayer, donde el cuello se encuentra con el hombro."

# game/script-a3-emi.rpy:4150
translate es a3_emi_storage_space_921b8658:

    # emi "Y-you've gotten pretty good at th-hee!"
    emi "T-te has vuelto bastante bueno en es-¡ah!"

# game/script-a3-emi.rpy:4152
translate es a3_emi_storage_space_61e89c85:

    # hi "I do try."
    hi "Me esfuerzo."

# game/script-a3-emi.rpy:4157
translate es a3_emi_storage_space_aa6d31a2:

    # "Emi pushes on my chest, insistently, and I draw back with a puzzled expression."
    "Emi empuja mi pecho, con insistencia, y retrocedo con una expresión de perplejidad."

# game/script-a3-emi.rpy:4159
translate es a3_emi_storage_space_e3db824e:

    # emi "I've got a confession, Hisao."
    emi "Tengo que confesar algo, Hisao."

# game/script-a3-emi.rpy:4161
translate es a3_emi_storage_space_f9075882:

    # hi "Oh?"
    hi "¿Oh?"

# game/script-a3-emi.rpy:4163
translate es a3_emi_storage_space_7b61edaf:

    # "Having pulled back, I decide instead to focus my attention on her breasts."
    "Habiendo retrocedido, decido concentrar mi atención en sus pechos."

# game/script-a3-emi.rpy:4168
translate es a3_emi_storage_space_77373c5a:

    # "As she attempts to speak, her words are interspersed with giggles that I find incredibly cute."
    "Mientras trata de hablar, intercala sus palabras con risitas que me parecen increíblemente adorables."

# game/script-a3-emi.rpy:4173
translate es a3_emi_storage_space_a81aebb6:

    # emi "I don't ac-hee hee hee-actually w-woah! Wear gloves."
    emi "En re-aja ja ja-realidad ¡G-guau! No uso guantes."

# game/script-a3-emi.rpy:4175
translate es a3_emi_storage_space_1410428c:

    # "My own reply is rather mumbled onto her chest instead of being addressed to her face."
    "Mi respuesta es más bien un balbuceo dirigido a su pecho en vez de a su cara."

# game/script-a3-emi.rpy:4177
translate es a3_emi_storage_space_123aff92:

    # hi "Should've known…"
    hi "Debí haberlo sabido…"

# game/script-a3-emi.rpy:4179
translate es a3_emi_storage_space_a586cac1:

    # "Words quickly become irrelevant."
    "Las palabras dejan de ser relevantes rápidamente."

# game/script-a3-emi.rpy:4184
translate es a3_emi_storage_space_49e00ad8:

    # "Emi's movements are almost frantic, as if she's been holding something back since we met this morning, and now she has an outlet."
    "Los movimientos de Emi son frenéticos, como si hubiera estado conteniendo algo desde que nos encontramos esta mañana, y ahora pudiera dejarlo salir."

# game/script-a3-emi.rpy:4186
translate es a3_emi_storage_space_71279886:

    # "I'm very nearly caught off guard by her aggressiveness, feeling her nearly rip my shirt off, the way she seems to vie to be in the dominant position."
    "Su agresividad casi me toma desprevenido, sintiendo cómo casi arranca mi camisa, la forma en la que parece luchar por estar en la posición dominante."

# game/script-a3-emi.rpy:4188
translate es a3_emi_storage_space_a89e999b:

    # "For my part, I confess that I'm caught up in her attitude as well, fighting back, rolling and wrestling even as I caress her breasts, even as her fingers dig into my shoulders, and I lose track of where we are."
    "Por mi parte, confieso que también soy atraído por su actitud, defendiéndome, girando y luchando incluso mientras acaricio sus pechos, incluso cuando sus dedos se clavan en mis hombros, y olvido dónde estamos."

# game/script-a3-emi.rpy:4193
translate es a3_emi_storage_space_a5cb8698:

    # "So much so that I roll right off the mat and land on something small and rather hard."
    "Tanto así que giro fuera de la estera y caigo sobre algo pequeño y bastante duro."

# game/script-a3-emi.rpy:4195
translate es a3_emi_storage_space_f9beb54d:

    # hi "Ow!"
    hi "¡Au!"

# game/script-a3-emi.rpy:4200
translate es a3_emi_storage_space_4255ee14:

    # "Emi, still flushed and breathing a little heavily, peers at me and bursts into laughter."
    "Emi, aún sonrojada y respirando pesadamente, me vuelve a ver y rompe en carcajadas."

# game/script-a3-emi.rpy:4202
translate es a3_emi_storage_space_dc712a53:

    # emi "I'm sorry, I'm sorry. Are you all right?"
    emi "Lo siento, lo siento. ¿Estás bien?"

# game/script-a3-emi.rpy:4204
translate es a3_emi_storage_space_824152f4:

    # hi "Yeah, I think so. Not sure what I landed on, though…"
    hi "Sí, creo. Aunque no estoy seguro sobre qué caí…"

# game/script-a3-emi.rpy:4206
translate es a3_emi_storage_space_7ff95099:

    # "I reach under my back and pull the offending object out, inspecting it closely."
    "Reviso bajo mi espalda y saco el objeto culpable, inspeccionándolo de cerca."

# game/script-a3-emi.rpy:4210
translate es a3_emi_storage_space_b09c9487:

    # "'Personal lubricant. Lemon-flavored.'"
    "“Lubricante personal. Sabor a limón”."

# game/script-a3-emi.rpy:4212
translate es a3_emi_storage_space_7afba655:

    # "Wait, what?"
    "Espera, ¿qué?"

# game/script-a3-emi.rpy:4219
translate es a3_emi_storage_space_f9b3d28c:

    # "Emi's eyes shoot upwards and she begins, if possible, to laugh even harder."
    "Emi lanza una mirada y empieza, si es posible, a reír con más fuerza."

# game/script-a3-emi.rpy:4221
translate es a3_emi_storage_space_81458087:

    # hi "Somehow, I don't think this is… this isn't track-related."
    hi "Por alguna razón, creo que esto… no tiene nada que ver con atletismo."

# game/script-a3-emi.rpy:4226
translate es a3_emi_storage_space_00b39f67:

    # emi "Oh man, I know whose that is!"
    emi "Oh, vaya, ¡yo sé de quién es eso!"

# game/script-a3-emi.rpy:4228
translate es a3_emi_storage_space_81d99227:

    # hi "What?"
    hi "¿Qué?"

# game/script-a3-emi.rpy:4233
translate es a3_emi_storage_space_a7c97c7d:

    # emi "It's the track captain's!"
    emi "¡Es del capitán del equipo!"

# game/script-a3-emi.rpy:4235
translate es a3_emi_storage_space_638c543e:

    # "Ah, my old nemesis. Or, kind of."
    "Ah, mi viejo némesis. O algo así."

# game/script-a3-emi.rpy:4237
translate es a3_emi_storage_space_4f4674e2:

    # hi "How d'you know it's his?"
    hi "¿Cómo sabes que es de él?"

# game/script-a3-emi.rpy:4242
translate es a3_emi_storage_space_d47e78a3:

    # "It appears that I've asked a stupid question, or at least Emi thinks so."
    "Al parecer hice una pregunta tonta, o al menos eso piensa Emi."

# game/script-a3-emi.rpy:4247
translate es a3_emi_storage_space_803bbd53:

    # emi "Because he's the one who told me the track shed was a good place for… what did he call them?"
    emi "Porque fue él quien me dijo que el almacén de atletismo era un buen lugar para… ¿cómo los llamó él?"

# game/script-a3-emi.rpy:4252
translate es a3_emi_storage_space_3e7181a4:

    # emi "'Clandestine encounters.'"
    emi "“Encuentros clandestinos”."

# game/script-a3-emi.rpy:4254
translate es a3_emi_storage_space_38331800:

    # hi "Oh? He invite you to one or something?"
    hi "¿Oh? ¿Te invitó a alguno o algo así?"

# game/script-a3-emi.rpy:4259
translate es a3_emi_storage_space_1ce46088:

    # "Emi bursts into more laughter."
    "Emi suelta otra carcajada."

# game/script-a3-emi.rpy:4261
translate es a3_emi_storage_space_c55ee112:

    # "I confess the sight of a naked Emi laughing is oddly beautiful."
    "Confieso que la imagen de una Emi desnuda riéndose es extrañamente hermosa."

# game/script-a3-emi.rpy:4263
translate es a3_emi_storage_space_fc484ad1:

    # "I feel an eagerness to end conversation and get back to what we were doing, despite my rather pointed questioning."
