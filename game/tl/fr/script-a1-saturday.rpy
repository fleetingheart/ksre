translate fr a1_saturday_shanghaied_f2121f81:

    # mi "It's a simple task, so getting it out of the way now would be good. It's not that much work. A small thing, really!"
    mi "C'est une tâche simple, s'en débarrasser serait bien. Ce n'est pas beaucoup de travail. Pas grand-chose, vraiment!"

# game/script-a1-saturday.rpy:2980
translate fr a1_saturday_shanghaied_ef2b123c_40:

    # shi "…"
    shi "…"

# game/script-a1-saturday.rpy:2982
translate fr a1_saturday_shanghaied_1254c0d3:

    # mi "And we'd really appreciate your help!"
    mi "Et on apprécierait vraiment ton aide!"

# game/script-a1-saturday.rpy:2984
translate fr a1_saturday_shanghaied_b44530f3:

    # mi "Really, really appreciate it!"
    mi "On apprécierait vraiment, vraiment ça!"

# game/script-a1-saturday.rpy:2989
