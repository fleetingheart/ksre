# game/script-a3-emi.rpy:4715
translate fr a3_emi_storage_space_f01c246e:

    # "This is the most awkward conversation I have ever had."
    "C'est la conversation la plus bizarre que je ne n'ai jamais eue."

# game/script-a3-emi.rpy:4720
translate fr a3_emi_storage_space_460a04ca:

